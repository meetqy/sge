[{"data":1,"prerenderedAt":1409},["Reactive",2],{"content-query-EGtuIjKzDe":3,"content-navigation-8C37fagqQL":312},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":308,"_type":309,"_source":310,"_file":311,"_extension":309},"/wang-wei","",false,"王維",[9,21,35,47,58,73,95,116,132,142,152,160,168,177,186,194,207,215,226,235,243,250,260,265,275,284,294,303],{"title":10,"content":11,"tags":15},"送別",[12,13,14],"下馬飲君酒，問君何所之。","君言不得意，歸臥南山陲。","但去莫復問，白雲無盡時。",[16,17,18,19,20],"唐诗三百首","抒情","写景","友情","送别",{"title":22,"content":23,"tags":32},"送綦毋潛落第還鄉",[24,25,26,27,28,29,30,31],"聖代無隱者，英靈盡來歸。","遂令東山客，不得顧采薇。","既至君門遠，孰云吾道非。","江淮度寒食，京洛縫春衣。","置酒臨長道，同心與我違。","行當浮桂櫂，未幾拂荆扉。","遠樹帶行客，孤村當落暉。","吾謀適不用，勿謂知音稀。",[16,20,33,34,19],"五言古诗","慰勉",{"title":36,"content":37,"tags":44},"青谿",[38,39,40,41,42,43],"言入黃花川，每逐青谿水。","隨山將萬轉，趣途無百里。","聲喧亂石中，色靜深松裏。","漾漾汎菱荇，澄澄映葭葦。","我心素已閑，清川澹如此。","請留盤石上，垂釣將已矣。",[16,33,45,46],"托物寄情","山水",{"title":48,"content":49,"tags":55},"渭川田家",[50,51,52,53,54],"斜陽照墟落，窮巷牛羊歸。","野老念牧童，倚杖候荆扉。","雉雊麥苗秀，蠶眠桑葉稀。","田夫荷鋤至，相見語依依。","即此羨閑逸，悵然吟式微。",[16,33,56,57],"归隐","田园",{"title":59,"content":60,"tags":68},"西施詠",[61,62,63,64,65,66,67],"豔色天下重，西施寧久微。","朝仍越溪女，暮作吳宮妃。","賤日豈殊衆，貴來方悟稀。","邀人傅香粉，不自著羅衣。","君寵益嬌態，君憐無是非。","當時浣紗伴，莫得同車歸。","持謝鄰家子，效顰安可希。",[16,69,33,70,71,72],"隋・唐・五代","怀古","写人","怀才不遇",{"title":74,"content":75,"tags":91},"老將行",[76,77,78,79,80,81,82,83,84,85,86,87,88,89,90],"少年十五二十時，步行奪得胡馬射。","射殺中山白額虎，肯數鄴下黃鬚兒。","一身轉戰三千里，一劒曾當百萬師。","漢兵奮迅如霹靂，虜騎崩騰畏蒺藜。","衞青不敗由天幸，李廣無功緣數奇。","自從棄置便衰朽，世事磋跎成白首。","昔時飛箭無全目，今日垂楊生左肘。","路傍時賣故侯瓜，門前學種先生柳。","蒼茫古木連窮巷，寥落寒山對虛牖。","誓令疏勒出飛泉，不似潁川空使酒。","賀蘭山下陣如雲，羽檄交馳日夕聞。","節使三河募年少，詔書五道出將軍。","試拂鐵衣如雪色，聊持寶劒動星文。","願得燕弓射天將，恥令越甲鳴吳軍。","莫嫌舊日雲中守，猶堪一戰取功勳。",[16,92,71,93,94],"乐府","新乐府辞","赞颂",{"title":96,"content":97,"tags":114},"桃源行",[98,99,100,101,102,103,104,105,106,107,108,109,110,111,112,113],"漁舟逐水愛山春，兩岸桃花夾去津。","坐看紅樹不知遠，行盡青溪不見人。","山口潛行始隈隩，山開曠望旋平陸。","遙看一處攢雲樹，近入千家散花竹。","樵客初傳漢姓名，居人未改秦衣服。","居人共住武陵源，還從物外起田園。","月明松下房櫳靜，日出雲中雞犬喧。","驚聞俗客爭來集，競引還家問都邑。","平明閭巷埽花開，薄暮漁樵乘水入。","初因避地去人間，及至成仙遂不還。","峽裏誰知有人事，世中遙望空雲山。","不疑靈境難聞見，塵心未盡思鄉縣。","出洞無論隔山水，辭家終擬長游衍。","自謂經過舊不迷，安知峰壑今來變。","當時只記入山深，青溪幾曲到雲林。","春來遍是桃花水，不辨仙源何處尋。",[16,18,92,93,115],"水",{"title":117,"content":118,"tags":129},"洛陽女兒行",[119,120,121,122,123,124,125,126,127,128],"洛陽女兒對門居，纔可容顏十五餘。","良人玉勒乘驄馬，侍女金盤鱠鯉魚。","畫閣朱樓盡相望，紅桃綠柳垂簷向。","羅幃送上七香車，寶扇迎歸九華帳。","狂夫富貴在青春，意氣驕奢劇季倫。","自憐碧玉親教舞，不惜珊瑚持與人。","春窗曙滅九微火，九微片片飛花璅。","戲罷曾無理曲時，妝成秪是薰香坐。","城中相識盡繁華，日夜經過趙李家。","誰憐越女顏如玉，貧賤江頭自浣紗。",[16,130,131,92,93],"生活","女子",{"title":133,"content":134,"tags":139},"輞川閑居贈裴秀才迪",[135,136,137,138],"寒山轉蒼翠，秋水日潺湲。","倚杖柴門外，臨風聽暮蟬。","渡頭餘落日，墟里上孤煙。","復值接輿醉，狂歌五柳前。",[16,18,17,140,141],"秋天","五言律诗",{"title":143,"content":144,"tags":149},"酬張少府",[145,146,147,148],"晚年唯好靜，萬事不關心。","自顧無長策，空知返舊林。","松風吹解帶，山月照彈琴。","君問窮通理，漁歌入浦深。",[16,150,19,151,141],"赠别","规劝",{"title":153,"content":154,"tags":159},"送梓州李使君",[155,156,157,158],"萬壑樹參天，千山響杜鵑。","山中一夜雨，樹杪百重泉。","漢女輸橦布，巴人訟芋田。","文翁翻教授，不敢倚先賢。",[16,19,20,141],{"title":161,"content":162,"tags":167},"過香積寺",[163,164,165,166],"不知香積寺，數里入雲峰。","古木無人逕，深山何處鐘。","泉聲咽危石，日色冷青松。","薄暮空潭曲，安禪制毒龍。",[16,18,141],{"title":169,"content":170,"tags":175},"山居秋暝",[171,172,173,174],"空山新雨後，天氣晚來秋。","明月松間照，清泉石上流。","竹喧歸浣女，蓮動下漁舟。","隨意春芳歇，王孫自可留。",[16,46,18,176,141,71],"秋雨",{"title":178,"content":179,"tags":184},"終南別業",[180,181,182,183],"中歲頗好道，晚家南山陲。","興來每獨往，勝事空自知。","行到水窮處，坐看雲起時。","偶然值林叟，談笑無還期。",[16,57,17,141,185,71],"山",{"title":187,"content":188,"tags":193},"歸嵩山作",[189,190,191,192],"清川帶長薄，車馬去閑閑。","流水如有意，暮禽相與還。","荒城臨古渡，落日滿秋山。","迢遞嵩高下，歸來且閉關。",[16,46,18,17,141],{"title":195,"content":196,"tags":201},"終南山",[197,198,199,200],"太乙近天都，連山接海隅。","白雲迴望合，青靄入看無。","分野中峰變，陰晴衆壑殊。","欲投人處宿，隔水問樵夫。",[16,202,203,141,204,205,206],"终南山","一年级下册","高中古诗","描写山","写山",{"title":208,"content":209,"tags":214},"漢江臨汎",[210,211,212,213],"楚塞三湘接，荆門九派通。","江流天地外，山色有無中。","郡邑浮前浦，波瀾動遠空。","襄陽好風日，留醉與山翁。",[16,46,17,141],{"title":216,"content":217,"tags":222},"和賈舍人早朝大明宮之作",[218,219,220,221],"絳幘雞人送曉籌，尚衣方進翠雲裘。","九天閶闔開宮殿，萬國衣冠拜冕旒。","日色纔臨仙掌動，香煙欲傍衮龍浮。","朝罷須裁五色詔，佩聲歸向鳳池頭。",[16,223,224,225],"宫廷","七言律诗","早朝",{"title":227,"content":228,"tags":233},"酬郭給事",[229,230,231,232],"洞門高閣靄餘輝，桃李陰陰柳絮飛。","禁裏疎鐘官舍晚，省中啼鳥吏人稀。","晨搖玉佩趨金殿，夕奉天書拜瑣闈。","強欲從君無那老，將因臥病解朝衣。",[16,234,224],"酬和",{"title":236,"content":237,"tags":242},"積雨輞川莊作",[238,239,240,241],"積雨空林煙火遲，蒸藜炊黍餉東菑。","漠漠水田飛白鷺，陰陰夏木囀黃鸝。","山中習靜觀朝槿，松下清齋折露葵。","野老與人爭席罷，海鷗何事更相疑。",[16,224,57],{"title":244,"content":245,"tags":248},"輞川集 鹿柴",[246,247],"空山不見人，但聞人語響。","返景入深林，復照青苔上。",[16,18,185,205,206,249],"五言绝句",{"title":251,"content":252,"tags":255},"輞川集 竹里館",[253,254],"獨坐幽篁裏，彈琴復長嘯。","深林人不知，明月來相照。",[16,256,130,257,258,56,249,259],"孤独","初中古诗","月亮","七年级下册(课外)",{"title":10,"content":261,"tags":264},[262,263],"山中相送罷，日暮掩柴扉。","春草明年綠，王孫歸不歸。",[16,249,20,19],{"title":266,"content":267,"tags":270},"雜詩三首 二",[268,269],"君自故鄉來，應知故鄉事。","來日綺窗前，寒梅着花未。",[16,271,18,272,273,274,249],"景中情","思亲","梅花","思乡",{"title":276,"content":277,"tags":280},"相思",[278,279],"紅豆生南國，秋來發故枝。","願君多采擷，此物最相思。",[16,281,282,17,283,249],"思念","咏物","咏物诗",{"title":285,"content":286,"tags":289},"九月九日憶山東兄弟",[287,288],"獨在異鄉爲異客，每逢佳節倍思親。","遙知兄弟登高處，遍插茱萸少一人。",[16,290,274,291,292,293],"七言绝句","三年级上册","重阳节","小学古诗",{"title":295,"content":296,"tags":299},"渭城曲",[297,298],"渭城朝雨浥輕塵，客舍青青楊柳春。","勸君更盡一杯酒，西出陽關無故人。",[300,19,293,20,257,92,16,301,302],"八年级下册(课外)","近代曲辞","四年级上册",{"title":304,"content":305,"tags":307},"雜曲歌辭 渭城曲",[306,298],"謂城朝雨浥輕塵，客舍青青柳色春。",[16,300,19,293,20,257,92,301,302],"content:wang-wei.json","json","content","wang-wei.json",[313,316,319,322,325,328,331,334,337,340,343,346,349,352,355,358,361,364,367,370,373,376,379,382,385,388,391,394,397,400,403,406,409,412,415,418,421,424,427,430,433,436,439,442,445,448,451,454,457,460,463,466,469,472,475,478,481,484,487,490,493,496,499,502,505,508,511,514,517,520,523,526,529,532,535,538,541,544,547,550,553,556,559,562,565,568,571,574,577,580,583,586,589,592,595,598,601,604,607,610,613,616,619,622,625,628,631,634,637,640,643,646,649,652,655,658,661,664,667,670,673,676,679,682,685,688,691,694,697,700,703,706,709,712,715,718,721,724,727,730,733,736,739,742,745,748,751,754,7