[{"data":1,"prerenderedAt":1184},["Reactive",2],{"content-query-dSy5T1NZK3":3,"content-navigation-8C37fagqQL":87},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":83,"_type":84,"_source":85,"_file":86,"_extension":84},"/du-mu","",false,"杜牧",[9,18,30,39,45,53,59,65,72],{"title":10,"content":11,"tags":14},"將赴吳興登樂遊原一絕",[12,13],"清時有味是無能，閑愛孤雲靜愛僧。","欲把一麾江海去，樂遊原上望昭陵。",[15,16,17],"唐诗三百首","七言绝句","爱国",{"title":19,"content":20,"tags":23},"赤壁",[21,22],"折戟沈沙鐵未銷，自將磨洗認前朝。","東風不與周郎便，銅雀春深鏁二喬。",[24,25,26,15,27,16,28,29],"隋・唐・五代","战争","八年级下册(课内)","初中古诗","咏史怀古","怀才不遇",{"title":31,"content":32,"tags":35},"泊秦淮",[33,34],"煙籠寒水月籠沙，夜泊秦淮近酒家。","商女不知亡國恨，隔江猶唱後庭花。",[15,24,17,36,27,16,37,28,38],"写景","抒怀","七年级上册(课外)",{"title":40,"content":41,"tags":44},"贈別二首 一",[42,43],"娉娉褭褭十三餘，豆蔻梢頭二月初。","春風十里揚州路，卷上珠簾總不如。",[15],{"title":46,"content":47,"tags":50},"贈別二首 二",[48,49],"多情却似總無情，唯覺尊前笑不成。","蠟燭有心還惜別，替人垂淚到天明。",[15,51,16,52],"离别","惜别",{"title":54,"content":55,"tags":58},"遣懷",[56,57],"落魄江南載酒行，楚腰腸斷掌中輕。","十年一覺揚州夢，贏得青樓薄倖名。",[15,16,37],{"title":60,"content":61,"tags":64},"秋夕",[62,63],"紅燭秋光冷畫屏，輕羅小扇撲流螢。","天階夜色涼如水，坐看牽牛織女星。",[15],{"title":66,"content":67,"tags":70},"金谷園",[68,69],"繁華事散逐香塵，流水無情草自春。","日暮東風怨啼鳥，落花猶似墮樓人。",[15,16,71],"怀古",{"title":73,"content":74,"tags":79},"旅宿",[75,76,77,78],"旅館無良伴，凝情自悄然。","寒燈思舊事，斷鴈警愁眠。","遠夢歸侵曉，家書到隔年。","湘江好煙月，門繫釣魚船。",[15,80,81,82],"羁旅","思乡","五言律诗","content:du-mu.json","json","content","du-mu.json",[88,91,94,97,100,103,106,109,112,115,118,121,124,127,130,133,136,139,142,145,148,151,154,157,160,163,166,169,172,175,178,181,184,187,190,193,196,199,202,203,206,209,212,215,218,221,224,227,230,233,236,239,242,245,248,251,254,257,260,263,266,269,272,275,278,281,284,287,290,293,296,299,302,305,308,311,314,317,320,323,326,329,332,335,338,341,344,347,350,353,356,359,362,365,368,371,374,377,380,383,386,389,392,395,398,401,404,407,410,413,416,419,422,425,428,431,434,437,440,443,446,449,452,455,458,461,464,467,470,473,476,479,482,485,488,491,494,497,500,503,506,509,512