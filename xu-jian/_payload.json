[{"data":1,"prerenderedAt":1177},["Reactive",2],{"content-query-qsIjlX0vy9":3,"content-navigation-8C37fagqQL":80},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":76,"_type":77,"_source":78,"_file":79,"_extension":77},"/xu-jian","",false,"許堅",[9,15,23,31,37,43,51,57,63,71],{"title":10,"content":11,"tags":14},"詩一首",[12,13],"祗應天上路，不爲下方開。","道既學不得，仙從何處來。",[],{"title":16,"content":17,"tags":22},"題簡寂觀",[18,19,20,21],"嘗恨真風千載隠，洞天還得恣遊遨。","松楸古跡一壇靜，鸞鶴不來青漢高。","茅氏井寒丹亦化，玄宗碑斷夢曾勞。","分明有箇長生路，不向紅塵白二毛。",[],{"title":24,"content":25,"tags":30},"宿溧陽靈泉精舍僧以白字韻請留詩",[26,27,28,29],"近枕吳溪與越峰，前朝恩錫靈泉額。","竹林晴見雁塔高，石室增棲幾禪伯。","荒碑字沒秋苔深，古池香泛荷花白。","客有經年別故林，落日啼猿情脈脈。",[],{"title":32,"content":33,"tags":36},"上徐舍人鉉",[34,35],"幾宵烟日鎖樓臺，欲寄侯門薦禰才。","滿面塵埃人不識，謾隨流水出山來。",[],{"title":38,"content":39,"tags":42},"戢兵山",[40,41],"石鼓高懸藴大音，白雲峰頂始鋪金。","能來斯地鼓斯鼓，盡達曹溪聖祖心。",[],{"title":44,"content":45,"tags":50},"題幽棲觀",[46,47,48,49],"仙翁上昇去，丹井寄晴壑。","山色接天台，湖光照寥廓。","玉洞絕無人，老檜猶棲鶴。","我欲掣青蛇，他時沖碧落。",[],{"title":52,"content":53,"tags":56},"登游齊山",[54,55],"星使南馳入楚重，此山偏得駐行蹤。","落花滿地月華冷，寂寞舊山三四峰。",[],{"title":58,"content":59,"tags":62},"題扇",[60,61],"哦吟但寫胸中妙，飲酒能忘身後名。","但願長閒有詩酒，一溪風月共清明。",[],{"title":64,"content":65,"tags":70},"題義門胡氏華林書院",[66,67,68,69],"盡說靈蹤妨畫圖，幽奇高尚義羣居。","山林總是神仙隠，禮樂爰修周孔書。","解駕十年惟壯士，擔簦千里結名廬。","功成霄漢非常事，對此那堪不我欺。",[],{"title":72,"content":73,"tags":75},"句",[74],"卧久就慵伸雪項，少遲猶未整霜衣。",[],"content:xu-jian.json","json","content","xu-jian.json",[81,84,87,90,93,96,99,102,105,108,111,114,117,120,123,126,129,132,135,138,141,144,147,150,153,156,159,162,165,168,171,174,177,180,183,186,189,192,195,198,201,204,207,210,213,216,219,222,225,228,231,234,237,240,243,246,249,252,255,258,261,264,267,270,273,276,279,282,285,288,291,294,297,300,303,306,309,312,315,318,321,324,327,330,333,336,339,342,345,348,351,354,357,360,363,366,369,372,375,378,381,384,387,390,393,396,399,402,405,408,411,414,417,420,423,426,429,432,435,438,441,444,447,450,453,456,459,462,465,468,471,474,477,480,483,486,489,492,495,498,501,504,507,510,513,5