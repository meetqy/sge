[{"data":1,"prerenderedAt":1144},["Reactive",2],{"content-query-BbpvxyGKJK":3,"content-navigation-8C37fagqQL":47},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":43,"_type":44,"_source":45,"_file":46,"_extension":44},"/lu-wen-zhong","",false,"呂文仲",[9,19,27,35],{"title":10,"content":11,"tags":18},"禁林讌會之什",[12,13,14,15,16,17],"青葱溫樹非塵境，鼇岫金鑾近日邊。","石壁天章垂雨露，璇題宸翰動雲烟。","西垣賈馬徵辭客，東觀蓬瀛集列仙。","讌喜酡顔飛玉斝，鏗鏘奮藻擘花牋。","唐虞盛化高千古，葵藿傾心祝萬年。","自顧薄才誠有媿，不知何以奉羣賢。",[],{"title":20,"content":21,"tags":26},"題義門胡氏華林書院  其一",[22,23,24,25],"世世趨庭列雁行，烟蘿泉石繞書堂。","分題月下聯書版，聚宴花前獻壽觴。","旌表异恩逢聖代，隠居佳致接仙鄉。","因知孝治垂風化，青史留名道允光。",[],{"title":28,"content":29,"tags":34},"題義門胡氏華林書院  其二",[30,31,32,33],"簪裾幾世住南昌，講學旌閭曄日光。","舊里葛山同井邑，遠方廬阜出門牆。","康成業自詩書顯，卜世家由孝弟彰。","二隠昔慚聯懿分，同將姓字列東堂。",[],{"title":36,"content":37,"tags":42},"題義門胡氏華林書院  其三",[38,39,40,41],"君家高誼冠南州，舊輯書堂聚北流。","北飯未嘗輕下客，名園不羨覓封侯。","挂冠令有陶彭澤，訪道人希許遠遊。","他年濯纓江海去，終期此日挹浮邱。",[],"content:lü-wen-zhong.json","json","content","lü-wen-zhong.json",[48,51,54,57,60,63,66,69,72,75,78,81,84,87,90,93,96,99,102,105,108,111,114,117,120,123,126,129,132,135,138,141,144,147,150,153,156,159,162,165,168,171,174,177,180,183,186,189,192,195,198,201,204,207,210,213,216,219,222,225,228,231,234,237,240,243,246,249,252,255,258,261,264,267,270,273,276,279,282,285,288,291,294,297,300,303,306,309,312,315,318,321,324,327,330,333,336,339,342,345,348,351,354,357,360,363,366,369,372,375,378,381,384,387,390,393,396,399,402,405,408,411,414,417,420,423,426,429,432,435,438,441,444,447,450,453,456,459,462,465,466,469,472,475,478,481,484,487,490,493,496,499,502,505,508,511,514,517,520,523,526,529,532,535,538,541,544,547,550,553,556,559,562,565,568,571,574,577,580,583,586,589,592,595,598,601,604,607,610,613,616,619,622,625,628,631,634,637,640,643,646,649,652,655,658,661,664,667,670,673,676,679,682,685,688,691,694,697,700,703,706,709,712,715,718,721,724,727,730,733,736,739,742,745,748,751,754,757,760,763,766,769,772,775,778,781,784,787,790,793,796,799,802,805,808,811,814,817,820,8