[{"data":1,"prerenderedAt":1144},["Reactive",2],{"content-query-OnhYUBsajA":3,"content-navigation-8C37fagqQL":47},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":43,"_type":44,"_source":45,"_file":46,"_extension":44},"/chen-bai","",false,"陳白",[9,15,21,29,35],{"title":10,"content":11,"tags":14},"題元象大師房",[12,13],"鷺行飛起綠楊岸，漁笛吹殘明月灘。","一片畫圖收不得，何人終日倚闌干。",[],{"title":16,"content":17,"tags":20},"妙智寺  其一",[18,19],"台嶺名將方丈齊，當年曾說二僊迷。","無人再得洞中信，空有桃花落剡溪。",[],{"title":22,"content":23,"tags":28},"妙智寺  其二",[24,25,26,27],"排危步步上天梯，喬木蒼蒼拂紫霓。","向日莫催琪樹老，清宵應見玉繩低。","雲蒸巨壑金虬蟄，月落高巖翠羽栖。","況復僊翁丹井在，碧霄歧路不須迷。",[],{"title":30,"content":31,"tags":34},"妙智寺  其三",[32,33],"八重峰頂望羣山，百谷東流去不還。","惟有右軍遺跡在，月波時落墨池間。",[],{"title":36,"content":37,"tags":42},"題勝光怡道軒",[38,39,40,41],"了了無凝滯，誰人知此心。","開池待海月，種樹宿沙禽。","白雪盈青簡，古風生素琴。","由來大隠者，何必在山林。",[],"content:chen-bai.json","json","content","chen-bai.json",[48,51,54,57,60,63,66,69,72,75,78,81,84,87,90,93,96,97,100,103,106,109,112,115,118,121,124,127,130,133,136,139,142,145,148,151,154,157,160,163,166,169,172,175,178,181,184,187,190,193,196,199,202,205,208,211,214,217,220,223,226,229,232,235,238,241,244,247,250,253,256,259,262,265,268,271,274,277,280,283,286,289,292,295,298,301,304,307,310,313,316,319,322,325,328,331,334,337,340,343,346,349,352,355,358,361,364,367,370,373,376,379,382,385,388,391,394,397,400,403,406,409,412,415,418,421,424,427,430,433,436,439,442,445,448,451,454,457,460,463,466,469,472,475,478,481,484,487,490,493,496,499,502,505,508,511,514,517,520,523,526,529,532,535,538,541,544,547,550,553,556,559,562,565,568,571,574,577,580,583,586,589,592,595,598,601,604,607,610,613,616,619,622,625,628,631,634,637,640,643,646,649,652,655,658,661,664,667,670,673,676,679,682,685,688,691,694,697,700,703,706,709,712,715,718,721,724,727,730,733,736,739,742,745,748,751,754,757,760,763,766,769,772,775,778,781,784,787,790,793,796,799,802,805,808,811,814,817,820,823,826,829,832,835,838,841,844,847,850,853,856,859,862,865,868,871,874,877,880,883