[{"data":1,"prerenderedAt":1627},["Reactive",2],{"content-query-sN7DrT40q5":3,"content-navigation-8C37fagqQL":356},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":352,"_type":353,"_source":354,"_file":355,"_extension":353},"/tan-yong-zhi","",false,"譚用之",[9,17,25,33,41,49,57,65,73,81,89,97,105,113,121,129,137,145,153,161,169,177,185,193,201,209,217,225,233,245,253,261,268,276,284,292,300,308,316,324,332,337,342,347],{"title":10,"content":11,"tags":16},"塞上  其一",[12,13,14,15],"秋風漢北雁飛天，單騎那堪遶賀蘭。","磧暗更無巖樹影，地平時有野燒瘢。","貂披寒色和衣冷，劍佩胡霜隔匣寒。","早晚橫戈似飛尉，擁旄深入異田單。",[],{"title":18,"content":19,"tags":24},"塞上  其二",[20,21,22,23],"鉢略城邊日欲西，遊人却憶舊山歸。","牛羊集水烟黏步，鵰鶚盤空雪滿圍。","獵騎靜逢邊氣薄，戍樓寒對暮烟微。","橫行總是男兒事，早晚重來似漢飛。",[],{"title":26,"content":27,"tags":32},"贈索處士",[28,29,30,31],"不將桂子種諸天，長得尋君水石邊。","玄豹夜寒和霧隠，驪龍春暖抱珠眠。","山中宰相陶弘景，洞裏真人葛稚川。","一度相思一惆悵，水寒烟澹落花前。",[],{"title":34,"content":35,"tags":40},"別雒下一二知己",[36,37,38,39],"金鼎光輝照雪袍，雒陽春夢憶波濤。","塵埃滿眼人情異，風雨前程馬足勞。","接塞峨眉通蜀險，過山仙掌倚秦高。","別來無限幽求子，應笑區區味六韜。",[],{"title":42,"content":43,"tags":48},"約張處士遊梁",[44,45,46,47],"莫學區區老一經，夷門關吏舊書生。","晉朝滅後無中散，韓國亡來絕上卿。","龍變洞中千谷冷，劍橫天外八風清。","好携長策干時去，免逐漁樵度太平。",[],{"title":50,"content":51,"tags":56},"送友人歸青社",[52,53,54,55],"鵰鶚途程在碧天，綵衣東去復何言。","二千賓客舊知己，十二山河新故園。","吟看桂生溪月上，醉聽鯤化海濤翻。","好期聖代重相見，莫學袁生老竹軒。",[],{"title":58,"content":59,"tags":64},"送丁道士歸南中",[60,61,62,63],"孤雲無定鶴辭巢，自負焦桐不說勞。","服藥幾年期碧落，驗符何處呪丹毫。","子陵山曉紅雲密，青草湖平雪浪高。","從此人稀見蹤跡，還應選地種仙桃。",[],{"title":66,"content":67,"tags":72},"月夜懷寄友人",[68,69,70,71],"劍氣徒勞望斗牛，故人別後阻仙舟。","殘春謾道深傾酒，好月那堪獨上樓。","何處是非隨馬足，由來得喪白人頭。","清風未許重携手，幾度高吟寄水流。",[],{"title":74,"content":75,"tags":80},"閒居寄陳山人",[76,77,78,79],"閒居何處得閒名，坐掩衡茅損性靈。","破夢曉鐘聞竹寺，沁心秋雨浸莎庭。","甕邊難負千杯綠，海上終眠萬仞青。","珍重先生全太古，應看名利似浮雲。",[],{"title":82,"content":83,"tags":88},"憶南中",[84,85,86,87],"碧江頭與白雲門，別後秋霜點鬢根。","長記學禪青石寺，最思共醉落花村。","林間竹有湘妃淚，窗外禽多杜宇魂。","未棹扁舟重回首，采薇收橘不堪論。",[],{"title":90,"content":91,"tags":96},"寄友人",[92,93,94,95],"病多慵引架書看，官職無才思已闌。","穴鳳瑞時來却易，人龍別後見何難。","琴樽風月閒生計，金玉松筠舊歲寒。","早晚烟村碧江畔，掛罾重對蓼花灘。",[],{"title":98,"content":99,"tags":104},"別江上一二友生",[100,101,102,103],"國風千載務重華，須逐浮雲背若耶。","無地可歸堪種玉，有天教上且乘槎。","白綸巾卸蘇門月，紅錦衣裁御苑花。","他日成都却回首，東山看取謝鯤家。",[],{"title":106,"content":107,"tags":112},"寄岐山林逢吉明府",[108,109,110,111],"岐山高與隴山連，製錦無私服晏眠。","鸚鵡語中分百里，鳳凰聲裏過三年。","秦無舊俗雲烟媚，周有遺風父老賢。","莫役生靈種楊柳，一枝枝折灞橋邊。",[],{"title":114,"content":115,"tags":120},"感懷呈所知",[116,117,118,119],"十年流落賦歸鴻，誰傍昏衢駕燭龍。","竹屋亂烟思梓澤，酒家疏雨夢臨邛。","千年別恨調琴懶，一片年光覽鏡慵。","早晚休歌白石爛，放教歸去卧羣峰。",[],{"title":122,"content":123,"tags":128},"江上聞笛",[124,125,126,127],"誰爲梅花怨未平，一聲高喚百龍驚。","風當閶闔庭初靜，月在姑蘇秋正明。","曲盡綠楊涵野渡，管吹青玉動江城。","臨流不欲殷勤聽，芳草王孫舊有情。",[],{"title":130,"content":131,"tags":136},"寄孟進士",[132,133,134,135],"依舊池邊草色芳，故人何處憶山陽。","書回科斗江帆暮，曲罷騶虞海樹蒼。","吟望曉烟思桂渚，醉衣殘月夢餘杭。","別來南國知誰在，空對襜褕一斷腸。",[],{"title":138,"content":139,"tags":144},"寄閻記室",[140,141,142,143],"織錦歌成下翠微，豈勞西去問搘機。","未開水府珠先見，不掘豐城劍自輝。","鼇逐玉蟾攀桂上，馬隨青帝踏花歸。","相逢半是雲霄客，應笑歌牛一布衣。",[],{"title":146,"content":147,"tags":152},"幽居寄李秘書",[148,149,150,151],"幾年帝里阻烟波，敢向明時叩角歌。","看盡好花春卧穩，醉殘紅日夜吟多。","印開夕照垂楊柳，畫破寒潭老芰荷。","昨夜前溪有龍鬭，石橋風雨少人過。",[],{"title":154,"content":155,"tags":160},"貽釣魚李處士",[156,157,158,159],"罷吟鸚鵡草芊芊，又泛鴛鴦水上天。","一棹冷涵楊柳雨，片帆香挂芰荷烟。","綠摇江澹萍離岸，紅點雲疏橘滿川。","何處邈將歸畫府，數莖紅蓼一漁船。",[],{"title":162,"content":163,"tags":168},"河橋樓賦得群公夜讌",[164,165,166,167],"芙蓉簾幕扇秋紅，蠻府新郎夜讌同。","滿座馬融吹笛月，一樓張翰過江風。","杯黏紫酒金螺重，談轉琱璫玉麈空。","深荷良宵慰顦顇，德星池館在江東。",[],{"title":170,"content":171,"tags":176},"寄左先輩",[172,173,174,175],"狂歌白鹿上青天，可似蘭塘釣紫烟。","萬卷祖龍坑外物，一泓孫楚耳中泉。","翩翾蠻榼薰晴浦，轂轆魚車響夜船。","學取青蓮李居士，一生杯酒在神仙。",[],{"title":178,"content":179,"tags":184},"貽費道人",[180,181,182,183],"誰如南浦傲烟霞，白葛衣輕稱帽紗。","碧玉蜉蝣迎客酒，黄金轂轆釣魚車。","吟歌雲鳥歸樵谷，卧愛神仙入畫家。","他日鳳書何處覓，武陵烟樹半桃花。",[],{"title":186,"content":187,"tags":192},"寄許下前管記王侍御",[188,189,190,191],"昔年南去得娛賓，頓遜杯前共好春。","蟻泛羽觴蠻酒膩，鳳銜瑶句蜀牋新。","花憐遊騎紅隨轡，草戀征車碧遶輪。","別後青青鄭南陌，不知風月屬何人。",[],{"title":194,"content":195,"tags":200},"秋日圃田送人隨計",[196,197,198,199],"僕射陂前是傳郵，去程鵰鶚弄高秋。","吟拋芍藥裁詩圃，醉下茱萸飲酒樓。","向日迥飛駒皎皎，臨風誰和鹿呦呦。","明年二月仙山下，莫遣桃花逐水流。",[],{"title":202,"content":203,"tags":208},"途次宿友人別墅",[204,205,206,207],"千里崤函一夢勞，豈知雲館共蕭騷。","半簾綠透偎寒竹，一榻紅侵墜晚桃。","蠻酒客稀知味長，蜀琴風定覺弦高。","感君巖下閒招隠，細縷金盤鱠錯刀。",[],{"title":210,"content":211,"tags":216},"春日期巢湖舊事",[212,213,214,215],"暖掠紅香燕燕飛，五雲仙珮曉相携。","花開鸚鵡韋郎曲，竹亞虯龍白帝溪。","富貴萬場歸紫酒，是非千載逐芳泥。","不知多少開元事，露泣春叢向日低。",[],{"title":218,"content":219,"tags":224},"再遊韋曲山寺",[220,221,222,223],"鵲巖烟斷玉巢敧，罨畫春塘太白低。","馬踏翠開垂柳寺，人耕紅破落花蹊。","千年勝概咸原上，幾代荒凉繡嶺西。","碧吐紅芳舊行處，豈堪回首草萋萋。",[],{"title":226,"content":227,"tags":232},"寄徐拾遺",[228,229,230,231],"長竿一繫白龍吟，誰和騶虞發素琴。","野客碧雲魂易斷，故人芳草夢難尋。","天從補後星辰穩，海自潮來島嶼深。","好向明庭拾遺事，莫教玄豹老泉林。",[],{"title":234,"content":235,"tags":240},"秋宿湘江遇雨",[236,237,238,239],"江上陰雲鎖夢魂，江邊深夜舞劉琨。","秋風萬里芙蓉國，暮雨千家薜荔村。","鄉思不堪悲橘柚，旅遊誰肯重王孫。","漁人相見不相問，長笛一聲歸島門。",[241,242,243,244],"写雨","秋天","雨","怀才不遇",{"title":246,"content":247,"tags":252},"貽南康陳處士陶",[248,249,250,251],"白玉堆邊蔣逕橫，空涵二十四灘聲。","老無征戰軒轅國，貧有茅茨帝舜城。","丹鳳晝飛羣木冷，一龍秋卧九江清。","時人莫笑非經濟，還待中原致太平。",[],{"title":254,"content":255,"tags":260},"渭城春晚",[256,257,258,259],"秦樹朦朧春色微，香風烟煖樹依依。","邊城夜靜月初上，芳草路長人未歸。","折柳且堪吟晚檻，弄花何處醉殘暉。","釣鄉千里斷消息，滿目碧雲空自飛。",[],{"title":262,"content":263,"tags":267},"山中春晚寄賈員外",[264,149,265,266],"不隨黄鶴起烟波，應笑無成返薜蘿。","高添雅興松千尺，暗養清音竹數科。","珍重仙曹舊知己，往來星騎一相過。",[],{"title":269,"content":270,"tags":275},"貽浄居寺新及第",[271,272,273,274],"秋池雲下白蓮香，池上吟仙寄竹房。","閒頌國風文字古，靜消心火夢魂凉。","三春蓬島花無限，八月銀河路更長。","此境空門不曾有，從頭好語與醫王。",[],{"title":277,"content":278,"tags":283},"江邊秋夕",[279,280,281,282],"千鍾紫酒薦菖蒲，松島蘭舟瀲灩居。","曲內橘香江客笛，字中嵐氣岳僧書。","吟期汗漫驅金虎，坐約丹青跨玉魚。","七色花虯一聲鶴，幾時乘興上清虛。",[],{"title":285,"content":286,"tags":291},"送僧中孚南歸",[287,288,289,290],"琵琶峽口月溪邊，玉乳頭佗憶舊川。","一錫冷涵蘭徑路，片帆香掛橘洲烟。","苔封石錦棲霞室，水迸衣珠噴玉蟬。","莫道翩翩去如夢，本來吟鳥在林泉。",[],{"title":293,"content":294,"tags":299},"江館秋夕",[295,296,297,298],"耿耿銀河雁半橫，夢敧金碧轆轤輕。","滿窗謝練江風白，一枕齊紈海月明。","楊柳敗梢飛葉響，芰荷香柄折秋鳴。","誰人更唱陽關曲，牢落烟霞夢不成。",[],{"title":301,"content":302,"tags":307},"秋夜同友人話舊",[303,304,305,306],"露下銀河雁度頻，囊中壚火幾時真。","數莖白髮生浮世，一盞寒燈共故人。","雲外簟凉吟嶠月，島邊花暖釣江春。","何當歸去重携手，依舊紅霞作近鄰。",[],{"title":309,"content":310,"tags":315},"古劍",[311,312,313,314],"鑄時天匠待英豪，紫焰寒星匣倍牢。","三尺何年拂塵土，四溟今日絕波濤。","雄應垓下收蛇陣，滯想溪頭伴豹韜。","惜是真龍懶拋擲，夜來衝斗氣何高。",[],{"title":317,"content":318,"tags":323},"寄王侍御",[319,320,321,322],"鳥盡弓藏良可哀，誰知歸釣子陵臺。","鍊多不信黄金耗，吟苦須驚白髮催。","喘月吳牛知夜至，嘶風胡馬識秋來。","燕歌別後休惆悵，黍已成畦菊已開。",[],{"title":325,"content":326,"tags":331},"別何處士陵俊老",[327,328,329,330],"三皇上人春夢醒，東侯老大麒麟生。","洞連龍穴全山冷，窗透鼇波盡室清。","計拙恥居巖麓老，氣狂慚與斗牛平。","誰人爲向青編上，直傍巢由寫一名。",[],{"title":333,"content":334,"tags":336},"句  其一",[335],"流水物情諳世態，落花春夢厭塵勞。",[],{"title":338,"content":339,"tags":341},"句  其二",[340],"織檻錦紋苔乍結，墮書花印菊初殘。",[],{"title":343,"content":344,"tags":346},"句  其三",[345],"光陰老去無成事，富貴不來爭奈何。",[],{"title":348,"content":349,"tags":351},"句  其四",[350],"眠雲無限好知己，應笑不歸花滿樽。",[],"content:tan-yong-zhi.json","json","content","tan-yong-zhi.json",[357,360,363,366,369,372,375,378,381,384,387,390,393,396,399,402,405,408,411,414,417,420,423,426,429,432,435,438,441,444,447,450,453,456,459,462,465,468,471,474,477,480,483,486,489,492,495,498,501,504,507,510,513,516,519,522,525,528,531,534,537,540,543,546,549,552,555,558,561,564,567,570,573,576,579,582,585,588,591,594,597,600,603,606,609,612,615,618,621,624,627,630,633,636,639,642,645,648,651,654,657,660,663,666,669,672,675,678,681,684,687,690,693,696,699,702,705,708,711,714,717,720,723,726,729,732,735,738,741,744,747,750,753,756,759,762,765,768,771,774,777,780,783,786,789,792,795,798,801,804,807,810,813,816,819,822,825,828,831,834,837,840,843,846,849,852,855,858,861,864,867,870,873,876,879,882,885,888,891,894,897,900,903,906,909,912,915,918,921,924,927,930,933,936,939,942,945,948,951,954,957,960,963,966,969,972,975,978,981,984,987,990,993,996,999,1002,1005,1008,1011,1014,1017,1020,1023,1026,1029,1032,1035,1038,1041,1044,1047,1050,1053,1056,1059,1062,1065,1068,1071,1074,1077,1080,1083,1086,1089,1092,1095,1098,1101,1104,1107,1110,1113,1116,1119,1122,1125,1128,1131,1134,1137,1140,1143,1146,1149,1152,1155,1158,1161,1164,1167,1170,1173,1176,1179,1182,1185,1188,1191,1194,1197,1200,1203,1206,1209,1212,1215,1218,1221,1224,1225,1228,1231,1234,1237,1240,1243,1246,1249,1252,1255,1258,1261,1264,1267,1270,1273,1276,1279,1282,1285,1288,1291,1294,1297,1300,1303,1306,1309,1312,1315,1318,1321,1324,1327,1330,1333,1336,1339,1342,1345,1348,1351,13