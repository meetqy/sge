[{"data":1,"prerenderedAt":2002},["Reactive",2],{"content-query-AJrNrdYRBW":3,"content-navigation-8C37fagqQL":905},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":901,"_type":902,"_source":903,"_file":904,"_extension":902},"/song-bai","",false,"宋白",[9,15,21,27,33,39,45,51,57,63,69,75,81,87,93,99,105,111,117,123,129,135,141,147,153,159,165,171,177,183,189,195,201,207,213,219,225,231,237,243,249,255,261,267,273,279,285,291,297,303,309,315,321,327,333,339,345,351,357,363,369,375,381,387,393,399,406,412,418,424,430,436,442,448,454,460,466,472,478,484,490,496,502,508,514,520,526,532,538,544,550,556,562,568,574,580,586,592,598,604,610,616,622,628,634,640,646,652,658,664,672,680,688,694,702,710,718,726,734,746,754,762,770,778,786,794,802,810,818,826,834,841,846,851,856,861,866,871,876,881,886,891,896],{"title":10,"content":11,"tags":14},"宮詞  其一",[12,13],"千官朝謁五門前，一路春風蠟燭烟。","宮漏乍停班已定，紫雲開處望堯天。",[],{"title":16,"content":17,"tags":20},"宮詞  其二",[18,19],"萬國車書一太平，宮花無數管弦聲。","近臣入奏新祥瑞，昨夜黄河徹底清。",[],{"title":22,"content":23,"tags":26},"宮詞  其三",[24,25],"律管飛灰報早春，壽陽梅淡落香裀。","詞人競進新詩入，俊思無過白舍人。",[],{"title":28,"content":29,"tags":32},"宮詞  其四",[30,31],"十二樓前御柳垂，九重城裏百花時。","宣州恰進紅絲毯，便遣宮娥舞柘枝。",[],{"title":34,"content":35,"tags":38},"宮詞  其五",[36,37],"彤廷立仗雪初銷，仙樂空懸讌百寮。","齊指終南稱萬壽，遠平香案碧岧嶢。",[],{"title":40,"content":41,"tags":44},"宮詞  其六",[42,43],"樓前宣赦掣金雞，大禮新成彩仗歸。","萬歲聲高天地喜，慶雲飛上衮龍衣。",[],{"title":46,"content":47,"tags":50},"宮詞  其七",[48,49],"中使傳宣出未央，外宮庭燎發榮光。","怪來連夜催儀注，元是平明冊五王。",[],{"title":52,"content":53,"tags":56},"宮詞  其八",[54,55],"掖庭宮女僅千人，有勅都教放出門。","戀主頓忘幽憤意，促辭中使盡啼痕。",[],{"title":58,"content":59,"tags":62},"宮詞  其九",[60,61],"翰苑新除李謫仙，鼇宮春雨濕花磚。","宣來立草和親詔，一筆書成對御前。",[],{"title":64,"content":65,"tags":68},"宮詞  其一○",[66,67],"龍腦天香撒地衣，錦書新冊太真妃。","宮官一夜鋪黄道，却踏金蓮步步歸。",[],{"title":70,"content":71,"tags":74},"宮詞  其一一",[72,73],"飛烟縹緲拂樓臺，纔捲珠簾雉扇開。","春滿天顔兩眉喜，玉關新有捷書來。",[],{"title":76,"content":77,"tags":80},"宮詞  其一二",[78,79],"水殿東西夾翠樓，春乘步輦過瀛洲。","赭袍如日花如錦，一半宮人盡裹頭。",[],{"title":82,"content":83,"tags":86},"宮詞  其一三",[84,85],"繡鞍新轡飾玫瑰，隨駕堤邊試馬回。","苑蕙香輕沙路濕，玉蹄沾得落花來。",[],{"title":88,"content":89,"tags":92},"宮詞  其一四",[90,91],"帝誥皇墳味有餘，萬幾無暇旋生疏。","終須別置三千女，分記金華殿裏書。",[],{"title":94,"content":95,"tags":98},"宮詞  其一五",[96,97],"後苑秋晴校獵歸，淡粧宮女盡戎衣。","數中供奉誰精熟，斜勒驕驄掠草飛。",[],{"title":100,"content":101,"tags":104},"宮詞  其一六",[102,103],"春雪輕輕灑露盤，真珠箔外玉勾欄。","仙妃未起宮娃小，錯報西樓曉月寒。",[],{"title":106,"content":107,"tags":110},"宮詞  其一七",[108,109],"玉殿金扉夜不扃，露華如水洗圓靈。","昭陽女伴新承寵，心祝君王拜壽星。",[],{"title":112,"content":113,"tags":116},"宮詞  其一八",[114,115],"宮門閒掩獸鐶斜，微雨新晴滿地花。","番次未當車駕出，却尋同伴學宣麻。",[],{"title":118,"content":119,"tags":122},"宮詞  其一九",[120,121],"離宮春晚雨霏霏，日晚君王带雨歸。","翠輦急行紅袂濕，中途方憶瓦油衣。",[],{"title":124,"content":125,"tags":128},"宮詞  其二○",[126,127],"天玉風清北內聞，東朝相慶冊儲君。","春坊曉奏元良瑞，雞戟門西現彩雲。",[],{"title":130,"content":131,"tags":134},"宮詞  其二一",[132,133],"雙雙金剎出宮牆，內寺幡花散道場。","妃子不來春雨細，玉真公主代行香。",[],{"title":136,"content":137,"tags":140},"宮詞  其二二",[138,139],"歡笑聲高出禁園，祿兒新樣繡初翻。","青娥阿監知非禮，心怕君王不敢言。",[],{"title":142,"content":143,"tags":146},"宮詞  其二三",[144,145],"貪領吳娃唱鷓鴣，洞簫詞賦頓生疏。","沉吟不記當時字，却向宮中問婕妤。",[],{"title":148,"content":149,"tags":152},"宮詞  其二四",[150,151],"碁品從來重一先，後庭無事綠窗閑。","昭容不賭尋常物，言得千金紫玉環。",[],{"title":154,"content":155,"tags":158},"宮詞  其二五",[156,157],"砌臺春暖映垂楊，皇子皇孫戲艷陽。","半騃半驕花裏鬧，真珠線斷紫羅囊。",[],{"title":160,"content":161,"tags":164},"宮詞  其二六",[162,163],"水晶宮閉月光鋪，羯鼓聲乾雨點麤。","一曲未終餘思在，急宣前殿喚花奴。",[],{"title":166,"content":167,"tags":170},"宮詞  其二七",[168,169],"春宵宮女著春綃，鈴索無風自動摇。","晝下珠簾猧子睡，紅蕉窠下對芭蕉。",[],{"title":172,"content":173,"tags":176},"宮詞  其二八",[174,175],"微雨點月漢宮秋，水殿疏螢黑處流。","鳳管不調慵進曲，却排銀燭夜藏鈎。",[],{"title":178,"content":179,"tags":182},"宮詞  其二九",[180,181],"竹宮春祀望神仙，百和烟高雜絳烟。","中夜上元來玉座，五雲隨鳳駕珠軿。",[],{"title":184,"content":185,"tags":188},"宮詞  其三○",[186,187],"小蘇年少最多才，半酒扶將入內來。","重飲玉鍾方下筆，思泉如涌不勞催。",[],{"title":190,"content":191,"tags":194},"宮詞  其三一",[192,193],"昨日傳宣喚打毬，星丸月杖奉宸遊。","上陽宮女偏蹻捷，爭得樓前第一籌。",[],{"title":196,"content":197,"tags":200},"宮詞  其三二",[198,199],"紅騎翩翩小繡旗，苑中秋雁恰來時。","何妨十日方歸內，料得千官總不知。",[],{"title":202,"content":203,"tags":206},"宮詞  其三三",[204,205],"蕭蕭宮樹正秋風，虎圈門開輦路通。","金屋阿嬌心膽小，却嗔馮媛獨當熊。",[],{"title":208,"content":209,"tags":212},"宮詞  其三四",[210,211],"御府書蹤字字奇，散教宮女倣來時。","研精筆法難停穩，併恨羲之與獻之。",[],{"title":214,"content":215,"tags":218},"宮詞  其三五",[216,217],"去年因戲賜霓裳，權戴金冠奉玉皇。","久著淡黄心覺厭，春來不敢便紅粧。",[],{"title":220,"content":221,"tags":224},"宮詞  其三六",[222,223],"驃國朝天進樂時，宮商依約似龜茲。","教坊風便伶官俊，全曲偷將並不知。",[],{"title":226,"content":227,"tags":230},"宮詞  其三七",[228,229],"道院秋眠對綺疏，翻思十五入宮初。","花前聽得君王道，禁掖三千總不如。",[],{"title":232,"content":233,"tags":236},"宮詞  其三八",[234,235],"天子今朝就食遲，延英徵得逸人歸。","靈臺監裏知嘉瑞，隔月先曾奏少微。",[],{"title":238,"content":239,"tags":242},"宮詞  其三九",[240,241],"彤管先修女史箴，清才絕妙繼詞林。","玉纖親出黄金賜，愈見中宮逮下心。",[],{"title":244,"content":245,"tags":248},"宮詞  其四○",[246,247],"白蓮亭下柳垂絲，正是春波涮馬時。","宮女隔花相倚看，如龍步驟出漣漪。",[],{"title":250,"content":251,"tags":254},"宮詞  其四一",[252,253],"慢拍紅牙轉眼波，念奴樓上一聲歌。","春喉爽朗天顔喜，不遣簾前樂器和。",[],{"title":256,"content":257,"tags":260},"宮詞  其四二",[258,259],"樂府新呈按舞圖，教時仍遣促功夫。","梨園弟子知天意，月五樓前欲大酺。",[],{"title":262,"content":263,"tags":266},"宮詞  其四三",[264,265],"南宮春牓奏賢良，萬乘臨軒試一場。","賦出御題深稱旨，狀元先拜秘書郎。",[],{"title":268,"content":269,"tags":272},"宮詞  其四四",[270,271],"春雲如鶴惹銀臺，銀牓宮門隔水開。","女史尚書供甲令，玉妃明日祀高禖。",[],{"title":274,"content":275,"tags":278},"宮詞  其四五",[276,277],"御製新成喜雪篇，六宮傳寫滿香牋。","金鑾殿女曾偷和，不敢將呈萬乘前。",[],{"title":280,"content":281,"tags":284},"宮詞  其四六",[282,283],"雪鶻輕籠好六梢，五方供進內家調。","靈和殿上乘風颺，一點秋星印碧霄。",[],{"title":286,"content":287,"tags":290},"宮詞  其四七",[288,289],"宮花灼灼柳綿綿，一道春風響靜鞭。","駕出未知何處去，月華池上教龍舡。",[],{"title":292,"content":293,"tags":296},"宮詞  其四八",[294,295],"絲絲新織御{革斿}韁，紫燕春鞍照地光。","雙控墜梢皆侍女，玉鞭柔軟拂垂楊。",[],{"title":298,"content":299,"tags":302},"宮詞  其四九",[300,301],"龍焙中春進乳茶，金瓶湯沃越甌花。","玉堂宣賜元承旨，明日登庸降白麻。",[],{"title":304,"content":305,"tags":308},"宮詞  其五○",[306,307],"新生帝子浴漪蘭，三日宮中列綺筵。","嬪御稱觴呼萬歲，笙歌一片夜掀天。",[],{"title":310,"content":311,"tags":314},"宮詞  其五一",[312,313],"日影風烏樹影濃，黄門輕騎疾如風。","一行御札緣何事，勅借寧王避暑宮。",[],{"title":316,"content":317,"tags":320},"宮詞  其五二",[318,319],"如何帝女降鈞天，澤國增封舊粉田。","仙史多才顔似玉，乘鸞消息在今年。",[],{"title":322,"content":323,"tags":326},"宮詞  其五三",[324,325],"金堂曲讌夜厭厭，仙樂聲清御酒黏。","不用司宮排蠟燭，海人新貢夜明簾。",[],{"title":328,"content":329,"tags":332},"宮詞  其五四",[330,331],"雪梅初謝小桃芳，院院鶑啼報海棠。","昨日司花新奉勅，後園差使結丁香。",[],{"title":334,"content":335,"tags":338},"宮詞  其五五",[336,337],"甘州唱罷曉光新，簇簇才人與美人。","不分昭陽趙飛燕，漢皇誇是掌中身。",[],{"title":340,"content":341,"tags":344},"宮詞  其五六",[342,343],"天上雞人唱曉籌，嚴粧鐘動景陽樓。","千門競洗燕脂面，流作香波入御溝。",[],{"title":346,"content":347,"tags":350},"宮詞  其五七",[348,349],"東宮鸞鳳卦中飛，花燭熒煌夜納妃。","明日龍樓歌舞罷，門前掃聚碎珠璣。",[],{"title":352,"content":353,"tags":356},"宮詞  其五八",[354,355],"珠爲雙珮玉爲簫，御女長眉稱細腰。","詔賜公卿縱遊賞，春來頻放紫宸朝。",[],{"title":358,"content":359,"tags":362},"宮詞  其五九",[360,361],"春風法曲舞霓裳，繡罷深宮請賜香。","司寶傳言貴妃坐，真珠簾映雪衣娘。",[],{"title":364,"content":365,"tags":368},"宮詞  其六○",[366,367],"紅牋一幅捲明霞，纖手題詩寄大家。","檀口微吟遶廊柱，濛濛春雨濕梨花。",[],{"title":370,"content":371,"tags":374},"宮詞  其六一",[372,373],"麝波蘭灩湧溫湯，白玉蓮花吐異香。","春雪怕融偎侍女，尚衣新進蹙金裳。",[],{"title":376,"content":377,"tags":380},"宮詞  其六二",[378,379],"鸚鵡今朝在輦竿，輦官防護換肩難。","逡巡漸入花深處，却索低驄鞴玉鞍。",[],{"title":382,"content":383,"tags":386},"宮詞  其六三",[384,385],"裹頭宮女淺粧梳，選入昭陽奉玉輿。","一曲新聲春稱旨，賜緋教賜小金魚。",[],{"title":388,"content":389,"tags":392},"宮詞  其六四",[390,391],"春賜千官讌喜錢，任從行樂太平年。","三十六所春宮館，一一香風送管弦。",[],{"title":394,"content":395,"tags":398},"宮詞  其六五",[396,397],"水心高殿接蘭塘，纔拂龍鬚一簟凉。","瑟瑟枕函雲母帳，冰盤仙果近人香。",[],{"title":400,"content":401,"tags":404},"宮詞  其六六",[402,403],"春封東嶽罷泥金，羽衛還宮禁禦深。","昨夜祥風來入律，侍臣朝進五弦琴。",[405],"泰山",{"title":407,"content":408,"tags":411},"宮詞  其六七",[409,410],"萬方琛贄泛雲濤，來慶星樞電彩高。","讌罷樂聲移別殿，對花重換赭紅袍。",[],{"title":413,"content":414,"tags":417},"宮詞  其六八",[415,416],"年來春早過新豐，只向官池祭玉龍。","一片香雲千里雨，樊川杜曲繡重重。",[],{"title":419,"content":420,"tags":423},"宮詞  其六九",[421,422],"繡騎前驅盡國娃，芳園初看牡丹花。","千官狼狽如蜂蝶，迴避楊妃七寶車。",[],{"title":425,"content":426,"tags":429},"宮詞  其七○",[427,428],"春水綾紋積翠池，凌波仙鳥拂花枝。","黄衣宮使宣閻相，錯入中書喚畫師。",[],{"title":431,"content":432,"tags":435},"宮詞  其七一",[433,434],"魚龍百戲鬧嘈嘈，戲罷珠樓日色高。","移入芳林排小宴，赤瑛盤裏進櫻桃。",[],{"title":437,"content":438,"tags":441},"宮詞  其七二",[439,440],"外國人來獻寶珠，瑠璃雙眼赤金鬚。","奇形不欲宮中見，詔遣滕王進畫圖。",[],{"title":443,"content":444,"tags":447},"宮詞  其七三",[445,446],"蠶館春晴陰綠桑，親蠶天氣日初長。","九嬪參酌前朝禮，須戴金釵十二行。",[],{"title":449,"content":450,"tags":453},"宮詞  其七四",[451,452],"天地儲祥集鳳麟，凌烟高閣寫功臣。","宮中記得鴉兒句，偏問虞南第幾人。",[],{"title":455,"content":456,"tags":459},"宮詞  其七五",[457,458],"禁臠成功大殿邦，入朝宮騎繡雙雙。","九霄弟子黄金輦，十萬人家憑看窗。",[],{"title":461,"content":462,"tags":465},"宮詞  其七六",[463,464],"春來車駕入西都，父老千人拜路隅。","潛問馬嵬端正樹，枝條猶似舊家無。",[],{"title":467,"content":468,"tags":471},"宮詞  其七七",[469,470],"臘日天寒苑獵遲，獵迴諸衛齪槍旗。","女真王子新朝覲，旋遣黄門賜口脂。",[],{"title":473,"content":474,"tags":477},"宮詞  其七八",[475,476],"寒食宮中也禁烟，鬰金堂北畫鞦韆。","梨花明月皆如雪，時送清香到酒前。",[],{"title":479,"content":480,"tags":483},"宮詞  其七九",[481,482],"玉管金箏入夜調，今宵靈匹下青霄。","穿針鬭巧香烟濕，樓閣層層拂虹橋。",[],{"title":485,"content":486,"tags":489},"宮詞  其八○",[487,488],"繡衣宮女把宮門，不遣中人謁至尊。","晚霽微行向何處，灞陵原接杏花村。",[],{"title":491,"content":492,"tags":495},"宮詞  其八一",[493,494],"殿角風琴細韻移，燭臺春幸翰林時。","三更向盡諸宮閉，猶在金鑾夜賦詩。",[],{"title":497,"content":498,"tags":501},"宮詞  其八二",[499,500],"幕次春排曉御樓，千官賀雨拜珠旒。","斜陽却幸鴛鴦殿，袍袴宮人盡築毬。",[],{"title":503,"content":504,"tags":507},"宮詞  其八三",[505,506],"驪山講武六師迴，日月旗高過苑來。","鞭入夾城簫鼓動，材林輕騎鬧如雷。",[],{"title":509,"content":510,"tags":513},"宮詞  其八四",[511,512],"蟲響金階擣練天，玉狻呀口噴香烟。","來宵待賞中秋月，剩取珊瑚飣御筵。",[],{"title":515,"content":516,"tags":519},"宮詞  其八五",[517,518],"月殿聞隨羽客過，桂宮門外近天河。","常娥裝束還何似，潛問君王記得麽。",[],{"title":521,"content":522,"tags":525},"宮詞  其八六",[523,524],"便路橫穿玉兔門，夜宣狎客讌金罇。","江郎才俊思華別，賜坐從容索繡墩。",[],{"title":527,"content":528,"tags":531},"宮詞  其八七",[529,530],"重陽菊蘂泛香醪，上壽因添飲興高。","玉項琵琶猶未快，別宣金鳳紫檀槽。",[],{"title":533,"content":534,"tags":537},"宮詞  其八八",[535,536],"上苑欣逢祓禊辰，踏青因過望賢春。","連昌宮使開金鎖，步障花深不見人。",[],{"title":539,"content":540,"tags":543},"宮詞  其八九",[541,542],"節假君王不坐朝，鬭殘芳草日難銷。","樗蒲長幸皆宮戲，輸與昭容玉步摇。",[],{"title":545,"content":546,"tags":549},"宮詞  其九○",[547,548],"花萼樓高望柳堤，春橋橫水短虹蜺。","五陵游俠翩翩過，半脫朱袍鬭錦雞。",[],{"title":551,"content":552,"tags":555},"宮詞  其九一",[553,554],"虢國威儀過薛王，擔夫爭路氣揚揚。","宮嬪後出香車送，颭碎釵頭玉鳳凰。",[],{"title":557,"content":558,"tags":561},"宮詞  其九二",[559,560],"秋天如水月如霜，玉笛風清曲破長。","衆樂任高聲迥別，六宮齊道是君王。",[],{"title":563,"content":564,"tags":567},"宮詞  其九三",[565,566],"今夜金吾不禁街，燈山火樹准宣牌。","長安一片星河裏，丹鳳樓高衆樂諧。",[],{"title":569,"content":570,"tags":573},"宮詞  其九四",[571,572],"太液春波淺盪舟，花如血點水如油。","波臣莫織青絲網，自有銀輪翡翠鈎。",[],{"title":575,"content":576,"tags":579},"宮詞  其九五",[577,578],"五嶽無塵四海清，太平天子愛長生。","蓬萊方士壺中宿，旋種仙芝一夜成。",[],{"title":581,"content":582,"tags":585},"宮詞  其九六",[583,584],"南內天花照從官，上皇春醉聖情歡。","月中閒說驪山事，笑向將軍道阿瞞。",[],{"title":587,"content":588,"tags":591},"宮詞  其九七",[589,590],"繡額珠簾窣地重，微風吹動牡丹枝。","金籠鸚鵡耽春睡，忘却新教御製詩。",[],{"title":593,"content":594,"tags":597},"宮詞  其九八",[595,596],"便殿沉沉永巷秋，宮中擊拂最風流。","參差玉指和花數，恰到君王自得頭。",[],{"title":599,"content":600,"tags":603},"宮詞  其九九",[601,602],"宮棋初罷請銀笙，合曲偏宜鈿面箏。","未伏延年花下唱，金錢百萬賞新聲。",[],{"title":605,"content":606,"tags":609},"宮詞  其一○○",[607,608],"春營小殿號披香，宣借天孫作學堂。","李白宮詞多好句，側書紅壁兩三行。",[],{"title":611,"content":612,"tags":615},"嘉州",[613,614],"嘉陽信美郡，風物眷多奇。","有湖題明月，有樓名荔枝。",[],{"title":617,"content":618,"tags":621},"普賢寺",[619,620],"菩薩悲深居此地，峨眉名重鎮南州。","不知立處高多少，只見星辰在下頭。",[],{"title":623,"content":624,"tags":627},"七夕",[625,626],"纖吹浮雲薦，非因散巧樓。","靈儔不可覿，仙溪泛悠悠。",[],{"title":629,"content":630,"tags":633},"中秋感懷  其一",[631,632],"去年今夜此堂前，人正清歌月正圓。","今夜秋來人且散，不如雲霧蔽青天。",[],{"title":635,"content":636,"tags":639},"中秋感懷  其二",[637,638],"雲彩冰光似去年，去年人月兩嬋娟。","今宵不辨連明坐，拚醉抽身黑處眠。",[],{"title":641,"content":642,"tags":645},"九日  其一",[643,644],"霜冷風清九月九，茱萸黄菊家園有。","何時玉殿接千官，稱觴共進南山壽。",[],{"title":647,"content":648,"tags":651},"九日  其二",[649,650],"陶家籬落菊花開，醉擬西風立釣臺。","一座良交皆不見，數聲燕雁戛雲來。",[],{"title":653,"content":654,"tags":657},"九日  其三",[655,656],"秋色蕭蕭野水邊，茱萸時節菊花天。","明時未達青雲晚，惆悵登高又一年。",[],{"title":659,"content":660,"tags":663},"贈麻仲英",[661,662],"宣毫歙墨川箋紙，寄與麻家小秀才。","七歲能吟天骨異，前生應折桂枝來。",[],{"title":665,"content":666,"tags":671},"中酒",[667,668,669,670],"中酒事俱妨，偷眠就黑房。","靜嫌鸚鵡鬧，渴憶荔枝香。","病與慵相續，心和夢尚狂。","從今改題品，不號醉爲鄉。",[],{"title":673,"content":674,"tags":679},"雲",[675,676,677,678],"化作奇形隨物態，散成文錦自天機。","紅堆遠嶠日初上，碧滿長空人未歸。","逸興狂來連海闊，高情間甚出山飛。","須知深處藏雷雨，不獨江邊遶釣磯。",[],{"title":681,"content":682,"tags":687},"一春",[683,684,685,686],"一春情調淡悠悠，閒倚書窗背小樓。","暖日只添中酒睡，晚風頻動惜花愁。","鶑衝舞蝶侵人過，絮逐天絲觸處遊。","已爲韶光發惆悵，可堪家近白蘋洲。",[],{"title":689,"content":690,"tags":693},"春",[691,692],"隔岸黄鸝語，當軒白鳥斜。","晚來風緊處，飛絮滿人家。",[],{"title":695,"content":696,"tags":701},"花",[697,698,699,700],"誰將枝葉染還裁，却使馨香次第來。","春柄定教三月主，天心合與一齊開。","如何零露方滋菊，豈得東風不爲梅。","低蹙艷容愁似語，也疑由命不由才。",[],{"title":703,"content":704,"tags":709},"惜別詩",[705,706,707,708],"年來又惜別清塵，故國榮過喜風新。","竹葉莫辭千日酒，梅花惟寄一枝春。","苦心曾未拋黄卷，得路終期近紫宸。","不待政成迎風詔，頌聲洋溢桂州人。",[],{"title":711,"content":712,"tags":717},"寄蔡崑",[713,714,715,716],"却憶醉相逢，雲深太白東。","離別經臘雪，魂夢又春風。","芳草晴天闊，黄鸝細雨中。","倚筇愁望處，烟樹遠濛籠。",[],{"title":719,"content":720,"tags":725},"黄陵二妃廟",[721,722,723,724],"重瞳天子狩南荒，二女音容已渺茫。","靜聽瑶琴當皓月，再聞珠淚灑幽篁。","松愁老鶴清無夢，水映疏螢冷放光。","一夜遼遼千古意，香魂依約遶瀟湘。",[],{"title":727,"content":728,"tags":733},"送陳堯叟赴廣西漕",[729,730,731,732],"莫辭征騎去迢迢，盡瀉純誠許聖朝。","北闕皇恩從此布，南方沴氣必然銷。","爲詩自可怡情性，服藥嘗聞助攝調。","仙館粉闈官職好，定期騫翥上烟霄。",[],{"title":735,"content":736,"tags":745},"狂吟八韻送英公暫歸故鄉遷殯二親",[737,738,739,740,741,742,743,744],"十八般書四海傳，長安話別已多年。","今來帝里重相見，轉覺師心更自然。","下筆入神皆得法，出言成句盡通玄。","花時乘興先行樂，月夜忘機靜坐禪。","到處僧俗爭識面，滿朝英俊贈佳篇。","長沙母葬彰純孝，八水人思有善緣。","衡嶽醉投秋雨寺，漢江吟渡夕陽舡。","前期指在春三月，迴首東風好著鞭。",[],{"title":747,"content":748,"tags":753},"牡丹詩十首  其一",[749,750,751,752],"□應春處即□春，粉面□相□番新。","語得必傳天上事，摘來真是掌中身。","百花推戴教爲主，□日妖□欲□人。","無可奈何□愛惜，少偷金蘂咽芳津。",[],{"title":755,"content":756,"tags":761},"牡丹詩十首  其二",[757,758,759,760],"日華鮮麗露華濃，梳流香朝競□容。","來在夢中甘化蝶，入伊心裏不如蜂。","宮娥捧擁西施醉，天樂喧闐虢國風。","高拗浮□須繼燭，玳簪分載酒千鍾。",[],{"title":763,"content":764,"tags":769},"牡丹詩十首  其三",[765,766,767,768],"春心相惜最相親，湘葉忩忩換翠裙。","雙蒂喜如嬪二女，一枝愁似寡文君。","艷繁誰卷真珠看，香好曾歌琥珀聞。","深染鮫綃籠玉檻，莫教飛去作卿雲。",[],{"title":771,"content":772,"tags":777},"牡丹詩十首  其四",[773,774,775,776],"烟容粉態傍歌樓，半似窺人半似羞。","把筆乍題先巧笑，憑欄微喚不迴頭。","吹乾玉笛香猶在，槌破靈鼉愛未休。","更得黄鸝將粉蝶，東西南北說風流。",[],{"title":779,"content":780,"tags":785},"牡丹詩十首  其五",[781,782,783,784],"澹黄容止間深檀，妥媠香紅露未乾。","和淚似嫌春漸老，向人如說夜來寒。","粧成有樣教天媛，禮絕無心下國蘭。","針繡筆描俱未是，好風相倚笑邊鸞。",[],{"title":787,"content":788,"tags":793},"牡丹詩十首  其六",[789,790,791,792],"錦爲行障繡爲衾，不殺猩猩色已深。","花譜揚名居一品，藥欄纔見賞千金。","誰忘正爲褰珠箔，得意惟能挑玉琴。","洛水橋南三月裏，兩無言語各知心。",[],{"title":795,"content":796,"tags":801},"牡丹詩十首  其七",[797,798,799,800],"風排香陣拂瑶墀，御苑新晴爛漫時。","十五素娥羞水色，三千紅面洗胭脂。","宮腰暎酒思輕舞，檀口偎牋欲詠詩。","鐵石心腸爲君變，多情多恨一枝枝。",[],{"title":803,"content":804,"tags":809},"牡丹詩十首  其八",[805,806,807,808],"水精冠葉鏤春冰，巧思鎸研仿未能。","風砑紅綃光點血，暖銷金鏤細含稜。","韶容旖旎終無比，晚艷低佪更可憎。","戲脫仙衣親手覆，香身柔軟力難勝。",[],{"title":811,"content":812,"tags":817},"牡丹詩十首  其九",[813,814,815,816],"春風平地謫花仙，紅裊生香下九天。","艷欲背身垂玉筯，動如移步索金蓮。","含情待去爲雲雨，忍笑佯來聽管弦。","朧月輕寒應不慣，夜深渾擬傍藂眠。",[],{"title":819,"content":820,"tags":825},"牡丹詩十首  其一○",[821,822,823,824],"許多顔色泥春嬌，就裏輕紅最□饒。","生處地須名紫府，落時魂想返青霄。","霞英散焰燒纖手，藥粉飄香著步摇。","輕剪自憐支節痛，倚烟啼露一無憀。",[],{"title":827,"content":828,"tags":833},"題義門胡氏華林書院",[829,830,831,832],"君家仙館带村塘，氣象清鮮雅趣長。","千里客來如海納，一樓書靜透山光。","門閭旌表芝泥貴，科篇聯翩桂籍香。","帝里詞人多景慕，謾題詩句滿修篁。",[],{"title":835,"content":836,"tags":840},"句  其一",[837,838,839],"吉音來碧落，帖子報紅牋。","清夜驚神王，朧明到省前。","風中宮漏盡，日出榜繩懸。",[],{"title":842,"content":843,"tags":845},"句  其二",[844],"勞君贈我古風詩，逋翁所謂撲凸奇。",[],{"title":847,"content":848,"tags":850},"句  其三",[849],"宋朝歸聖主，潘閬是詩人。",[],{"title":852,"content":853,"tags":855},"句  其四",[854],"凌晨走退蝦蟆鼻，幾日飛經灩澦堆。",[],{"title":857,"content":858,"tags":860},"句  其五",[859],"春風麥隴連蠻芊，細雨梨花問海棠。",[],{"title":862,"content":863,"tags":865},"句  其六",[864],"竹底衙人吏，花間押簿書。",[],{"title":867,"content":868,"tags":870},"句  其七",[869],"峨眉多雨雅江秋，溢岸清波入郡流。",[],{"title":872,"content":873,"tags":875},"句  其八",[874],"最念春風嘗酒處，梅酸笋翠後溪頭。",[],{"title":877,"content":878,"tags":880},"句  其九",[879],"玉津縣裏三年悶，金粟山前九月愁。",[],{"title":882,"content":883,"tags":885},"句  其一○",[884],"梅雪初銷臘酒香，嘉州屬縣且尋芳。",[],{"title":887,"content":888,"tags":890},"句  其一一",[889],"道出嘉陽正西指，一带靈峰如湧起。",[],{"title":892,"content":893,"tags":895},"句  其一二",[894],"回眸已覺三山近，滿壁潛驚五月寒。",[],{"title":897,"content":898,"tags":900},"句  其一三",[899],"昔日曾爲尺木階，今朝真是青雲友。",[],"content:song-bai.json","json","content","song-bai.json",[906,909,912,915,918,921,924,927,930,933,936,939,942,945,948,951,954,957,960,963,966,969,972,975,978,981,984,987,990,993,996,999,1002,1005,1008,1011,1014,1017,1020,1023,1026,1029,1032,1035,1038,1041,1044,1047,1050,1053,1056,1059,1062,1