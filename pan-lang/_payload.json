[{"data":1,"prerenderedAt":1729},["Reactive",2],{"content-query-8fHJ3F4UaO":3,"content-navigation-8C37fagqQL":632},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":628,"_type":629,"_source":630,"_file":631,"_extension":629},"/pan-lang","",false,"潘閬",[9,17,25,37,45,53,61,71,79,87,95,103,111,119,127,135,143,151,159,167,175,183,191,199,207,215,223,231,239,247,255,263,271,279,287,295,303,311,319,327,333,339,345,351,357,364,370,377,383,389,395,401,407,413,419,425,431,437,443,449,455,461,467,475,481,487,495,501,509,517,523,531,538,544,552,560,568,573,578,583,588,593,598,603,608,613,618,623],{"title":10,"content":11,"tags":16},"送王長洲禹偁赴闕",[12,13,14,15],"蒿蘭不並香，涇渭安同流。","小人有千險，君子生百憂。","名重聖主徵，道光史策收。","一鶚秋空飛，鳥雀徒啾啾。",[],{"title":18,"content":19,"tags":24},"曉泊嶀浦寄剡縣劉貺員外",[20,21,22,23],"曉汎剡溪水，晚見剡溪山。","徘徊駐行櫂，待月思再還。","漁唱深潭上，鳥棲高樹間。","應當金石交，念我無暫閒。",[],{"title":26,"content":27,"tags":36},"金山寺留題",[28,29,30,31,32,33,34,35],"金山碧崔嵬，我泛扁舟來。","虛閣登還下，長廊去得迴。","梵剎絕頂立，僧房八面開。","波濤起蛟蜃，洞穴生風雷。","千載有高松，萬古無纖埃。","葛衣惹秋雲，草履黏蒼苔。","孤城寒角動，片帆暮鐘催。","朗吟成此章，欲返猶徘徊。",[],{"title":38,"content":39,"tags":44},"闕下留別孫丁二學士歸舊山",[40,41,42,43],"名利路萬轍，我來意如何。","紅塵三尺深，中有是非波。","波翻幾潛沒，來者猶更過。","歸去感知淚，永灑青松柯。",[],{"title":46,"content":47,"tags":52},"贈道士王介",[48,49,50,51],"頭冠星斗光，七十鬢不蒼。","篋有化金方，詩無入俗章。","舉步雲霞輕，出語芝朮香。","約我游羅浮，絕頂天風凉。",[],{"title":54,"content":55,"tags":60},"雪夜有感",[56,57,58,59],"大雪擁蓬戶，寒夢不成歸。","孤坐北窗風，飄落辭家衣。","向曉酒力減，背壁燈影微。","誰知游子心，暗逐長空飛。",[],{"title":62,"content":63,"tags":70},"贈馮德之道士",[64,65,66,67,68,69],"上清消息久不聞，紫鸞丹鳳應離羣。","羲和縱轡日又曛，九衢旦暮徒紛紛。","誰識天書數行字，識者便是真僊子。","直言鉛汞成還丹，鍊之餌之身不死。","嵩陽道士早來別，纔承天澤辭玉闕。","紫章深秘莫漏泄，他年華頂爲君說。",[],{"title":72,"content":73,"tags":78},"憶賈閬仙",[74,75,76,77],"風雅道何玄，高吟憶閬仙。","人雖終百歲，君合壽千年。","骨已西埋蜀，魂應北入燕。","不知天地內，誰爲讀遺編。",[],{"title":80,"content":81,"tags":86},"與人游會潤州金山寺",[82,83,84,85],"浮生難會此，此寺絕塵埃。","容易寞言去，等閒爭得來。","江聲寒若吼，山色翠如堆。","自是忘機者，憑闌不忍迴。",[],{"title":88,"content":89,"tags":94},"望湖樓上作",[90,91,92,93],"望湖樓上立，竟日懶思還。","聽水分他浦，看雲過別山。","孤舟依岸靜，獨鳥向人閒。","迴首重門閉，蛙鳴夕照間。",[],{"title":96,"content":97,"tags":102},"寓泊浙江寄漢陽孫邁郎中",[98,99,100,101],"維揚相別後，十載浙江居。","氣概貧猶在，顛狂老漸疏。","已憑天柱客，曾寄漢陽書。","近見人來說，才華患有餘。",[],{"title":104,"content":105,"tags":110},"錢塘秋夕旅舍感懷",[106,107,108,109],"永夜不能寐，閒門懶復開。","片心生萬緒，孤枕轉千迴。","敗葉聲如雨，狂風響似雷。","更堪江上笛，歷歷有餘零。",[],{"title":112,"content":113,"tags":118},"秋日旅舍感懷",[114,115,116,117],"秋色入簷楹，閒門開復扃。","愁人欲長醉，薄酒又還醒。","攪照頭將白，逢誰眼暫青。","祇應南與北，唯此念飄零。",[],{"title":120,"content":121,"tags":126},"歲暮自桐廬歸錢塘晚泊漁浦",[122,123,124,125],"久客見華髮，孤櫂桐廬歸。","新月無朗照，落日有餘暉。","漁浦風水急，龍山煙火微。","時聞沙上雁，一一背人飛。",[],{"title":128,"content":129,"tags":134},"孤山寺易從房留師",[130,131,132,133],"寶塔孤峰頂，題居積翠中。","四邊湖水繞，一逕郭門通。","香滴松梢雨，凉生竹簟風。","閒吟與閒坐，此興與誰同。",[],{"title":136,"content":137,"tags":142},"春日對酒書事",[138,139,140,141],"車馬不暫駐，年光如瀉波。","人間歡樂少，陌上別離多。","往事祇如此，浮生終若何。","花前一尊酒，得失且高歌。",[],{"title":144,"content":145,"tags":150},"與張覃秀才鄴中途次言別",[146,147,148,149],"南北各何之，重來寧有期。","相將行數里，欲別立多時。","野店人煙迥，前村草木衰。","祇應今夜夢，未苦遠分離。",[],{"title":152,"content":153,"tags":158},"敘事答所知",[154,155,156,157],"久客向天末，唯將鸞鶴羣。","盈虧頻歎月，舒卷幾憐雲。","有志思光國，無才可佐君。","知音時見贈，應達聖朝聞。",[],{"title":160,"content":161,"tags":166},"暮春漳川閒居收事",[162,163,164,165],"吟齋漳水濱，孤僻度殘春。","長喜詩無病，不憂家更貧。","鐘聲來遠寺，山色徧諸鄰。","門有誰相訪，僧過豈厭頻。",[],{"title":168,"content":169,"tags":174},"夏日宿西禪院",[170,171,172,173],"此地絕炎蒸，深疑到不能。","夜凉如有雨，院靜若無僧。","枕潤連雲石，窗明照佛燈。","浮生多賤骨，時日恐難勝。",[],{"title":176,"content":177,"tags":182},"舟中自吳之越寄潤州柳侍御開楊博士邁",[178,179,180,181],"兩岸山光合，況當三月天。","好花紅照水，芳草綠隨船。","貨藥村橋醉，收帆野渡眠。","因思二朝客，同泛恐無緣。",[],{"title":184,"content":185,"tags":190},"上李學士",[186,187,188,189],"名場欲獨立，一念一心寒。","白日昇天易，明時取仕難。","家園半牢落，鬚髮漸衰殘。","試把平生業，來投作者看。",[],{"title":192,"content":193,"tags":198},"中秋無月",[194,195,196,197],"西風妒秋月，浮雲重疊生。","一夕不見光，雙目如失明。","立恐疾足折，坐覺愁抱盈。","況當百蟲苦，遶階千萬聲。",[],{"title":200,"content":201,"tags":206},"秋日題琅琊山寺",[202,203,204,205],"巖下多幽景，且無塵事喧。","鐘聲晴徹郭，山色曉當門。","深洞藏泉脈，懸崖露樹根。","更期來此宿，絕頂聽寒猿。",[],{"title":208,"content":209,"tags":214},"落葉",[210,211,212,213],"片片落復落，園林漸向空。","幾番經夜雨，一半是秋風。","靜擁莎階下，閒堆蘚徑中。","谷松與巖檜，寧共此時同。",[],{"title":216,"content":217,"tags":222},"敘吟",[218,219,220,221],"高吟見太平，不恥老無成。","髮任莖莖白，詩須字字清。","搜疑滄海竭，得恐鬼神驚。","此外非關念，人間萬事輕。",[],{"title":224,"content":225,"tags":230},"北高峰塔",[226,227,228,229],"北高峰上塔，竟上最高層。","嘗謂人難到，何當我獨登。","天香聞不斷，海月見微稜。","懶下紅塵路，重來恐未能。",[],{"title":232,"content":233,"tags":238},"寄陳希夷",[234,235,236,237],"不信先生語，剛來帝里遊。","清宵無好夢，白日有閒愁。","世態既如此，壯心應已休。","求歸歸未得，吟上水邊樓。",[],{"title":240,"content":241,"tags":246},"自諸暨抵剡  其一",[242,243,244,245],"莫歎塵泥汩，且圖山水游。","雙峰天姥翠，一舸剡溪秋。","不見戴安道，有懷王子猷。","西風無限意，盡屬釣魚舟。",[],{"title":248,"content":249,"tags":254},"自諸暨抵剡  其二",[250,251,252,253],"夷猶雙槳去，暮不辨東西。","夕照偏依樹，秋光半落溪。","風高一雁小，雲薄四天低。","莽蕩孤帆卸，水村楊柳堤。",[],{"title":256,"content":257,"tags":262},"自諸暨抵剡  其三",[258,259,260,261],"秋渚涵空碧，秋山刷眼青。","排頭煙樹老，撲面水風醒。","上瀨復下瀨，長亭仍短亭。","夜船明月好，客夢滿流螢。",[],{"title":264,"content":265,"tags":270},"自諸暨抵剡  其四",[266,267,268,269],"出得雲門路，風悽日夕曛。","船撐鑑湖月，路指沃州雲。","山色周遭見，谿流屈曲分。","一觴還一詠，誰是右將軍。",[],{"title":272,"content":273,"tags":278},"瓜州臨江亭留題",[274,275,276,277],"誰構危亭已半空，野人時得恣疏慵。","閒觀揚子江心浪，靜聽金山寺裏鐘。","醉卧豈能妨燕雀，狂吟爭不動魚龍。","夜來雨歇蛙聲亂，憶著嵩陽千萬峰。",[],{"title":280,"content":281,"tags":286},"秋日登樓客次懷張覃進士",[282,283,284,285],"聞說飄零亦異鄉，登樓吟望益悲凉。","當時欲別言難盡，他日相逢話更長。","蟬噪水村千萬樹，雁過雲岫兩三行。","明朝策蹇還無定，空倚危欄到夕陽。",[],{"title":288,"content":289,"tags":294},"寄贈柳殿院開授崇儀使赴邊上",[290,291,292,293],"從來長見說兵機，今日君恩志豈違。","驄馬不騎騎鐵馬，繡衣休挂挂戎衣。","雄師已聽心皆伏，醜虜將聞魄盡飛。","應笑苦吟頭白者，二南章句轉衰微。",[],{"title":296,"content":297,"tags":302},"維揚秋日牡丹因寄六合縣尉郭承範",[298,299,300,301],"遶欄忽見思傍徨，造化功深莫可量。","穠艷算無三月盛，殘紅更向九秋芳。","萬家珠翠還爭賞，一郡笙歌又是狂。","惆悵東籬下黄菊，有誰來折泛瑶觴。",[],{"title":304,"content":305,"tags":310},"樽前勉兄長",[306,307,308,309],"一家久寄浙江濱，倏忽如今二十春。","須信百年都似夢，莫嗟萬事不如人。","樽中有酒何妨醉，篋裏無金未是貧。","但看故鄉榮達者，算來多葬北邙塵。",[],{"title":312,"content":313,"tags":318},"赴滁州散參軍途中書事",[314,315,316,317],"微軀不殺謝天恩，容養疏慵世未聞。","昔日已爲閒助教，今朝又作散參軍。","高吟瘦馬衝殘雪，遠看孤鴻入斷雲。","到任也應無別事，願將清俸買香焚。",[],{"title":320,"content":321,"tags":326},"聞高舍人錫下世",[322,323,324,325],"早掌綸言世盡知，豈期謫宦至于斯。","生前是客能投卷，死後何人爲撰碑。","讒佞萬端應已矣，文章千古恐無之。","野夫舊句多蒙賞，昨日閒思淚暗垂。",[],{"title":328,"content":329,"tags":332},"長安道中",[330,331],"游子辭鄉早，高堂親已老。","燕來不東飛，寄書何日到。",[],{"title":334,"content":335,"tags":338},"題思歸鳥",[336,337],"高枝枝上鳥，終日叫思歸。","如斯羽翼全，何不望鄉飛。",[],{"title":340,"content":341,"tags":344},"謝寇員外準見示詩卷",[342,343],"一軸新詩意轉深，幾迴看了又重尋。","最憐積水浮秋漢，閒望滄溟盡日吟。",[],{"title":346,"content":347,"tags":350},"送成庫部肅建安監稅",[348,349],"萬樹青青带雨寒，離亭相別淚闌干。","莫將便作無情物，亦待抽身入建安。",[],{"title":352,"content":353,"tags":356},"中秋月",[354,355],"待月中秋上古城，迥然光掩別宵明。","野人望久翻惆悵，無奈浮雲滅又生。",[],{"title":358,"content":359,"tags":362},"呈錢塘知府薛諫議",[360,361],"再到錢塘眼塹清，騎馿看月又南行。","可憐一片西湖水，不得垂綸老此生。",[363],"西湖",{"title":365,"content":366,"tags":369},"金陵禁林有感",[367,368],"六代繁華難重問，此時興廢動吟情。","空流一带秦淮水，朝暮惟聞鳴咽聲。",[],{"title":371,"content":372,"tags":375},"到錢塘却寄姑蘇太守",[373,374],"姑蘇太守義何長，東泛仙舟已不忘。","更借洞庭湖上月，數宵相送到錢塘。",[376],"洞庭湖",{"title":378,"content":379,"tags":382},"贈林處士逋",[380,381],"雲翦烏妙霧翦衣，存神養氣語還稀。","人人盡喚孫思邈，只恐身輕白日飛。",[],{"title":384,"content":385,"tags":388},"宿靈隠寺",[386,387],"繞寺千千萬萬峰，滿天風雪打杉松。","地爐火暖黄昏睡，更有何人似我慵。",[],{"title":390,"content":391,"tags":394},"贈吳處士棋",[392,393],"昔日長饒國手先，如今多病卧林泉。","有時閒倚青松笑，猶憶曾輸一著玄。",[],{"title":396,"content":397,"tags":400},"留別金山寺",[398,399],"山頂風摇萬樹鳴，扁舟欲解覺難行。","羨師齋後關門睡，不管波濤四面生。",[],{"title":402,"content":403,"tags":406},"書璿公房牡丹",[404,405],"寺中聞說牡丹花，多少人爭耳傍誇。","潦倒參軍來看晚，數枝已謝病僧家。",[],{"title":408,"content":409,"tags":412},"旅舍秋夕書懷",[410,411],"邊鴻過盡背枕卧，弟姪無書憂膽破。","蛩聲更苦不忍聞，半夜起來塞耳坐。",[],{"title":414,"content":415,"tags":418},"寄石隠之",[416,417],"獨上危樓返照間，亂鴉羣鳥噪迴還。","故人歲暮在何處，空見千山與萬山。",[],{"title":420,"content":421,"tags":424},"送馮尊師德之出長安",[422,423],"浩浩霜風萬木寒，高吟大醉出長安。","杖頭一卷金丹訣，塵世何人借得看。",[],{"title":426,"content":427,"tags":430},"與道士馮德之話別",[428,429],"一宿山房話別離，慇懃勸我酒盈卮。","更教弟子橫琴送，彈徹悲風盡淚垂。",[],{"title":432,"content":433,"tags":436},"聞陳贄郎中下世",[434,435],"有家數口寄伊川，孤宦東西二十年。","見說葬時無一物，空留滿篋舊文編。",[],{"title":438,"content":439,"tags":442},"喜臘雪",[440,441],"久旱臘月如夏熱，夜來忽降一尺雪。","叫謝上天聲應徹，且壓瘴氣不作孽。",[],{"title":444,"content":445,"tags":448},"宮詞",[446,447],"學畫蛾眉獨出羣，當時人道便承恩。","年年不見君王面，花落黄昏空掩門。",[],{"title":450,"content":451,"tags":454},"過華山",[452,453],"高愛三峰插太虛，掉頭吟望倒騎馿。","旁人大笑從他笑，終擬移家向此居。",[],{"title":456,"content":457,"tags":460},"題資福院石井",[458,459],"炎炎畏日樹將焚，却恨都無一點雲。","强跨蹇馿來得到，皆疑渴殺老參軍。",[],{"title":462,"content":463,"tags":466},"曹娥廟",[464,465],"曹娥廟前秋草平，曹娥廟裏秋月明。","扁舟一夜炯無寐，近聽潮聲似哭聲。",[],{"title":468,"content":469,"tags":472},"九華山",[470,471],"將齊華嶽猶多六，若並巫山又欠三。","好是雨餘江上望，白雲堆裏潑濃藍。",[473,474],"华山","九华山",{"title":476,"content":477,"tags":480},"離滁陽",[478,479],"長亭黄葉正紛紛，一曲驪歌酒十分。","須信滁陽人有意，滿城來送散參軍。",[],{"title":482,"content":483,"tags":486},"書詩卷末",[484,485],"一卷詩成二十年，畫曾忘食夜忘眠。","莫言不及相如賦，誰敢高吟漢帝前。",[],{"title":488,"content":489,"tags":494},"登應天寺塔",[490,491,492,493],"閑上應天寺裏塔，九層突兀入幽雲。","下窺已覺紅塵別，低語猶疑碧落聞。","秦望山高如髻簇，照湖水闊若甌分。","野人不管興亡事，空憑危欄到日曛。",[],{"title":496,"content":497,"tags":500},"泊禹祠",[498,499],"禹廟高高萬木齊，蟾蜍影裏月光低。","山中不慣聞寒漏，一夜猿驚與鳥啼。",[],{"title":502,"content":503,"tags":508},"癸未歲秋七月禱禹廟",[504,505,506,507],"萬古稽山下，森森大禹祠。","幽人來暗禱，靈魄望潛知。","帝慮河頻决，民憂業旋移。","自慙無異策，載拜淚雙垂。",[],{"title":510,"content":511,"tags":516},"寄會稽仲休山人",[512,513,514,515],"近携琴鶴遊東越，曾接談諧氣味深。","佛意已知師達了，儒書却爲俗披尋。","稽山有雪寒凝骨，鑑水無風冷徹心。","別後相思向誰說，祗應霜鬢暗相侵。",[],{"title":518,"content":519,"tags":522},"題蕭山祇園寺門",[520,521],"門掩多年生綠苔，想師心地似寒灰。","勞心擾擾休來此，我是閒人尚不開。",[],{"title":524,"content":525,"tags":530},"送崇教大師惠思歸山",[526,527,528,529],"受知聖睠與誰同，師號封題寄梵宮。","爲謝國恩來闕下，又承天澤去山中。","禪餘靜對江亭月，吟苦凉生海樹風。","若到赤城逢羽客，但言孤鶴在金籠。",[],{"title":532,"content":533,"tags":537},"游當塗",[534,535,536],"扁舟不復見袁宏，故壘猶連謝尚城。","騎鯨仙子千年恨，化石佳人萬古情。","牛渚磯前浪如屋，區區名利與生輕。",[],{"title":539,"content":540,"tags":543},"夏",[541,542],"野花成子落，江燕引雛飛。","暗草薰苔徑，晴楊拂石磯。",[],{"title":545,"content":546,"tags":551},"落花",[547,548,549,550],"花殘雨又風，飄紫復飄紅。","不到三春盡，都經數夜空。","繞闌仍聚散，逐水尚西東。","可惜多情蝶，徘徊晚照中。",[],{"title":553,"content":554,"tags":559},"遊龍虎山贈天師  其一",[555,556,557,558],"遠挹高風自漢廷，老松猶带昔年青。","留侯高卧衣冠冷，鼻祖重來印劍靈。","鶴和猿吟清徹底，龍蟠虎踞翠爲屏。","真人一笑梅花發，三十六天春不扃。",[],{"title":561,"content":562,"tags":567},"遊龍虎山贈天師  其二",[563,564,565,566],"鐵鞋踏破幾山行，惟此山行越樣清。","顧我塵中一簪紱，任他世上幾公卿。","分安淡泊全真性，身厭輕肥已半生。","漢鼎餘丹今在否，願須匕粒守三彭。",[],{"title":569,"content":570,"tags":572},"句  其一",[571],"何須三叫絕，已恨一聲多。",[],{"title":574,"content":575,"tags":577},"句  其二",[576],"莫嗟黑髮從頭白，終見黄河到底清。",[],{"title":579,"content":580,"tags":582},"句  其三",[581],"到底不能成一曲。",[],{"title":584,"content":585,"tags":587},"句  其四",[586],"土床安睡穩，紙被轉身鳴。",[],{"title":589,"content":590,"tags":592},"句  其五",[591],"滁上相逢俱禁煙。",[],{"title":594,"content":595,"tags":597},"句  其六",[596],"窺魚白鳥明殘照，抱石幽雲點半山。",[],{"title":599,"content":600,"tags":602},"句  其七",[601],"歸來遍檢登科記，未見連年放弟兄。",[],{"title":604,"content":605,"tags":607},"句  其八",[606],"峽口聞猿夜。",[],{"title":609,"content":610,"tags":612},"句  其九",[611],"出秋霜，價錢可，贏得撥灰兼弄火，暢殺我。",[],{"title":614,"content":615,"tags":617},"句  其一○",[616],"散拽禪師來踘蹴，亂拖游女上秋千。",[],{"title":619,"content":620,"tags":622},"目存  其一",[621],"詩題：古意首句：妾有羅衣裳。",[],{"title":624,"content":625,"tags":627},"目存  其二",[626],"詩題：渭上秋夕閒望首句：秋色滿秦川。",[],"content:pan-lang.json","json","content","pan-lang.json",[633,636,639,642,645,648,651,654,657,660,663,666,669,672,675,678,681,684,687,690,693,696,699,702,705,708,711,714,717,720,723,726,729,732,735,738,741,744,747,750,753,756,759,762,765,768,771,774,777,780,783,786,789,792,795,798,801,804,807,810,813,816,819,822,825,828,831,834,837,840,843,846,849,852,855,858,861,864,867,870,873,876,879,882,885,888,891,894,897,900,903,906,909,912,915,918,921,924,927,930,933,936,939,942,945,948,951,954,957,960,963,966,969,972,975,978,981,984,987,990,993,996,999,1002,1005,1008,1011,1014,1017,1020,1023,1026,1029,1032,1035,1038,1041,1044,1047,1050,1053,1056,1059,1062,1065,1068,1071,1074,1077,1080,1083,1086,1089,1092,1095,1098,1101,1104,1107,1110,1113,1116,1119,1122,1123,1126,1129,1132,1135,1138,1141,1144,1147,1150,1153,1156,1159,1162,1165,1168,1171,1174,1177,1180,1183,1186,1189,1192,1195,1198,1201,1204,1207,1210,1213,1216,1219,1222,1225,1228,1231,1234,1237,1240,1243,1246,1249,1252,1255,1258,1261,1264,1267,1270,1273,1276,1279,1282,1285,1288,1291,1294,1297,1300,1303,1306,1309,1312,1315,1318,1321,1324,1327,1330,1333,1336,1339,1342,1345,1348,1351,1354,1357,1360,1363,1366,1369,1372,1375,1378,1381,1384,1387,1390,1393,1396,1399,1402,1405,1408,1411,1414,1417,1420,1423,142