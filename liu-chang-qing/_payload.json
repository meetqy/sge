[{"data":1,"prerenderedAt":1219},["Reactive",2],{"content-query-cOFwvuVSOe":3,"content-navigation-8C37fagqQL":122},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":118,"_type":119,"_source":120,"_file":121,"_extension":119},"/liu-chang-qing","",false,"劉長卿",[9,19,26,36,47,58,67,76,85,95,106],{"title":10,"content":11,"tags":14},"聽彈琴",[12,13],"泠泠七絲上，靜聽松風寒。","古調雖自愛，今人多不彈。",[15,16,17,18],"唐诗三百首","抒情","音乐","五言绝句",{"title":20,"content":21,"tags":24},"送方外上人",[22,23],"孤雲將野鶴，豈向人間住。","莫買沃洲山，時人已知處。",[15,25,18],"送别",{"title":27,"content":28,"tags":31},"送靈澈上人",[29,30],"蒼蒼竹林寺，杳杳鐘聲晚。","荷笠帶夕陽，青山獨歸遠。",[15,32,16,25,33,34,18,35],"山水","初中古诗","即景抒情","七年级下册(课外)",{"title":37,"content":38,"tags":43},"新年作",[39,40,41,42],"鄉心新歲切，天畔獨潸然。","老至居人下，春歸在客先。","嶺猨同旦暮，江柳共風煙。","已是長沙傅，從今又幾年。",[15,44,45,46],"五言律诗","春节","思乡",{"title":48,"content":49,"tags":54},"秋日登吳公臺上寺遠眺寺即陳將吳明徹戰場",[50,51,52,53],"古臺搖落後，秋日望鄉心。","野寺人來少，雲峰水隔深。","夕陽依舊壘，寒磬滿空林。","惆悵南朝事，長江獨至今。",[15,55,56,57,44],"长江","怀古","秋天",{"title":59,"content":60,"tags":65},"尋南溪常山道人隱居",[61,62,63,64],"一路經行處，莓苔見履痕。","白雲依靜渚，春草閉閑門。","過雨看松色，隨山到水源。","溪花與禪意，相對亦忘言。",[15,66,44],"写景",{"title":68,"content":69,"tags":74},"餞別王十一南遊",[70,71,72,73],"望君煙水闊，揮手淚霑巾。","飛鳥沒何處，青山空向人。","長江一帆遠，落日五湖春。","誰見汀洲上，相思愁白蘋。",[15,55,75,25,44],"友情",{"title":77,"content":78,"tags":83},"江州重別薛六柳八二員外",[79,80,81,82],"生涯豈料承優詔，世事空知學醉歌。","江上月明胡鴈過，淮南木落楚山多。","寄身且喜滄洲近，顧影無如白髮何。","今日龍鍾人共棄，媿君猶遣慎風波。",[15,25,84],"七言律诗",{"title":86,"content":87,"tags":92},"長沙過賈誼宅",[88,89,90,91],"三年謫宦此棲遲，萬古惟留楚客悲。","秋草獨尋人去後，寒林空見日斜時。","漢文有道恩猶薄，湘水無情弔豈知。","寂寂江山搖落處，憐君何事到天涯。",[15,93,94,56,84],"迁谪","生活",{"title":96,"content":97,"tags":102},"自夏口至鸚鵡洲夕望岳陽寄源中丞",[98,99,100,101],"[汀]洲無浪復無煙，楚客相思益渺然。","漢口夕陽斜渡鳥，洞庭秋水遠連天。","孤城背嶺寒吹角，獨戍臨江夜泊船。","賈誼上書憂漢室，長沙謫去古今憐。",[15,103,104,84,105],"洞庭湖","贬谪","触景感怀",{"title":107,"content":108,"tags":113},"賦得",[109,110,111,112],"鶯啼燕語報新年，馬邑龍堆路幾千。","家住層城臨漢苑，[心]隨明月到胡天。","機中錦字論長恨，樓上花枝笑獨眠。","爲問元戎竇車騎，何時返斾勒燕然。",[114,84,115,116,15,117],"思念","春天","妇女","闺怨","content:liu-chang-qing.json","jso