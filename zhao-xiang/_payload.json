[{"data":1,"prerenderedAt":2552},["Reactive",2],{"content-query-uFGPmUVTS4":3,"content-navigation-8C37fagqQL":1281},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":1277,"_type":1278,"_source":1279,"_file":1280,"_extension":1278},"/zhao-xiang","",false,"趙湘",[9,20,30,40,48,56,64,72,80,140,181,189,197,205,213,221,229,237,245,253,261,269,277,285,293,301,309,317,325,333,341,349,357,365,373,381,389,397,405,413,421,429,437,445,453,461,469,477,485,493,501,509,517,525,533,541,549,557,565,573,581,589,597,605,613,621,629,637,645,653,661,669,677,685,693,701,709,717,725,733,741,749,757,765,773,781,789,797,805,813,821,829,837,845,853,861,869,877,885,893,901,909,917,925,933,941,949,957,965,973,981,997,1013,1024,1030,1036,1042,1048,1054,1060,1066,1072,1078,1084,1090,1096,1102,1108,1114,1119,1124,1129,1134,1139,1145,1151,1157,1162,1167,1172,1177,1185,1193,1201,1209,1217,1223,1231,1239,1247,1252,1257,1262,1267,1272],{"title":10,"content":11,"tags":19},"聖號雅二篇  其一",[12,13,14,15,16,17,18],"天監王德，莫如宋赫。","宋道平平，莫如法天。","皇法昊天，覆罔攸私。","達乎鬼方，化被孔時。","維戩維祿，維生維育。","被彼蟲魚，及爾草木。","惟命克遂，于其有俶。",[],{"title":21,"content":22,"tags":29},"聖號雅二篇  其二",[23,24,25,26,27,28],"皇振厥猷，法天以寧。","我法我度，我經我營。","或風或雨，或雷或霆。","日月昭然，奮皇之明。","霜肅以平，露澤以清。","左右百辟，爛兮敷星。",[],{"title":31,"content":32,"tags":39},"聖號雅二篇  其三",[33,34,35,36,37,38],"皇實法之，配道無作。","始則以寬，終靡庸度。","嗚呼下民，亦孔之莫。","皇鑑下土，將安將樂。","皇視四方，匪醜匪惡。","法爾天永，民禱斯恪。",[],{"title":41,"content":42,"tags":47},"聖號雅二篇  其四",[43,44,45,46],"赫赫君道，皇其崇之。","遹彼丕教，皇實基之。","維基伊何，實以無爲。","海隅蒼生，被服熙熙。",[],{"title":49,"content":50,"tags":55},"聖號雅二篇  其五",[51,52,53,54],"天下既平，狄人既庭。","其棘匪勞，神聖以寧。","無亂斯聰，無惑斯明。","衣裳自垂，冕旒是凝。",[],{"title":57,"content":58,"tags":63},"聖號雅二篇  其六",[59,60,61,62],"烝哉皇極，道靡有息。","勤勤以謨，罔視其迹。","鑿飲自鼓，耕食自績。","維相以歌，勿荷後辟。",[],{"title":65,"content":66,"tags":71},"聖號雅二篇  其七",[67,68,69,70],"皇宅斯道，汗汗浩浩。","賢俊茀茀，鐘鼓考考。","爰煦爾赤，爰妥其老。","樂只萬國，寧德攸寶。",[],{"title":73,"content":74,"tags":79},"聖號雅二篇  其八",[75,76,77,78],"皇道靡徹，載昭載晰。","克彼有截，萬國是悅。","皇矣聖朝，綿爾瓜瓞。","是崇是潔，萬世貽厥。",[],{"title":81,"content":82,"tags":139},"宋頌",[83,84,85,86,87,88,89,90,91,92,93,94,95,96,97,98,99,100,101,102,103,104,105,106,107,108,109,110,111,112,113,114,115,116,117,118,119,120,121,122,123,124,125,126,127,128,129,130,131,132,133,134,135,136,137,138],"九區混茫，皇天錫皇。","羲教農化，勛紀華彰。","繼華於禹，承禹於湯。","比於文武，逮其成康。","聖道攸傳，富壽無疆。","隋之棄仁，天命不常。","殱厥侈毒，祚歸巨唐。","唐孫復昏，蕩於四方。","民瘼其咨，逾於懷襄。","草創中間，崩析滋章。","割山裂川，國爲披猖。","氓不堪命，神恫其殃。","俾宋有圖，橫弛豎張。","日月五辰，齊其啟光。","實維太祖，肇基建邦。","有若有苗，逆命弗祥。","爰整其師，赫怒斯揚。","荆舒既貢，南海趨蹌。","庸蜀微盧，雖險無當。","惟爾曰夏，毒靡有流，民始克昌。","屢饑而饌，久渴而漿。","冰極授衣，暑煩降凉。","洎我聖君，傳葉振芳。","聖神仁明，浩浩汪汪。","吳人來王，并汾懾降。","服不以戎，鼓罔其鏜。","風雨攸暨，如禽集翔。","淳復天人，禮修夏商。","前正後直，左賢右良。","忠正並驅，回邪跼藏。","鰥恤寡矜，寇屏宄亡。","藥蒸石黎，法衡度量。","乃宅孝悌，乃謹序庠。","衣帛食肉，罔有攸傷。","關譏匪艱，什一靡龐。","雖有罟網，不罹汙潢。","雖有山林，不亂斧斨。","雖有權衡，民不以强。","雖有郛郭，民不以防。","宗廟既清，郊社甚莊。","品物爭瑞，史載交相。","未誥俗化，將時合蒼。","盈耳四海，但聞洋洋。","其雍其熙，無施無爲。","乾坤法之，治於垂衣。","蒙彼蚩蚩，不覺不知。","天下一致，夫何慮思。","清之浄之，平之泰之。","倏變於道，殊途同歸。","偉焉厥圖，本蕃益枝。","百世之後，實流其奇。","史官既良，康哉具書。","古有吉甫，爲宣王詩。","是故臣湘，作夫頌辭。","聖德形容，神明告茲。","狂斐恐慴，少頌史遺。",[],{"title":141,"content":142,"tags":180},"送駕部劉侯赴闕詩",[143,144,145,146,147,148,149,150,151,152,153,154,155,156,157,158,159,160,161,162,163,164,165,166,167,168,169,170,171,172,173,174,175,176,177,178,179],"帝有天命，四方是宣。","祥有時數，元極則傳。","愛物在皇，惡盈在天。","惟彼廬墟，安寧有年。","土沃無蠹，民驕且專。","窮侈勿度，宄猾相肩。","彼孽既盈，厥兆斯綿。","天子眷之，丞相竦然。","僉曰疇當，劉侯其賢。","帝命欽哉，布教甚玄。","儉以示之，信以布之。","侈焉猾焉，禁之制之。","惟公拳拳，是民咨咨。","惟民未知，其惡去遲。","公恐公懼，吾知天時。","將及弗度，明年果饑。","雨師不顧，其歲莫支。","回祿仍怒，公豫其知。","雖凶雖燄，弗禱弗祈。","厥民既羸，爰求其私。","始惻天變，相議而辭。","公始儉我，我莫肯隨。","公始廉我，謂公啟威。","公實愛我，而莫之知。","洎稟公教，里閭相師。","易侈以儉，猾庸悔追。","逮夫次歲，百物咸宜。","雨奮於畢，火藏於離。","民歌且謠，四郊怡怡。","教民肅祗，功德流滋。","史臣弗遺，後世是資。","孝績未彰，徵於帝畿。","腹心腎腸，帝將布其。","民之懷仁，亦孔之微。","公不我留，齎咨涕洟。","來既云暮，去惟是思。","湘採民謠，播於聲詩。",[],{"title":182,"content":183,"tags":188},"華頂峰",[184,185,186,187],"連天含積翠，石路接危藤。","日近棲鵰樹，雲迎望海僧。","狖傳幽谷響，泉落斷崖冰。","中有仙人宅，知居第幾層。",[],{"title":190,"content":191,"tags":196},"黄鶑",[192,193,194,195],"黄金類羽翰，飛出雪晴山。","語曉他禽默，啼花別樹閒。","微陽疑獨照，急管偶相關。","紅杏園林闊，經時得往還。",[],{"title":198,"content":199,"tags":204},"登程主簿南亭",[200,201,202,203],"南溪亭蔽月，主簿好看雲。","暇日曾移樹，炎風盡带薰。","四窗山曲入，一徑草微分。","買石清流底，名賢待贈君。",[],{"title":206,"content":207,"tags":212},"蘭江仁智亭",[208,209,210,211],"蘭江江上亭，仁智稱爲名。","山色曉爭出，水光秋更清。","微風飄復斷，好鳥過還鳴。","曾伴陶公醉，疊聞漁唱聲。",[],{"title":214,"content":215,"tags":220},"題石橋寺山井",[216,217,218,219],"何年鑿蘚紋，寒氣觸僧魂。","穴在青松底，泉通白石根。","汲因嘗茗客，窺有斷腸猿。","偶向秋來照，吟髭雪露痕。",[],{"title":222,"content":223,"tags":228},"書松門寺壁",[224,225,226,227],"物外纖塵絕，堪聞樂道歌。","好山光不盡，幽鳥語無多。","樹影秋雲雜，泉聲晝磬和。","高僧自禪定，時有野人過。",[],{"title":230,"content":231,"tags":236},"登揚州孝先寺佛閣",[232,233,234,235],"勝寺登危閣，凭欄即近鵰。","無雲生北固，看日下南朝。","木落吳山迥，天晴楚水遥。","從容萬慮息，目喜赴僧招。",[],{"title":238,"content":239,"tags":244},"金山寺",[240,241,242,243],"衆木皆穿石，名香自合焚。","兩廊吳分月，一閣楚天雲。","雨過新燈露，潮平晚磬分。","僧吟貝多偈，夜靜釣人聞。",[],{"title":246,"content":247,"tags":252},"九日松林寺登高",[248,249,250,251],"九日松林寺，登高過石橋。","野僧憐不醉，山菊笑無憀。","屐齒憑泉漱，雲衣近樹飄。","自知清靜意，應免俗人招。",[],{"title":254,"content":255,"tags":260},"登天竺靈隠寺上方",[256,257,258,259],"寺高傳絕頂，一到即忘機。","夜月逢僧早，秋雲出樹微。","磬邊巖狖過，窗外海鵰飛。","欲下莓苔磴，題名却惜歸。",[],{"title":262,"content":263,"tags":268},"題國清寺",[264,265,266,267],"物外千年寺，人間四絕名。","兩廊諸嶽色，九里亂松聲。","海氣飄僧院，秋鐘徹懸城。","夜來疏磬斷，月影徧樓清。",[],{"title":270,"content":271,"tags":276},"蘭汀晚泊",[272,273,274,275],"孤舟愁晚泊，葭葦暫相鄰。","旅雁初棲雨，江花偶向人。","强吟還自愛，忽語有誰親。","倚棹煙將暝，秋風起白蘋。",[],{"title":278,"content":279,"tags":284},"秦淮晚泊",[280,281,282,283],"晚泊秦淮上，無人獨倚檣。","遥峰平積水，別浦露殘陽。","往事隨天望，新蟬聒樹黄。","漁翁有深意，不語自鳴榔。",[],{"title":286,"content":287,"tags":292},"書韓隠人壁",[288,289,290,291],"雨黑徑垂莎，全家在薜蘿。","樹空穿井出，門掩燒痕過。","寫易澌生硯，懸壺月上柯。","近來山屐老，枯齒旋無多。",[],{"title":294,"content":295,"tags":300},"戊子歲東歸書事呈龐先輩",[296,297,298,299],"上國有春榜，遠人無試期。","携琴出關早，聞雨到家遲。","卧病鄰皆笑，嫌貧僕轉癡。","高堂倚門久，此去尚多岐。",[],{"title":302,"content":303,"tags":308},"題張處士山庭落葉",[304,305,306,307],"摇落拂殘棋，蕭騷傍菊籬。","硯浮窗外影，人拾樹中詩。","掃綠粘苔冷，燒紅煮茗遲。","巢烏還有意，猶在月明枝。",[],{"title":310,"content":311,"tags":316},"招阮山人宿",[312,313,314,315],"初晴石路新，溪戶別無鄰。","自夏評詩徧，經秋掃葉頻。","拂琴堂有月，餐藥病離身。","欲話因來宿，西軒只兩人。",[],{"title":318,"content":319,"tags":324},"自題",[320,321,322,323],"獨居顔子巷，還自種忘憂。","偶病閒辜月，因吟瘦過秋。","泣多曾憶阮，貧極尚師丘。","散拙兼成癖，如何稱俗流。",[],{"title":326,"content":327,"tags":332},"江秀才新居",[328,329,330,331],"營居知避俗，領鶴過秋郊。","泉照書初架，窗移樹未巢。","留題新墨濕，掩戶舊鄰敲。","數本當簾竹，茶煙漸欲交。",[],{"title":334,"content":335,"tags":340},"剡中唐郎中所居",[336,337,338,339],"古柳垂溪水，當門繫雪舟。","開樽延白鳥，掃樹带清秋。","閣上看華頂，窗中見沃洲。","尋常投刺少，來即是詩流。",[],{"title":342,"content":343,"tags":348},"秋夜集李式西齋",[344,345,346,347],"澤國秋光淡，詩家夜會清。","雨餘逢月色，風靜得琴聲。","小徑幽蟲絕，寒階落葉并。","嘗茶不擬睡，自起遶池行。",[],{"title":350,"content":351,"tags":356},"別耶溪諸叔",[352,353,354,355],"又散耶溪會，斜陽在古原。","出門逢落葉，去路有啼猿。","短髮不堪照，新詩誰與論。","如何爲遠別，風雪宿孤村。",[],{"title":358,"content":359,"tags":364},"遊爛柯山",[360,361,362,363],"仙人與王質，相會偶多時。","落日千年事，空山一局棋。","樹高明月在，風動白雲移。","未得酬身計，閒來學採芝。",[],{"title":366,"content":367,"tags":372},"天台香柏峰會思上人",[368,369,370,371],"孤峰連鳥道，幽會興難忘。","對坐無人到，空林盡日香。","斷崖逢積雪，高木露微陽。","靜話看晴曉，聯聯過晚岡。",[],{"title":374,"content":375,"tags":380},"送杜少府赴華陰",[376,377,378,379],"行路華山前，之官髮黑年。","移家擬拔宅，作尉已呼仙。","近縣雲隨鶴，當門樹著蟬。","吟詩因夜坐，毛女落秋天。",[],{"title":382,"content":383,"tags":388},"送張用礪殿丞赴闕",[384,385,386,387],"詔書徵入闕，數日醉離筵。","野老衣多淚，山溪雨送船。","郡齋閒月色，江鳥得詩篇。","省閣人相待，供吟幾樹煙。",[],{"title":390,"content":391,"tags":396},"寄梁鼎殿丞",[392,393,394,395],"登門長失約，千里阻關河。","每爲秋風至，空令夜夢多。","舊時曾見許，新諾復如何。","想望臨荒徑，微微雨滴蓑。",[],{"title":398,"content":399,"tags":404},"送韓嶼秀才南遊",[400,401,402,403],"楊柳一蟬清，關河九日行。","却來時未熟，此去雨初晴。","僕嬾嫌書重，馿幽喜骨輕。","還因得新句，山寺徧題名。",[],{"title":406,"content":407,"tags":412},"送李先輩罷官之四明",[408,409,410,411],"官滿鶴衣輕，全家在四明。","上船秋月白，到岸海山青。","藥妙初痊病，詩高舊振名。","遥思落帆處，太守出郊迎。",[],{"title":414,"content":415,"tags":420},"送李著作赴四明",[416,417,418,419],"暫辭芸閣伴，欲上海邊城。","地近瀛洲見，山連越水迎。","駐帆寒草動，移旆曉霞明。","到日新題出，登高待雪晴。",[],{"title":422,"content":423,"tags":428},"送沈天錫赴始興司戶",[424,425,426,427],"落葉上青衫，携家赴嶺南。","欲行猶買藥，侵早即衝嵐。","久賤惟知事，長貧不解慚。","山城騎病馬，吟句去庭參。",[],{"title":430,"content":431,"tags":436},"送周洞之蜀",[432,433,434,435],"劍閣衝秋去，迢迢動鬰陶。","千山非故國，九日莫登高。","峽樹參天碧，巴猿向夕號。","當壚人不在，何以慰離騷。",[],{"title":438,"content":439,"tags":444},"送丁鶚下第客遊",[440,441,442,443],"尊前泣復歌，清淚酒相和。","別岸春風靜，孤舟夜雨多。","鳥鳴煙草露，花逐夕陽波。","我亦無憀者，東西欲奈何。",[],{"title":446,"content":447,"tags":452},"送周湜秀才之武川謁黄員外",[448,449,450,451],"特地照青銅，行行謁謝公。","山窗新入夢，池館舊聞鴻。","岐路雲邊去，炎蒸雨後空。","郡齋如伴話，滿座是清風。",[],{"title":454,"content":455,"tags":460},"送羅權",[456,457,458,459],"相逢從冷澹，此別復凄凉。","去路迎秋色，行衣拂夕陽。","酒濃心易感，詩好意難忘。","莫作關山役，吟髭況漸長。",[],{"title":462,"content":463,"tags":468},"送韓起",[464,465,466,467],"門外雨初晴，春深送客行。","離筵近流水，芳草向空城。","遠鳥啼仍亂，殘花墜欲并。","會須頻飲酒，莫遣白髭生。",[],{"title":470,"content":471,"tags":476},"送到逢吉",[472,473,474,475],"獨去誰相待，全家寄舊林。","雨聲鄉外盡，秋色水邊深。","別酒香和菊，新詩束傍琴。","將行還共立，江上起孤砧。",[],{"title":478,"content":479,"tags":484},"輦下送沈天錫",[480,481,482,483],"未第逢花盡，殘陽照渡頭。","入關頻值雨，歸棹獨隨鷗。","樹暗藏鳴鳥，山空接漫流。","琴書不同去，猶在酒家樓。",[],{"title":486,"content":487,"tags":492},"送劉熙",[488,489,490,491],"落日照行衣，鄉程隔翠微。","聽蟬臨水別，聞雨抱琴歸。","鬢髮各疑短，天涯相見稀。","離歌一聲斷，江上有鴻飛。",[],{"title":494,"content":495,"tags":500},"答徐本",[496,497,498,499],"天遠草離離，秋霖寄信遲。","相思逢葉盡，獨坐聽蟬悲。","岳色寒前見，松心雪後知。","頻招猶未至，時復檢清辭。",[],{"title":502,"content":503,"tags":508},"別所知",[504,505,506,507],"知音不可得，此去但消魂。","別夜俱聞雨，殘秋自出門。","水橫芳草渡，煙蔽夕陽村。","應念千山外，明朝獨聽猿。",[],{"title":510,"content":511,"tags":516},"別楊塤",[512,513,514,515],"此身偏似雁，南北但孤飛。","久客難爲別，窮秋況未歸。","去程寒雨細，留話夕陽微。","試共江邊立，疏砧又搗衣。",[],{"title":518,"content":519,"tags":524},"及第東歸泗上遇孫穆之話別",[520,521,522,523],"偶負同年約，知君先出京。","相逢得酒病，欲別值蟬聲。","共立山當眼，分題雨過城。","東歸幾人問，爲子耻詩名。",[],{"title":526,"content":527,"tags":532},"贈何明府",[528,529,530,531],"陶家宜寂寞，多醉復多才。","鎖印秋山入，移琴夜雨來。","試茶還掃葉，買樹亦和苔。","妙句鄰僧乞，仍聞得藥回。",[],{"title":534,"content":535,"tags":540},"寄東陽王明府",[536,537,538,539],"經炎雖得信，旦夕夢兼思。","多暇知無事，新秋合有詩。","樹凉看雨過，吏散見蟬移。","誰共清宵坐，西軒月滿池。",[],{"title":542,"content":543,"tags":548},"聞蟬因寄東陽王穆明府",[544,545,546,547],"孤蟬鳴水湄，獨立少人知。","急恐移他樹，清還似舊時。","曾同秋館聽，併起雨堂思。","南望臨風久，經年不得詩。",[],{"title":550,"content":551,"tags":556},"別王穆明府",[552,553,554,555],"又作殘秋役，陶公爲斷魂。","此行知入夢，欲去更登門。","落葉堆霜徑，微陽隔雨村。","預愁山館宿，相憶獨聞猿。",[],{"title":558,"content":559,"tags":564},"寄楊坦明府",[560,561,562,563],"不語仍孤立，何人知此情。","春禽鳴別樹，夜雨入空城。","望遠魂堪斷，思閒夢亦清。","曾陪遊嶽寺，遶竹共題名。",[],{"title":566,"content":567,"tags":572},"贈潛溪李明府",[568,569,570,571],"縣幽多水石，栽竹亦成畦。","聽雨公田近，開窗月嶽齊。","宿禽無別語，病馬立閒蹄。","夜坐聞清唱，何人釣月溪。",[],{"title":574,"content":575,"tags":580},"寄楊塤",[576,577,578,579],"閉門苔自長，春恨極天涯。","落日山橫水，空城雨過花。","斷狂曾避蝶，多病更聞蛙。","江上無消息，風吹渡柳斜。",[],{"title":582,"content":583,"tags":588},"旅中示所知",[584,585,586,587],"無憀作客時，況逼入關期。","聽雨醒如醉，聞蟬坐欲移。","病催新髮長，貧有故人知。","應念將行計，出門多路岐。",[],{"title":590,"content":591,"tags":596},"秋夕宿楊明府廳",[592,593,594,595],"主人無俗緣，留客菊花邊。","雨氣寒并竹，秋聲夜雜蟬。","爽知於酒稱，閒覺愛棋偏。","不欲欹孤枕，更深却潄泉。",[],{"title":598,"content":599,"tags":604},"暮春郊園雨霽",[600,601,602,603],"郊園春已暮，霽色射疏籬。","地靜苔過竹，沙清樹入池。","野人來盡日，山鳥話多時。","偶傍松窗立，微風動七絲。",[],{"title":606,"content":607,"tags":612},"詠山屐",[608,609,610,611],"鳥道人分去，同行只有筇。","過橋泉照齒，穿竹蘚留蹤。","夜閣吟登月，秋山醉掛松。","自窺還憶謝，會著在諸峰。",[],{"title":614,"content":615,"tags":620},"秋日過吳侃幽居",[616,617,618,619],"掩門多徑草，凉氣動疏林。","細雨經煙盡，微陽到菊深。","話詩無鳥過，試茗有僧尋。","欲去還留宿，窗邊月照琴。",[],{"title":622,"content":623,"tags":628},"登高",[624,625,626,627],"天晴九月九，秋思攪騰騰。","大醉無人會，高山獨自登。","看鴻時背水，採菊阻尋僧。","往事還堪憶，狂吟罷未能。",[],{"title":630,"content":631,"tags":636},"自樂",[632,633,634,635],"自樂原西趣，疏林入戶清。","洗池秋得月，移菊夜棲螢。","古意憐琴淡，新題喜病醒。","時聞岳僧至，閒講煮茶經。",[],{"title":638,"content":639,"tags":644},"官舍偶書",[640,641,642,643],"病枕方行藥，公門似隠居。","孤煙寄庭木，微雨長秋蔬。","宦薄慚騎馬，山高夢結廬。","期僧仍未至，慵寫與人書。",[],{"title":646,"content":647,"tags":652},"野步",[648,649,650,651],"原野宜秋步，支筇日欲斜。","斷橋時立鳥，疏草露行蛇。","細雨霑梨葉，微風過稻花。","坡西又吹笛，煙火兩三家。",[],{"title":654,"content":655,"tags":660},"贈省安上人",[656,657,658,659],"閉門棲古寺，塵路杳無蹤。","夜講香飄月，晨齋磬入松。","石屏泉氣冷，山屐蘚痕濃。","自說秋來興，時時夢遠峰。",[],{"title":662,"content":663,"tags":668},"僧院苔",[664,665,666,667],"香銷人寂寂，鋪碧斷塵埃。","積雨聲初盡，高僧定未開。","冷痕穿竹去，秋陣隔泉回。","此處應長在，閒人況不來。",[],{"title":670,"content":671,"tags":676},"會平上人夜話",[672,673,674,675],"秋燈照岳客，深夜坐前軒。","白髮新聞雨，青山舊聽猿。","病多添藥債，吟苦避蟲喧。","不寐還相對，高梧葉自翻。",[],{"title":678,"content":679,"tags":684},"照上人山房庭樹",[680,681,682,683],"惹雲非手植，自與薜蘿交。","雨過苔侵影，秋來月照巢。","錫寒枝上掛，偈好葉中抄。","誰見僧行遶，煙凉夜磬敲。",[],{"title":686,"content":687,"tags":692},"贈水墨巒上人",[688,689,690,691],"講餘精小筆，深院竹脩脩。","試墨因磨雨，思山忽寫秋。","靜曾窮鶴趣，高亦近詩流。","更擬緣清思，和雲狀沃洲。",[],{"title":694,"content":695,"tags":700},"送僧歸終南",[696,697,698,699],"夏滿辭京寺，終南路有松。","月高緣白閣，葉盡到中峰。","漱齒泉飄錫，冥心露滴鐘。","長安如不去，期與鹿相逢。",[],{"title":702,"content":703,"tags":708},"夏夜山中喜雨",[704,705,706,707],"何峰雲共出，一夜灑山簷。","爽氣連燈濕，凉聲得樹兼。","醉吟還有意，卧聽似無厭。","已約凌晨起，泉聲自捲簾。",[],{"title":710,"content":711,"tags":716},"飲茶",[712,713,714,715],"晝夢回窗下，秋聲碾樹邊。","僧敲石裏火，瓶汲竹根泉。","影照吟髭碧，香醫酒病痊。","坐餘重有味，猶見半牆煙。",[],{"title":718,"content":719,"tags":724},"方廣寺石橋",[720,721,722,723],"白石峰猶在，橋橫一徑微。","多年無客過，落日有雲歸。","水浄苔生髮，山寒樹著枝。","如何方廣寺，千古去人稀。",[],{"title":726,"content":727,"tags":732},"上穀川尹殿丞",[728,729,730,731],"瑣闥題詩送赴官，郡齋瀟灑謝公閒。","樹邊棋閣開秋鎖，池上吟窗見晝山。","吏散獨行紅葉外，酒醒時望白雲間。","郊原漸有新烟火，知是逋民鳥道還。",[],{"title":734,"content":735,"tags":740},"寄新安梁殿丞",[736,737,738,739],"郡孤詩句許秋供，見說多閒却似慵。","移樹好禽來獨院，上樓高雪在諸峰。","泉當月際摇吟影，蘚自雲邊得屣蹤。","此興未尋頻挂夢，阮公池館舊相容。",[],{"title":742,"content":743,"tags":748},"寄湖州刁殿丞",[744,745,746,747],"白蘋溪湛五亭寒，物象全宜謝守閒。","秋盡棋聲過竹寺，雨餘詩思落茶山。","鳥依高樹和煙宿，人釣清流带月還。","池館有情多入夢，近來誰在笛聲間。",[],{"title":750,"content":751,"tags":756},"秋日過韓原隠居",[752,753,754,755],"霜落門前石齒清，靜中曾聽咽泉聲。","分題會客多無說，抱疾迎僧忽强行。","窗外葉隨蟬影過，井邊苔趁雨痕生。","紗巾任側衣從破，二十年來不到城。",[],{"title":758,"content":759,"tags":764},"贈蘭江鞠明府",[760,761,762,763],"笛裏聲飄柳色寒，縣齋深在白雲間。","孤吟夜倚琴邊月，半醉秋登宅後山。","煙徑樹清苔蘚長，雨塘人散鷺鷥還。","蘭舟有客題詩望，溪上家家晚唱闌。",[],{"title":766,"content":767,"tags":772},"寄蘭江鞠評事",[768,769,770,771],"東望堪思捧刺時，姓名相齒有人知。","公堂伴語山光入，秋閣嘗茶樹影移。","石上琴尊容賞月，水邊臺榭許題詩。","重因送客留連甚，長記還家不敢辭。",[],{"title":774,"content":775,"tags":780},"送春",[776,777,778,779],"留客强歌還强醉，送春多雨又多風。","五更池館孤燈盡，三月江城一夜空。","青鑑照髭疑有白，子規啼樹已無紅。","相逢頼得明年約，應向花前慰斷蓬。",[],{"title":782,"content":783,"tags":788},"春日宿信安張明府廳",[784,785,786,787],"夕景含鐘露欲零，幽人醉宿長官廳。","牀頭蘚色雖連月，池底花陰更雜星。","梧葉乍題無物汙，蜀琴微響有風經。","陶公伴客不思睡，門外數峰山曉青。",[],{"title":790,"content":791,"tags":796},"秋日江亭",[792,793,794,795],"遠岸鳴蟬未擬休，蟬邊風葉動颼飀。","平沙露白聊宜鶴，孤島分青獨映秋。","野色半侵凉氣濕，水光低引夕陽流。","題詩盡日無人伴，空憶瀟湘有釣舟。",[],{"title":798,"content":799,"tags":804},"秋夜舟中作",[800,801,802,803],"酒醒身計正悠悠，澤國如萍信水流。","獨夜聽鴻深浦宿，壞篷連雨一燈秋。","年來自覺新詩澹，江上誰憐舊業優。","腸斷山家數聲笛，不知孤客在孤舟。",[],{"title":806,"content":807,"tags":812},"秋夕旅館言事",[808,809,810,811],"一點孤燈萬里身，離腸曾攪九回頻。","梧桐自管秋來雨，蟋蟀誰妨夜後人。","枕上夢將衣共短，卷中詩與髮同新。","江邊但厭携琴去，明月西風滿路塵。",[],{"title":814,"content":815,"tags":820},"郊居言事",[816,817,818,819],"斷徑危橋積蘚痕，閉關終與俗塵分。","深秋鶴影臨池見，靜夜棋聲隔水聞。","山榻病來誰寄藥，石牀僧去獨看雲。","閒吟閒醉慵開眼，門外寒蟲叫日曛。",[],{"title":822,"content":823,"tags":828},"早秋言事",[824,825,826,827],"苔染衡門綠有錢，不堪風月漸凄然。","蛩聲乍出斜陽外，山色初來落木邊。","掃葉偶窺栽菊地，卷簾因作擣衣篇。","故人南北新髭長，昨夜高懷雨滴蟬。",[],{"title":830,"content":831,"tags":836},"暮秋過沈齊別業",[832,833,834,835],"暮蟬頻響勝聞歌，白石牀清蘚色和。","徑冷獨留寒菊在，樹疏分得夕陽多。","沙泉月澹曾棲雁，籬落烟微半著莎。","誰問寂寥詩景趣，竹扉時有野僧過。",[],{"title":838,"content":839,"tags":844},"宿成秀才水閣",[840,841,842,843],"卷箔無應永夜虛，凭欄還似釣魚師。","牀頭雨細飄蘆葦，池上燈寒照鷺鷥。","孤枕聽泉隨斷夢，好風吹竹隔殘棋。","主人不寐客魂爽，曉色欲來猶賦詩。",[],{"title":846,"content":847,"tags":852},"雨中寄所思",[848,849,850,851],"寒氣蕭蕭瘦骨驚，自知分得四愁平。","秋堂病起莓苔色，池閣夢回蘆葦聲。","幽滴冷痕連樹影，暗驅浮氣入詩情。","衡門獨掩還無語，一日相思萬縷并。",[],{"title":854,"content":855,"tags":860},"登杭州冷泉亭",[856,857,858,859],"天竺峰西月幾層，亭孤應忌俗人登。","山光曉射聞猿客，泉影秋摇倚樹僧。","微雨洒臺青有點，片雲生石冷多稜。","斜陽更起題詩興，還喜重來或未能。",[],{"title":862,"content":863,"tags":868},"題包山人拄杖",[864,865,866,867],"倚雲生處絕塵埃，見說嵩高客寄來。","扶醉未聞和屐到，伴吟曾是探山迴。","尋僧過竹青敲月，看鶴沿池冷卓苔。","知渡寒溪偏得力，夕陽風緊打冰開。",[],{"title":870,"content":871,"tags":876},"蕭山李宰君北亭即事",[872,873,874,875],"掃灑仍聞屬意勞，遶池生蘚不生蒿。","敲花雨過琴絃潤，吹竹風來鶴語高。","窗裏樹光清著枕，硯中山色冷趨毫。","何人可作題詩伴，試茗應思謝法曹。",[],{"title":878,"content":879,"tags":884},"留連李寺丞吟詩閣",[880,881,882,883],"靜閣採詩人罕到，棲棲樹石盡當簷。","長因夜月全開戶，獨爲南山不下簾。","得句硯中秋水減，覓題池上晚花添。","炎天幾度相留宿，曾值風凉雨氣兼。",[],{"title":886,"content":887,"tags":892},"遊石橋寺",[888,889,890,891],"陶令尋山勝境開，偶陪清賞憶天台。","秋雲盡放諸峰出，野水時聞獨鳥來。","巖畔坐宜窺蟋蟀，樹邊行亦避莓苔。","斜陽石上題詩去，更向松陰遶一迴。",[],{"title":894,"content":895,"tags":900},"秋日宿楚江西寺",[896,897,898,899],"滄浪生計任西東，人向蕭園寄斷蓬。","燈影半軒無夢寐，雨聲長夜在梧桐。","閒僧正笑吟髭白，故國那堪水樹紅。","志業未知誰念我，明朝江上葦花風。",[],{"title":902,"content":903,"tags":908},"聞曉角",[904,905,906,907],"一聲初起曉光浮，吟笛啼猿亦暗羞。","清動月華猶滿樹，冷呼山色欲歸棲。","時縈別館侵燈過，偶带凉颸入夢流。","誰會酒醒傾耳聽，近來沙塞不吹秋。",[],{"title":910,"content":911,"tags":916},"觀楚上人陳處士夜棋",[912,913,914,915],"對局清宵解客醺，坐窺玄妙杳難羣。","勢侵孤燭和秋冷，聲隔高梧带雨分。","關外散飛星出漢，水邊斜起雁穿雲。","無人得見爭先意，時被西林宿鳥聞。",[],{"title":918,"content":919,"tags":924},"山居引泉",[920,921,922,923],"自引靈泉勝取冰，入雲窮穴始因僧。","閒疏咽處秋無際，靜聽時來月有層。","乍覺疏林多爽氣，漸疑幽石減寒棱。","雨餘終夜西風起，流過吟窗濺著燈。",[],{"title":926,"content":927,"tags":932},"九日幽居書懷",[928,929,930,931],"九月蟬稀九日催，年光如逐水聲回。","人思往事凄凉在，菊倚荒庭寂寞開。","落葉偶從池上過，夕陽初傍酒邊來。","白衣去後無消息，雨濕東離欲長苔。",[],{"title":934,"content":935,"tags":940},"山行",[936,937,938,939],"翠滿衣裳遠徑分，漸供幽興入詩門。","臨泉自愛窺筇影，過蘚何妨駐屐痕。","因惹斷雲還倚石，偶看殘照學呼猿。","行行却喜無人到，曾爲秋來挂夢魂。",[],{"title":942,"content":943,"tags":948},"憶別",[944,945,946,947],"何處籬筵酒欲空，夜凉橫玉自吹風。","人當木落關山外，秋在月明池館中。","隔水不堪驚遠夢，入雲猶擬觸孤鴻。","憑欄寂寂還多感，誰會無言憶馬融。",[],{"title":950,"content":951,"tags":956},"書韓山人壁",[952,953,954,955],"山木爭高葉盡深，山人半醉在庭陰。","常時具饌多餐藥，偶夜聽松不弄琴。","雨後看苔連井口，秋來見月到池心。","近聞牆外諸峰賣，只向茶鐺便煮金。",[],{"title":958,"content":959,"tags":964},"閒居書事寄逸人平上人",[960,961,962,963],"雨驅寒蘚入衡門，顔子簞瓢近水濆。","靜話忽容幽鳥聽，好詩長忌俗人聞。","半簾松影和秋卷，一檻棋聲與竹分。","野客有情僧有意，相思時望過溪雲。",[],{"title":966,"content":967,"tags":972},"春夕偶作",[968,969,970,971],"一夕衡門獨自開，雨陰深巷少塵埃。","酒醒風傍池邊起，坐久月從花上來。","何處夜歌初欲斷，四鄰春夢未知回。","因思此興無人共，時復孤吟步綠苔。",[],{"title":974,"content":975,"tags":980},"贈張處士",[976,977,978,979],"應問秋雲學得閒，飄然如不在人間。","青籐篋裏詩多怪，紫栗枝邊藥更瘢。","江客對棋曾賭鶴，野僧分屐借登山。","仍聞昨日來城市，又抱孤琴踏月還。",[],{"title":982,"content":983,"tags":996},"寄霅川刁殿丞十二韻",[984,985,986,987,988,989,990,991,992,993,994,995],"柳渾曾吟郡，中間遇牧之。","留連皆往日，寂寞偶多時。","物象應無改，軒車自有期。","憶山辭闕早，採藥上官遲。","澤國閒堪賞，公堂靜好窺。","掃苔紅葉過，臨水白蘋移。","樹石秋供畫，汀洲曉入詩。","謳謠漁父得，孤潔鷺鷥知。","笛外風含酒，樓中月照棋。","野田耕带雨，寒井汲和澌。","城郭煙霞近，人家橘柚垂。","會須尋此景，況與二公宜。",[],{"title":998,"content":999,"tags":1012},"寄梁鼎殿丞十二韻",[1000,1001,1002,1003,1004,1005,1006,1007,1008,1009,1010,1011],"憶昔初投刺，秋風八詠邊。","偶參知好日，一見自多年。","吏散公堂雨，簾垂館樹煙。","共吟曾試茗，同坐或聞蟬。","渡水尋支遁，登山學地仙。","獻酬多奧妙，談笑屢遷延。","夜月侵詩卷，寒燈落酒船。","醉容韋带去，卧許石床連。","池館今何遠，汀洲思倍牽。","庾樓知有約，顔巷奈多緣。","病眼隨蓬斷，愁心逐旆懸。","高臺聊一望，芳草夕陽天。",[],{"title":1014,"content":1015,"tags":1023},"答聖俞設膾示客",[1016,1017,1018,1019,1020,1021,1022],"梅侯三年江上居，盤羞慣飽鮆與鱸。","客居京城厭粗糲，買魚斫鱠邀朋徒。","孰親刀匕擅精巧，閨中麗人家本吳。","縷裁長絲葉剪藿，餖飣自與尋常殊。","霜橙擣虀飯香稻，一飽豈顧家有無。","我雖日病興不淺，坐想落紙霏紅腴。","雕盤雋味儻可再，贈子佳酒隨長魚。",[],{"title":1025,"content":1026,"tags":1029},"皇帝閤春帖子  其一",[1027,1028],"和氣生金殿，微風轉畫旗。","年年好春色，先是帝先知。",[],{"title":1031,"content":1032,"tags":1035},"皇帝閤春帖子  其二",[1033,1034],"迎氣來歸祚，宜春祝降祥。","皇心勤治切，應喜寸陰長。",[],{"title":1037,"content":1038,"tags":1041},"皇帝閤春帖子  其三",[1039,1040],"醇醴浮金斝，柔蔬飣玉盤。","嘗新兼介壽，齊奉兩宮歡。",[],{"title":1043,"content":1044,"tags":1047},"皇帝閤春帖子  其四",[1045,1046],"晝漏新添刻，春寒早放班。","恩隨寬大詔，一日徧人間。",[],{"title":1049,"content":1050,"tags":1053},"太皇太后閤春帖子  其一",[1051,1052],"位歷三朝重，風行四海深。","坐將仁厚意，潜助發生心。",[],{"title":1055,"content":1056,"tags":1059},"太皇太后閤春帖子  其二",[1057,1058],"銅霤消殘凍，朱樓拂早霞。","傳聲回步輦，來賞小桃花。",[],{"title":1061,"content":1062,"tags":1065},"太皇太后閤春帖子  其三",[1063,1064],"視膳回天仗，含飴樂聖辰。","定應彤管筆，書美繫王春。",[],{"title":1067,"content":1068,"tags":1071},"太后閤春帖子  其一",[1069,1070],"正閫成王化，開宮奉母儀。","身當四海養，壽祝萬年期。",[],{"title":1073,"content":1074,"tags":1077},"太后閤春帖子  其二",[1075,1076],"玉律生陽早，金鋪上日遲。","枝間雙鵲語，應是報春祺。",[],{"title":1079,"content":1080,"tags":1083},"太后閤春帖子  其三",[1081,1082],"平曉開朱戶，輕寒御練衣。","宮童移鈿扇，慶壽早期歸。",[],{"title":1085,"content":1086,"tags":1089},"皇后閤春帖子  其一",[1087,1088],"后德侔姬國，嬪風協舜家。","進賢陽爲長，修教月增華。",[],{"title":1091,"content":1092,"tags":1095},"皇后閤春帖子  其二",[1093,1094],"春色平明到，微風弄綵幡。","稍回宮柳弱，更助壁椒溫。",[],{"title":1097,"content":1098,"tags":1101},"皇后閤春帖子  其三",[1099,1100],"暖日暉暉動，清渠泱泱流。","六宮修歲事，拂拭採桑鈎。",[],{"title":1103,"content":1104,"tags":1107},"夫人閤春帖子  其一",[1105,1106],"臘雪餘香徑，朝暉上綺甍。","翠生蘭蕙色，和入管絃聲。",[],{"title":1109,"content":1110,"tags":1113},"夫人閤春帖子  其二",[1111,1112],"重錦褰粧幕，輕羅換舞衣。","釵頭雙燕子，先向社前飛。",[],{"title":1025,"content":1115,"tags":1118},[1116,1117],"晴日漸消宮瓦雪，和風微散御爐烟。","千官拜舞皇恩罷，綵勝金幡下九天。",[],{"title":1031,"content":1120,"tags":1123},[1121,1122],"早鶑啼滑知寒薄，午馬休遲覺晝長。","四海歡康幾務暇，簫韶時奉萬年觴。",[],{"title":1049,"content":1125,"tags":1128},[1126,1127],"鏤成寶字題宮戶，剪出名花趁御筵。","人世不知春色早，忽驚歌吹下中天。",[],{"title":1055,"content":1130,"tags":1133},[1131,1132],"長樂鐘殘寶殿開，鳴梢移仗問安來。","欲知純孝通天地，和氣氤氳遶御杯。",[],{"title":1061,"content":1135,"tags":1138},[1136,1137],"殘冰未放溝聲滑，小雨頻催柳色新。","晝漏漸長無一字，坐披黄老味天真。",[],{"title":1140,"content":1141,"tags":1144},"太皇太后閤春帖子  其四",[1142,1143],"金花鏤勝隨春燕，綵仗縈絲逐土牛。","迎得韶華入中禁，和風次第遍神州。",[],{"title":1146,"content":1147,"tags":1150},"太皇太后閤春帖子  其五",[1148,1149],"綵仗朝來散玉京，綺窗新網結初晴。","靜呼宮女教調曲，閒引皇孫看學行。",[],{"title":1152,"content":1153,"tags":1156},"太皇太后閤春帖子  其六",[1154,1155],"九奏清新稱玉斝，八珍和旨奉蘭羞。","須知天子娛親意，不爲乘春事燕游。",[],{"title":1085,"content":1158,"tags":1161},[1159,1160],"紫蘭紅蓼簇香盤，曉逐金壺下太官。","朝罷三宮歸已晚，日華明麗雪消殘。",[],{"title":1091,"content":1163,"tags":1166},[1164,1165],"葭灰已逐陽和動，繡鏤初隨日景加。","欲助君王修儉德，不將宮樣織新花。",[],{"title":1103,"content":1168,"tags":1171},[1169,1170],"漸暖正當挑菜日，輕陰新變養花天。","君王勤政稀游幸，院院相過理管絃。",[],{"title":1109,"content":1173,"tags":1176},[1174,1175],"宮娃拂曉已催班，拜謝春幡列御前。","不待春風報花信，紅酥綵縷鬭芳妍。",[],{"title":1178,"content":1179,"tags":1184},"新定旅館中作",[1180,1181,1182,1183],"歲月鄉關外，溪山螟色中。","孤城秋閉雨，獨客夜聞鴻。","病使新髭出，貧令舊業空。","思歸不得夢，欹枕近梧桐。",[],{"title":1186,"content":1187,"tags":1192},"桐江晚望",[1188,1189,1190,1191],"疊浪浸天青，離愁望處生。","雨餘孤島暝，花落一船橫。","岸遠紅蘭濕，魚狂白鳥驚。","無人問行客，山寺莫鐘聲。",[],{"title":1194,"content":1195,"tags":1200},"莫冬新定郡樓閒望",[1196,1197,1198,1199],"江城逢歲莫，獨自倚樓臺。","積雪明孤島，微陽在早梅。","水摇冰欲泮，春近雁思回。","故國還如此，歸心但暗催。",[],{"title":1202,"content":1203,"tags":1208},"秋晚舟泊桐江",[1204,1205,1206,1207],"嚴子陵邊水自流，夕陽無語倚松舟。","乍逢風月羞爲客，及到溪山識盡秋。","移樹斷蟬初過雨，立沙孤雁偶隨鷗。","鄉心旅思何人會，蘆葦蕭蕭一笛幽。",[],{"title":1210,"content":1211,"tags":1216},"寄國清處謙",[1212,1213,1214,1215],"三江風浪隔天台，想見當時賦詠才。","近有高僧飛錫去，更無餘事出山來。","猿猱歷歷窺香火，日月紛紛付劫灰。","我欲相期談實相，東林何必謝劉雷。",[],{"title":1218,"content":1219,"tags":1222},"天台思古",[1220,1221],"遊人行盡天台路，仙家杳杳知何處。","惟有山前一派溪，落花依舊流春暮。",[],{"title":1224,"content":1225,"tags":1230},"柘湖",[1226,1227,1228,1229],"鹽邑淪此地，柘山巋其瀕。","復驚秦時女，化作湖上神。","世事喜反覆，孰能識涯津。","物變雖萬殊，不惑固有人。",[],{"title":1232,"content":1233,"tags":1238},"翻經臺",[1234,1235,1236,1237],"勝事如飛去不還，無人更立白雲間。","西風落盡貝多葉，明月自來天竺山。","巖桂影寒泉暗滴，玉爐香減蘚空斑。","孤猿亦有斜陽恨，莫道三聲是等閑。",[],{"title":1240,"content":1241,"tags":1246},"望雲巖",[1242,1243,1244,1245],"不見寒山子，寒巖日欲曛。","無人到深處，衆木隠孤雲。","水照秋痕盡，霞明晚翠分。","孤猿啼亦在，誰向此中聞。",[],{"title":1248,"content":1249,"tags":1251},"句  其一",[1250],"十度花殘一覺眠。",[],{"title":1253,"content":1254,"tags":1256},"句  其二",[1255],"天遠草離離。",[],{"title":1258,"content":1259,"tags":1261},"存目  其一",[1260],"詩題：和子華對雨有感（二首）首句：小桃凌亂逐東風，雪白朱紅幾萬枝。",[],{"title":12