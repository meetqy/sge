[{"data":1,"prerenderedAt":1924},["Reactive",2],{"content-query-SuaAgRPVCi":3,"content-navigation-8C37fagqQL":653},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":649,"_type":650,"_source":651,"_file":652,"_extension":650},"/liu-jian","",false,"劉兼",[9,17,25,33,41,49,57,65,73,81,89,97,103,111,119,127,135,143,151,159,167,175,183,191,199,207,215,223,231,239,247,255,263,271,279,287,295,303,311,319,327,335,343,351,359,367,375,383,391,399,407,415,423,431,439,447,455,463,471,479,487,495,503,511,519,525,533,541,549,557,565,573,581,589,597,605,613,619,625,633,641],{"title":10,"content":11,"tags":16},"送從弟舍人入蜀",[12,13,14,15],"嘉陵江畔餞行車，離袂難分十里餘。","慷慨莫誇心似鐵，留連不覺淚成珠。","風光川谷梅將發，音信雲天雁未疏。","立馬舉鞭無限意，會稀別遠擬何如。",[],{"title":18,"content":19,"tags":24},"再見從弟舍人",[20,21,22,23],"屈指依稀十五年，鸞臺秘閣位相懸。","分飛淮甸雁行斷，重見江樓蟾影圓。","滯跡未偕朝北闕，高才方命入西川。","願君通理須還早，拜慶慈親几杖前。",[],{"title":26,"content":27,"tags":32},"春晚寓懷",[28,29,30,31],"一承兌澤蒞方州，八度春光照郡樓。","好景幾時官吏醉，名山時領管弦遊。","空花任爾頻侵眼，老雪從他漸滿頭。","歸去杜陵池閣在，只能歡笑不能愁。",[],{"title":34,"content":35,"tags":40},"送文英大師",[36,37,38,39],"屈指平陽別社蓮，蟾光一百度曾圓。","孤雲自在知何處，薄宦參差亦信緣。","山郡披風方穆若，花時分袂更凄然。","摇鞭相送嘉陵岸，迴首羣峰隔囊邊。",[],{"title":42,"content":43,"tags":48},"早秋遊湖上亭",[44,45,46,47],"摇落江天萬木空，雁行斜戛塞垣風。","征閨搗月離愁遠，舊館眠雲旅夢通。","郢客豈能陪下里，臯禽爭肯戀樊籠。","此心曠蕩誰相會，盡在南華十卷中。",[],{"title":50,"content":51,"tags":56},"征婦怨",[52,53,54,55],"金閨寂寞罷粧臺，玉筯欄干界粉腮。","花落掩關春欲暮，月圓欹枕夢初迴。","鸞膠豈續愁腸斷，龍劍難揮別緒開。","曾寄錦書無限意，塞鴻何事不歸來。",[],{"title":58,"content":59,"tags":64},"寄高書記",[60,61,62,63],"齊朝慶裔祖敖曹，麟角無雙鳳九毛。","聲價五侯爭辟命，文章一代振風騷。","醉琴自寄陶家意，夢枕誰聽益郡刀。","補衮應星曾奏舉，北山南海孰爲高。",[],{"title":66,"content":67,"tags":72},"晚樓寓懷",[68,69,70,71],"薄暮疏林宿鳥還，倚樓垂袂復憑欄。","月沉江底珠輪凈，雲鎖峰頭玉葉寒。","劉毅暫貧雖壯志，馮唐將老自低顔。","無言獨對秋風立，擬把朝簪換釣竿。",[],{"title":74,"content":75,"tags":80},"偶有下殤因而自遣",[76,77,78,79],"彭壽殤齡共兩空，幻泡緣影夢魂中。","缺圓宿會長如月，飄忽浮生疾似風。","脩短百年先後定，賢愚千古是非同。","南柯太守知人意，休問陶陶塞上翁。",[],{"title":82,"content":83,"tags":88},"旭川祁宰思家而卒因述意呈秦川知己",[84,85,86,87],"歲稔民康絕訟論，政成公暇自由身。","朝看五馬閑如社，夜擁雙姬暖似春。","家記不憂憑冢子，官資無愧是朝臣。","豈同齷齪祁員外，至死悲凉一婦人。",[],{"title":90,"content":91,"tags":96},"偶聞官吏舉請輒有一篇寄呈成都府從弟舍人",[92,93,94,95],"官吏潜陳借寇詞，宦情鄉夢兩相違。","青城錦水無心住，紫閣蓮峰有意歸。","張翰鱸魚因醉憶，孟光書信近春稀。","黄茅瘴色看看起，貪者猶疑別是機。",[],{"title":98,"content":99,"tags":102},"春遊",[100,101],"柳成金穗草如茵，載酒尋花共賞春。","先入醉鄉君莫問，十年風景在三秦。",[],{"title":104,"content":105,"tags":110},"再看光福寺牡丹",[106,107,108,109],"去年曾看牡丹花，蛺蝶迎人傍綵霞。","今日再遊光福寺，春風吹我入仙家。","當筵芬馥歌脣動，倚檻嬌羞醉眼斜。","來歲未朝京闕去，依前和露載歸衙。",[],{"title":112,"content":113,"tags":118},"從弟舍人惠茶",[114,115,116,117],"曾求芳茗貢蕪詞，果沐頒霑味甚奇。","龜背起紋輕炙處，雲頭翻液乍烹時。","老丞倦悶偏宜矣，舊客過從別有之。","珍重宗親相寄惠，水亭山閣自携持。",[],{"title":120,"content":121,"tags":126},"登郡樓書懷三首  其一",[122,123,124,125],"烟雨樓臺漸晦冥，錦江澄碧浪花平。","卞和未雪荆山耻，莊舄空傷越國情。","天際寂寥無雁下，雲端依約有僧行。","登高欲繼離騷詠，魂斷愁深寫不成。",[],{"title":128,"content":129,"tags":134},"登郡樓書懷三首  其二",[130,131,132,133],"邊郡荒凉悲且歌，故園迢遞隔烟波。","琴聲背俗終如是，劍氣衝星又若何。","朝客漸通書信少，釣舟頻引夢魂多。","北山更有移文者，白首無塵歸去麽。",[],{"title":136,"content":137,"tags":142},"登郡樓書懷三首  其三",[138,139,140,141],"莫嗔阮氏哭途窮，萬代深沉恨亦同。","瑞玉豈知將抵鵲，鉛刀何事却屠龍。","九夷欲適嗟吾道，五柳終歸效古風。","獨倚郡樓無限意，滿江烟雨正冥濛。",[],{"title":144,"content":145,"tags":150},"春夕遣懷",[146,147,148,149],"窮通分定莫凄凉，且放歡情入醉鄉。","范蠡扁舟終去相，馮唐半世只爲郎。","風飄玉笛梅初落，酒泛金樽月未央。","休把虛名撓懷抱，九原丘壠盡侯王。",[],{"title":152,"content":153,"tags":158},"貴遊",[154,155,156,157],"綉衣公子宴池塘，淑景融融萬卉芳。","珠翠照天春未老，管弦臨水日初長。","風飄柳線金成穗，雨洗梨花玉有香。","醉後不能離綺席，擬憑青帝繫斜陽。",[],{"title":160,"content":161,"tags":166},"中春登樓二首  其一",[162,163,164,165],"金盃不以滌愁腸，江郡芳時憶故鄉。","兩岸烟花春富貴，一樓風月夜凄凉。","王章莫耻牛衣淚，潘岳休驚鶴鬢霜。","歸去蓮花歸未得，白雲深處有茅堂。",[],{"title":168,"content":169,"tags":174},"中春登樓二首  其二",[170,171,172,173],"古今通塞莫咨嗟，謾把霜髯敵歲華。","失手已慚蛇有足，用心休爲鼠無牙。","九天雲凈方憐月，一夜風高便厭花。","獨倚郡樓人不會，釣舟春浪接平沙。",[],{"title":176,"content":177,"tags":182},"中春宴遊",[178,179,180,181],"二月風光似洞天，紅英翠萼蔟芳筵。","楚王雲雨迷巫峽，江令文章媚蜀箋。","歌黛久顰春岫歛，舞衣新綉曉霞鮮。","酒闌香袂初分散，笑指漁翁釣暮烟。",[],{"title":184,"content":185,"tags":190},"江岸獨步",[186,187,188,189],"醉卓寒筇傍水行，漁翁不會獨吟情。","龜能顧印誰相重，鶴偶乘軒自可輕。","簪組百年終長物，文章千古亦虛名。","是非得喪皆閑事，休向南柯與夢爭。",[],{"title":192,"content":193,"tags":198},"咸陽懷古",[194,195,196,197],"高秋咸鎬起霜風，秦漢荒陵樹葉紅。","七國鬭鷄方賈勇，中原逐鹿更爭雄。","南山漠漠雲常在，渭水悠悠事旋空。","立馬舉鞭遥望處，阿房遺址夕陽東。",[],{"title":200,"content":201,"tags":206},"倦學",[202,203,204,205],"樂廣亡來水鏡稀，宓妃嫫母混妍媸。","且於霧裏藏玄豹，休向窗中問碧鷄。","百氏典墳空自苦，一堆螢雪竟誰知。","門前春色芳如晝，好掩書齋任所之。",[],{"title":208,"content":209,"tags":214},"晝寢",[210,211,212,213],"花落青苔錦數重，書淫不覺避春慵。","恣情枕上飛莊蝶，任爾雲間騁陸龍。","玉液未能消氣魄，牙籤方可滌昏蒙。","起來已被詩魔引，窗外寒敲翠竹風。",[],{"title":216,"content":217,"tags":222},"簡豎儒",[218,219,220,221],"蹄涔豈信有滄浪，螢火何堪並太陽。","淵奧未曾探禹穴，矜誇便擬越丘牆。","小巫神氣終須怯，下俚音聲必不長。","近日冰壺多晦昧，虎皮羊質也觀光。",[],{"title":224,"content":225,"tags":230},"春夕寓興",[226,227,228,229],"忘憂何必在庭萱，是事悠悠竟可寬。","酒病未能辭錦里，春狂又擬入桃源。","風吹楊柳絲千縷，月照梨花雪萬團。","閑泥金徽度芳夕，幽泉石上自潺湲。",[],{"title":232,"content":233,"tags":238},"江樓望鄉寄內子",[234,235,236,237],"獨上江樓望故鄉，淚襟霜笛共凄凉。","雲生隴首秋雖早，月在天心夜已長。","魂夢只能隨蛺蝶，烟波無計學鴛鴦。","蜀箋都有三千幅，總寫離情寄孟光。",[],{"title":240,"content":241,"tags":246},"春晝醉眠",[242,243,244,245],"朱欄芳草綠纖纖，欹枕高堂卷晝簾。","處處落花春寂寂，時時中酒病厭厭。","塞鴻信斷雖堪訝，梁燕詞多且莫嫌。","自有卷書銷永日，霜華未用鬢邊添。",[],{"title":248,"content":249,"tags":254},"命妓不至",[250,251,252,253],"琴中難挑孰憐才，獨對良宵酒數盃。","蘇子黑貂將已敝，宋弘青鳥又空迴。","月空凈牖霜成隙，風卷殘花錦作堆。","欹枕夢魂何處去，醉和春色入天台。",[],{"title":256,"content":257,"tags":262},"誡是非",[258,259,260,261],"巧舌如簧總莫聽，是非多自愛憎生。","三人告母雖投杼，百犬聞風只吠聲。","辨玉且寬和氏罪，誣金須認不疑情。","因思疇昔遊談者，六國交馳亦受烹。",[],{"title":264,"content":265,"tags":270},"自遣",[266,267,268,269],"未上亨衢獨醉吟，賦成無處博黄金。","家人莫問張儀舌，國士須知豫讓心。","照乘始堪沽善價，陽春爭忍混凡音。","鵾鵬鱗翼途程在，九萬風雲海浪深。",[],{"title":272,"content":273,"tags":278},"蓮塘霽望",[274,275,276,277],"新秋菡萏發紅英，向晚風飄滿郡馨。","萬疊水紋羅乍展，一雙鸂鶒綉初成。","採蓮奴散吳歌闋，拾翠人歸楚雨晴。","遠岸牧童吹短笛，蓼花深處信牛行。",[],{"title":280,"content":281,"tags":286},"秋夕書懷二首  其一",[282,283,284,285],"荒僻淹留歲已深，解龜無計恨難任。","守方半會蠻夷語，賀厦全忘燕雀心。","夜靜倚樓悲月笛，秋寒欹枕泣霜砧。","宦情總逐愁腸斷，一筯鱸魚直萬金。",[],{"title":288,"content":289,"tags":294},"秋夕書懷二首  其二",[290,291,292,293],"直氣從來不入時，掩關慵更釣磻溪。","斯文未喪宣尼歎，吾道將窮阮籍悲。","輕粉覆牆凝夜砌，亂金鋪菊織秋籬。","南陽卧久無人問，薄命非才有可疑。",[],{"title":296,"content":297,"tags":302},"長春節",[298,299,300,301],"聖朝佳節遇長春，跪捧金爐祝又焚。","寶藏發來天地秀，兵戈銷後帝皇尊。","太平基址千年永，混一車書萬古存。","更有馨香滿芳檻，和風遲日在蘭蓀。",[],{"title":304,"content":305,"tags":310},"對雨",[306,307,308,309],"幽庭凝碧亦漣漪，簷霤聲繁聒夢歸。","半岫金烏纔委照，一川石燕又交飛。","濯枝霢霂梅花吐，吹渚飄颻暑氣微。","因憶故園閑釣處，蒼苔斑駁滿漁磯。",[],{"title":312,"content":313,"tags":318},"登樓寓望",[314,315,316,317],"憑高多是偶汍瀾，紅葉何堪照病顔。","萬疊雲山供遠恨，一軒風物送秋寒。","背琴鶴客歸松徑，橫笛牛童卧蓼灘。","獨倚郡樓無限意，夕陽西邁水東邊。",[],{"title":320,"content":321,"tags":326},"訪飲妓不遇招酒徒不至",[322,323,324,325],"小橋流水接平沙，何處行雲不在家。","畢卓未來輕竹葉，劉晨重到殢桃花。","琴樽冷落春將盡，幃幌蕭條日又斜。","回首却尋芳草路，金鞭拂柳思無涯。",[],{"title":328,"content":329,"tags":334},"春宴河亭",[330,331,332,333],"柳擺輕絲拂嫩黄，檻前流水滿池塘。","一筵金翠臨芳岸，四面烟花出粉牆。","舞袖逐風翻綉浪，歌塵隨燕下雕梁。","蠻箋象管休凝思，且放春心入醉鄉。",[],{"title":336,"content":337,"tags":342},"夢歸故園二首  其一",[338,339,340,341],"桐葉飛霜落井欄，菱花藏雪助衰顔。","夜窗颯颯摇寒竹，秋枕迢迢夢故山。","臨水釣舟橫葦岸，隔溪禪侶啟柴關。","覺來依舊三更月，離緒鄉心起萬端。",[],{"title":344,"content":345,"tags":350},"夢歸故園二首  其二",[346,347,348,349],"舊館秋寒一夢長，水簾疏影入迴塘。","宦情率爾拖漁艇，客恨依然在燕梁。","白鷺獨飄山面雪，紅蕖全謝鏡心香。","起來不語無人會，醉倚東軒半夕陽。",[],{"title":352,"content":353,"tags":358},"新竹",[354,355,356,357],"近窗卧砌兩三叢，佐靜添幽別有功。","影鏤碎金初透月，聲敲寒玉乍摇風。","無憑費叟烟波碧，莫信湘妃淚點紅。","自是子猷偏愛爾，虛心高節雪霜中。",[],{"title":360,"content":361,"tags":366},"木芙蓉",[362,363,364,365],"素靈失律詐風流，强把芳菲半載偷。","是葉葳蕤霜照夜，此花爛熳火燒秋。","謝蓮色淡爭堪種，陶菊香穠亦合羞。","誰道金風能肅物，因何厚薄不相侔。",[],{"title":368,"content":369,"tags":374},"玉燭花",[370,371,372,373],"裊裊香英三四枝，亭亭紅艷照階墀。","正當晚檻初開處，恰似春闈就試時。","少女不吹方熠爚，東君偏惜未離披。","夜深斜倚朱欄外，擬把隣光借與誰。",[],{"title":376,"content":377,"tags":382},"新蟬",[378,379,380,381],"齊女幈幃失舊容，侍中冠冕有芳蹤。","翅翻晚鬢尋香露，聲引秋絲逐遠風。","旅館聽時髭欲白，戍樓聞處葉多紅。","只知送恨添愁事，誰見凌霄羽蛻功。",[],{"title":384,"content":385,"tags":390},"晨鷄",[386,387,388,389],"朱冠金距綵毛身，昧爽高聲已報晨。","作瑞莫慚先貢楚，擅場須信獨推秦。","淮南也伴升仙犬，函谷曾容借曉人。","此日卑棲隨飲啄，宰君驅我亦相馴。",[],{"title":392,"content":393,"tags":398},"春燕",[394,395,396,397],"多時窗外語呢喃，只要佳人卷綉簾。","大厦已成須慶賀，高門頻入莫憎嫌。","花間舞蝶和香趁，江畔春泥带雨啣。","栖息數年情已厚，營巢爭肯傍他簷。",[],{"title":400,"content":401,"tags":406},"萬葛樹",[402,403,404,405],"葉如羽蓋豈堪論，百步清陰鎖綠雲。","善政已聞思召伯，英風偏稱號將軍。","靜鋪講席麟經潤，高拂□枝兔影分。","更有歲寒霜雪操，莫將樗櫟擬相羣。",[],{"title":408,"content":409,"tags":414},"秋夕書懷呈戎州郎中  其一",[410,411,412,413],"素律初迴枕簟凉，松風飄泊入華堂。","談鷄寂默紗窗靜，夢蝶蕭條玉漏長。","歸去水雲多阻隔，別來情緒足悲傷。","霜砧月笛休相引，只有離襟泪兩行。",[],{"title":416,"content":417,"tags":422},"秋夕書懷呈戎州郎中  其二",[418,419,420,421],"風送秋荷滿鼻香，竹聲敲玉近虛廊。","夢迴故國情方黯，月過疏簾夜正凉。","菱鏡也知移艷態，錦書其柰隔年光。","鸞膠處處難尋覓，斷盡相思寸寸腸。",[],{"title":424,"content":425,"tags":430},"重陽感懷二首  其一",[426,427,428,429],"重陽不忍上高樓，寒菊年年照暮秋。","萬疊故山雲總隔，兩行香淚血和流。","黄茅莽莽連邊郡，紅葉紛紛落釣舟。","歸計未成年漸老，茱萸羞戴雪霜頭。",[],{"title":432,"content":433,"tags":438},"重陽感懷二首  其二",[434,435,436,437],"載花乘酒上高山，四望秋空八極寬。","蜀國江山存不得，劉家豚犬取何難。","張儀舊壁蒼苔厚，葛亮荒祠古木寒。","獨對斜陽更惆悵，錦江東注似波瀾。",[],{"title":440,"content":441,"tags":446},"蜀都道中作",[442,443,444,445],"劍關雲棧亂崢嶸，得喪何由險與平。","千載龜城終失守，一堆鬼錄謾留名。","季年必不延昏主，薄賞那堪激懦兵。","李特後來多二世，納降歸擬盡公卿。",[],{"title":448,"content":449,"tags":454},"蜀都春晚感懷",[450,451,452,453],"蜀都春色漸離披，夢斷雲空事莫追。","宮闕一城荒作草，王孫猶自醉如泥。","誰家玉笛吹殘照，柳市金絲拂舊堤。","可惜錦江無錦濯，海棠花下杜鵑啼。",[],{"title":456,"content":457,"tags":462},"新迴車院筵上作",[458,459,460,461],"迴車院了未迴車，三載疲民詠袴襦。","借寇已承英主詔，乞骸須上老臣書。","黄金蜀柳籠朱戶，碧玉湘筠映綺疏。","因問滿筵詩酒客，錦江何處有鱸魚。",[],{"title":464,"content":465,"tags":470},"宣賜錦袍設上贈諸郡客",[466,467,468,469],"十月芙蓉花滿枝，天庭驛騎賜寒衣。","將同玉蝶侵肌冷，也遣金鵬遍體飛。","夜卧始知多忝竊，晝行方覺轉光輝。","深冬若得朝丹闕，太華峰前衣錦歸。",[],{"title":472,"content":473,"tags":478},"海棠花",[474,475,476,477],"淡淡微紅色不深，依依偏得似春心。","烟輕虢國顰歌黛，露重長門歛淚衿。","低傍綉簾人易折，密藏香蕊蝶難尋。","良宵更有多情處，月下芬芳伴醉吟。",[],{"title":480,"content":481,"tags":486},"春晚閑望",[482,483,484,485],"東風滿地是梨花，只把琴心殢酒家。","立處晚樓橫短笛，望中春草接平沙。","雁行斷續晴天遠，燕翼參差翠幕斜。","歸計未成頭欲白，釣舟烟浪思無涯。",[],{"title":488,"content":489,"tags":494},"寄酬滑州文秀大師",[490,491,492,493],"分飛屈指十三年，菡萏峰前別社蓮。","薄宦偶然來左蜀，孤雲何事在南燕。","一封瑶檢音初達，兩處金沙色共圓。","珍重湯休惠佳句，郡齋吟久不成眠。",[],{"title":496,"content":497,"tags":502},"寄長安鄭員外",[498,499,500,501],"屈指良交十四人，隙駒風燭漸爲塵。","當初花下三秦客，只有天涯二老身。","乘醉幾同遊北內，尋芳多共謁東鄰。","此時阻隔關山遠，月滿江樓淚滿巾。",[],{"title":504,"content":505,"tags":510},"貽諸學童",[506,507,508,509],"橫經叉手步還趨，積善方知慶有餘。","五箇小雛離學院，一行新雁入貧居。","攘羊告罪言何直，舐犢牽情理豈虛。","勸汝立身須苦志，月中丹桂自扶疏。",[],{"title":512,"content":513,"tags":518},"對鏡",[514,515,516,517],"青鏡重磨照白鬚，白鬚閑撚意何如。","故園迢遞千山外，荒郡淹留四載餘。","風送竹聲侵枕簟，月移花影過庭除。","秋霜滿領難消釋，莫讀離騷失意書。",[],{"title":520,"content":521,"tags":524},"中夏晝卧",[522,523],"寂寂無憀九夏中，傍簷依壁待清風。","壯圖奇策無人問，不及南陽一卧龍。",[],{"title":526,"content":527,"tags":532},"到郡後寄西川從弟舍人右司閻郎中齊殿院",[528,529,530,531],"蜀路新修盡坦平，交親深幸再逢迎。","正當返袂思鄉國，却似歸家見弟兄。","霑澤只慚堯綍重，泝流還喜范舟輕。","欲將感戀裁書旨，多少魚箋寫得成。",[],{"title":534,"content":535,"tags":540},"初至郡界",[536,537,538,539],"嘉陵江畔接榮川，兩畔旌旗下瀨船。","郡印已分炎瘴地，朝衣猶惹御爐烟。","蓮塘小飲香隨艇，月榭高吟水壓天。","錦字莫嫌歸路遠，華夷一統太平年。",[],{"title":542,"content":543,"tags":548},"送二郎君歸長安",[544,545,546,547],"我兒辭去淚雙流，蜀郡秦川兩處愁。","紅葉滿山歸故國，黄茅徧地住他州。","荷衣曉掛慚官吏，菱鏡秋窺訝鬢髹。","好向雲泉營舊隠，莫教莊叟畏犧牛。",[],{"title":550,"content":551,"tags":556},"登郡樓書事",[552,553,554,555],"偶奉綸書蒞旭川，郡樓嘉致盡依然。","松欹鳥道雲藏寺，月滿漁舟水浸天。","望帝古祠花簇簇，錦城歸路草芊芊。","有時倚檻垂雙袂，故國風光似眼前。",[],{"title":558,"content":559,"tags":564},"酬勾評事",[560,561,562,563],"閑庭欹枕正悲秋，忽覺新編涴遠愁。","才薄只愁安雁戶，年高空憶復漁舟。","鷺翹皓雪臨汀岸，蓮裊紅香匝郡樓。","對景却慚無藻思，南金荆玉卒難酬。",[],{"title":566,"content":567,"tags":572},"郡樓閑望書懷",[568,569,570,571],"郡城樓閣遶江濱，風物清秋入望頻。","銅鼓祭龍雲塞廟，蘆花飄市雪粘人。","蓮披凈沼羣香散，鷺點寒烟玉片新。","歸去杜陵池館在，且將朝服拂埃塵。",[],{"title":574,"content":575,"tags":580},"郡齋寓興",[576,577,578,579],"依約樊川似旭川，郡齋風物盡蕭然。","秋庭碧蘚鋪雲錦，晚閣紅蕖簇水仙。","醉筆語狂揮粉壁，歌梁塵亂拂花鈿。","情懷放蕩無羈束，地角天涯亦信緣。",[],{"title":582,"content":583,"tags":588},"春游",[584,585,586,587],"摇摇離緒不能持，滿郡花開酒熟時。","羞聽黄鶑求善友，强隨綠柳展愁眉。","隔雲故國山千疊，傍水芳林錦萬枝。","聖主未容歸北闕，且將勤儉撫南夷。",[],{"title":590,"content":591,"tags":596},"春宵",[592,593,594,595],"春雲春日共朦朧，滿院梨花半夜風。","宿酒未醒珠箔卷，艷歌初闋玉樓空。","五湖范蠡才堪重，六印蘇秦道不同。","再取素琴聊假寐，南柯靈夢莫相通。",[],{"title":598,"content":599,"tags":604},"芳春",[600,601,602,603],"微雨微風隔晝簾，金爐檀炷冷慵添。","桃花滿地春牢落，柳絮成堆雪棄嫌。","寶瑟不能邀卓氏，綵毫何必夢江淹。","宦情歸興休相撓，隼旆漁舟總未厭。",[],{"title":606,"content":607,"tags":612},"春霽",[608,609,610,611],"春霽江山似畫圖，醉垂鞭袂出康衢。","猖狂亂打貔貅鼓，稽懶慵修鴛鷺書。","老色漸來欺鬢髮，閑情將欲傲簪裾。","苔錢遍地知多少，買得花枝不落無。",[],{"title":614,"content":615,"tags":618},"春夜",[616,617],"薄薄春雲籠皓月，杏花滿地堆香雪。","醉垂羅袂倚朱欄，小數玉仙歌未闋。",[],{"title":620,"content":621,"tags":624},"西齋",[622,623],"西齋新竹兩三莖，也有風敲碎玉聲。","莫恨移來欄檻遠，譬如元本此間生。",[],{"title":626,"content":627,"tags":632},"春怨",[628,629,630,631],"繡林紅岸落花鈿，故去新來感自然。","絕塞杪春悲漢月，長林深夜泣緗弦。","錦書雁斷應難寄，菱鏡鸞孤貌可憐。","獨倚畫屏人不會，夢魂纔別戍樓邊。",[],{"title":634,"content":635,"tags":640},"宴遊池館",[636,637,638,639],"綺筵金碧照芳菲，酒滿瑶卮水滿池。","去歲南岐離郡日，今春東蜀看花時。","儉蓮發臉當籌著，緒柳生腰按柘枝。","座客半酣言笑狎，孔融懷抱正怡怡。",[],{"title":642,"content":643,"tags":648},"去年今日",[644,645,646,647],"去年今日到榮州，五騎紅塵入郡樓。","貔虎只知迎太守，蠻夷不信是儒流。","姦豪已息時將泰，疲瘵全蘇歲又周。","聖主若容辭重祿，便歸烟水狎羣鷗。",[],"content:liu-jian.json","json","content","liu-jian.json",[654,657,660,663,666,669,672,675,678,681,684,687,690,693,696,699,702,705,708,711,714,717,720,723,726,729,732,735,738,741,744,747,750,753,756,759,762,765,768,771,774,777,780,783,786,789,792,795,798,801,804,807,810,813,816,819,822,825,828,831,834,837,840,843,846,849,852,855,858,861,864,867,870,873,876,879,882,885,888,891,894,897,900,903,906,909,912,915,918,921,924,927,930,933,936,939,942,945,948,951,954,957,960,963,966,969,972,975,978,981,984,987,990,993,996,999,1002,1005,1008,1011,1014,1017,1020,1023,1026,1029,1032,1035,1038,1041,1044,1047,1050,1053,1056,1059,1062,1065,1068,1071,1074,1077,1080,1083,1086,1089,1092,1095,1098,1101,1104,1107,1108,1111,1114,1117,1120,1123,1126,1129,1132,1135,1138,1141,1144,1147,1150,1153,1156,1159,1162,1165,1168,1171,1174,1177,1180,1183,1186,1189,1192,1195,1198,1201,1204,1207,1210,1213,1216,1219,1222,1225,1228,1231,1234,1237,1240,1243,1246,1249,1252,1255,1258,1261,1264,1267,1270,1273,1276,1279,1282,1285,1288,1291,1294,1297,1300,1303,1306,1309,1312,1315,1318,1321,1324,1327,1330,1333,1336,1339,1342,1345,1348,1351,1354,1357,1360,1363,1366,1369,1372,1375,1378,1381,1384,1387,1390,1393,1396,1399,1402,1405,1408,1411,1414,1417,1420,1423,1426,1429,1432,1435,1438,1441,1444,1447,1450,1453,1456,1459,1462,1465,1468,1471,1474,1477,1480,1483,1486,1489,1492,1495,1498,1501,1504,1507,1510,1513,1516,1519,1522,1525,1528,1531,1534,1537,1540,1543,1546,1549,1552,1555,1558,1561,1564,1567,1570,1573,1576,1579,1582,1585,1588,1591,1594,1597,1600,1603,1606,1609,1612,1615,1618,1621,1624,1627,1630,1633,1636,1639,1642,1645,1648,1651,1654,1657,1660,1663,1666,1669,1672,1675,1678,1681,1684,1687,1690,1693,1696,1699,1702,1705,1708,1711,1714,1717,1720,1723,1726,1729,1732,1735,1738,1741,1744,1747,1750,1753,1756,1759,1762,1765,1768,1771,1774,1777,1780,1783,1786,1789,1792,1795,1798,1801,1804,1807,1810,1813,1816,1819,1822,1825,1828,1831,1834,1837,1840,1843,1846,1849,1852,