[{"data":1,"prerenderedAt":1536},["Reactive",2],{"content-query-s4XGl83uZI":3,"content-navigation-8C37fagqQL":265},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":261,"_type":262,"_source":263,"_file":264,"_extension":262},"/li-shang-yin","",false,"李商隱",[9,22,33,41,56,89,99,106,116,127,139,148,157,162,173,180,189,196,204,214,220,228,235,244,253],{"title":10,"content":11,"tags":16},"錦瑟",[12,13,14,15],"錦瑟無端五十絃，一絃一柱思華年。","莊生曉夢迷蝴蝶，望帝春心託杜鵑。","滄海月明珠有淚，藍田日暖玉生煙。","此情可待成追憶，只是當時已惘然。",[17,18,19,20,21],"唐诗三百首","思念","七言律诗","抒情","爱情",{"title":23,"content":24,"tags":29},"蟬",[25,26,27,28],"本以高難飽，徒勞恨費聲。","五更疎欲斷，一樹碧無情。","薄宦梗猶汎，故園蕪已平。","煩君最相警，我亦舉家清。",[17,30,31,32],"咏物","咏物诗","五言律诗",{"title":34,"content":35,"tags":38},"樂遊原",[36,37],"向晚意不適，驅車登古原。","夕陽無限好，只是近黃昏。",[17,39,40],"惜时","哲理",{"title":42,"content":43,"tags":46},"夜雨寄北",[44,45],"君問歸期未有期，巴山夜雨漲秋池。","何當共剪西窗燭，却話巴山夜雨時。",[17,18,47,48,49,50,51,52,21,53,54,55],"写雨","写景","景中情","初中古诗","高中古诗","七言绝句","三年级上册","雨","七年级上册(课外)",{"title":57,"content":58,"tags":85},"韓碑",[59,60,61,62,63,64,65,66,67,68,69,70,71,72,73,74,75,76,77,78,79,80,81,82,83,84],"元和天子神武姿，彼何人哉軒與羲。","誓將上雪列聖恥，坐法宮中朝四夷。","淮西有賊五十載，封狼生貙貙生羆。","不據山河據平地，長戈利矛日可麾。","帝得聖相相曰度，賊斫不死神扶持。","腰懸相印作都統，陰風慘澹天王旗。","愬武古通作牙爪，儀曹外郎載筆隨。","行軍司馬智且勇，十四萬衆猶虎貔。","入蔡縛賊獻太廟，功無與讓恩不訾。","帝曰汝度功第一，汝從事愈宜爲辭。","愈拜稽首蹈且舞，金石刻畫臣能爲。","古者世稱大手筆，此事不繫于職司。","當仁自古有不讓，言訖屢頷天子頤。","公退齋戒坐小閣，濡染大筆何淋漓。","點竄堯典舜典字，塗改清廟生民詩。","文成破體書在紙，清晨再拜鋪丹墀。","表曰臣愈昧死上，詠神聖功書之碑。","碑高三丈字如斗，負以靈鼇蟠以螭。","句奇語重喻者少，讒之天子言其私。","長繩百尺拽碑倒，麤砂大石相磨治。","公之斯文若元氣，先時已入人肝脾。","湯盤孔鼎有述作，今無其器存其辭。","嗚呼聖皇及聖相，相與烜赫流淳熙。","公之斯文不示後，曷與三五相攀追。","願書萬本誦萬過，口角流沫右手胝。","傳之七十有二代，以爲封禪玉檢明堂基。",[17,86,87,88],"叙事","七言古诗","咏史",{"title":90,"content":91,"tags":96},"風雨",[92,93,94,95],"淒涼寶劍篇，羈泊欲窮年。","黃葉仍風雨，青樓自管絃。","新知遭薄俗，舊好隔良緣。","心斷新豐酒，銷愁斗幾千。",[17,97,32,98],"风雨","怀才不遇",{"title":100,"content":101,"tags":104},"寄令狐郎中",[102,103],"嵩雲秦樹久離居，雙鯉迢迢一紙書。","休問梁園舊賓客，茂陵秋雨病相如。",[17,18,48,52,105],"友情",{"title":107,"content":108,"tags":113},"隋宮",[109,110,111,112],"紫泉宮殿鎖煙霞，欲取蕪城作帝家。","玉璽不緣歸日角，錦帆應是到天涯。","于今腐草無螢火，終古垂楊有暮鴉。","地下若逢陳後主，豈宜重問後庭花。",[17,114,19,115,88],"隋・唐・五代","讽刺",{"title":117,"content":118,"tags":123},"籌筆驛",[119,120,121,122],"猿鳥猶疑畏簡書，風雲常爲護儲胥。","徒令上將揮神筆，終見降王走傳車。","管樂有才終不忝，關張無命欲何如。","他年錦里經祠廟，梁父吟成恨有餘。",[17,114,19,124,125,126],"典故","写人","咏史怀古",{"title":128,"content":129,"tags":134},"無題二首 一",[130,131,132,133],"昨夜星辰昨夜風，畫樓西畔桂堂東。","身無綵鳳雙飛翼，心有靈犀一點通。","隔座送鉤春酒暖，分曹射覆蠟燈紅。","嗟余聽鼓應官去，走馬蘭臺類斷蓬。",[17,135,19,21,136,137,138],"宴饮","相思","感慨","组诗",{"title":140,"content":141,"tags":146},"無題四首 一",[142,143,144,145],"來是空言去絕踪，月斜樓上五更鐘。","夢爲遠別啼難喚，書被催成墨未濃。","蠟照半籠金翡翠，麝熏微度繡芙蓉。","劉郎已恨蓬山遠，更隔蓬山一萬重。",[17,18,147,19,21],"记梦",{"title":149,"content":150,"tags":155},"無題四首 二",[151,152,153,154],"颯颯東風細雨來，芙蓉塘外有輕雷。","金蟾齧鏁燒香入，玉虎牽絲汲井迴。","賈氏窺簾韓掾少，宓妃留枕魏王才。","春心莫共花爭發，一寸相思一寸灰。",[17,136,156,21,19],"女子",{"title":107,"content":158,"tags":161},[159,160],"乘興南遊不戒嚴，九重誰省諫書函。","春風舉國裁宮錦，半作障泥半作帆。",[17,52,126,115],{"title":163,"content":164,"tags":169},"落花",[165,166,167,168],"高閣客竟去，小園花亂飛。","參差連曲陌，迢遰送斜暉。","腸斷未忍掃，眼穿仍欲歸。","芳心向春盡，所得是沾衣。",[17,30,32,170,171,172,31],"写花","抒怀","花",{"title":174,"content":175,"tags":178},"爲有",[176,177],"爲有雲屏無限嬌，鳳城寒盡怕春宵。","無端嫁得金龜壻，辜負香衾事早朝。",[17,52,179],"怨情",{"title":181,"content":182,"tags":187},"無題",[183,184,185,186],"相見時難別亦難，東風無力百花殘。","春蠶到死絲方盡，蠟炬成灰淚始乾。","曉鏡但愁雲鬢改，夜吟應覺月光寒。","蓬山此去無多路，青鳥殷勤爲探看。",[17,18,19,188,50,21],"八年级下册(课外)",{"title":190,"content":191,"tags":194},"瑤池",[192,193],"瑤池阿母綺窗開，黃竹歌聲動地哀。","八駿日行三萬里，穆王何事不重來。",[17,52,195,115],"神话",{"title":197,"content":198,"tags":203},"春雨",[199,200,201,202],"悵臥新春白袷衣，白門寥落意多違。","紅樓隔雨相望冷，珠箔飄燈獨自歸。","遠路應悲春晼晚，殘宵猶得夢依稀。","玉璫緘札何由達，萬里雲羅一雁飛。",[17,47,19,21,54],{"title":205,"content":206,"tags":209},"常娥",[207,208],"雲母屏風燭影深，長河漸落曉星沈。","常娥應悔偷靈藥，碧海青天夜夜心。",[17,210,211,125,212,52,213],"孤独","中秋","三年级下册","小学古诗",{"title":128,"content":215,"tags":219},[216,217,218],"鳳尾香羅薄幾重，碧文圓頂夜深縫。","扇裁月魄羞難掩，車走雷聲語未通。","曾是寂寥金燼暗，斷無消息石榴紅。",[18,19,17],{"title":221,"content":222,"tags":227},"無題二首 二",[223,224,225,226],"重帷深下莫愁堂，臥後清宵細細長。","神女生涯原是夢，小姑居處本無郎。","風波不信菱枝弱，月露誰教桂葉香。","直道相思了無益，未妨惆悵是清狂。",[17,18,19,20,21],{"title":229,"content":230,"tags":233},"賈生",[231,232],"宣室求賢訪逐臣，賈生才調更無倫。","可憐夜半虛前席，不問蒼生問鬼神。",[17,114,234,52,98],"托古讽今",{"title":236,"content":237,"tags":242},"涼思",[238,239,240,241],"客去波平檻，蟬休露滿枝。","永懷當此節，倚立自移時。","北斗兼春遠，南陵寓使遲。","天涯占夢數，疑誤有新知。",[17,243,32],"愁思",{"title":245,"content":246,"tags":251},"北青蘿",[247,248,249,250],"殘陽西入崦，茅屋訪孤僧。","落葉人何在，寒雲路幾層。","獨敲初夜磬，閑倚一枝藤。","世界微塵裏，吾寧愛與憎。",[17,252,32],"寻访",{"title":254,"content":255,"tags":258},"赤壁",[256,257],"折戟沈沙鐵未銷，自將磨洗認前朝。","東風不與周郎便，銅雀春深鎖二喬。",[17,114,259,260,50,52,126,98],"战争","八年级下册(课内)","content:li-shang-yin.json","json","content","li-shang-yin.json",[266,269,272,275,278,281,284,287,290,293,296,299,302,305,308,311,314,317,320,323,326,329,332,335,338,341,344,347,350,353,356,359,362,365,368,371,374,377,380,383,386,389,392,395,398,401,404,407,410,413,416,419,422,425,428,431,434,437,440,443,446,449,452,455,458,461,464,467,470,473,476,479,482,485,488,491,494,497,500,503,506,509,512,515,518,521,524,527,530,533,536,539,542,545,548,551,554,557,560,563,566,569,572,575,578,581,584,587,590,593,596,599,602,605,608,611,614,617,620,623,626,629,632,635,638,641,644,647,648,651,654,657,660,663,666,669,672,675,678,681,684,687,690,693,696,699,702,705,708,711,714,717,720,723,726,729,732,735,738,741,744,747,750,753,756,759,762,765,768,771,774,777,780,783,786,789,792,795,798,801,804,807,810,813,816,819,822,825,828,831,834,837,840,843,846,849,852,855,858,861,864,867,870,873,876,879,882,885,888,891,894,897,900,903,906,909,912,915,918,921,924,927,930,933,936,939,942,945,948,951,954,957,960,963,966,969,972,975,978,981,984,987,990,993,996,999,1002,1005,1008,1011,1014,1017,1020,1023,1026,1029,1032,1035,1038,1041,1044,1047,1050,1053,1056,1059,1062,1065,1068,1071,1074,1077,1080,1083,1086,1089,1092,1095,1098,1101,1104,1107,1110,1113,1116,1119,1122,1125,1128,1131,1134,1137,1140,1143,1146,1149,1152,1155,1158,1161,1164,1167,1170,1173,1176,1179,1182,1185,1188,1191,1194,1197,1200,1203,1206,1209,1212,1215,1218,1221,1224,1227,1230,1233,1236,1239,