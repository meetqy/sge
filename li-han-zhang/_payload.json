[{"data":1,"prerenderedAt":1215},["Reactive",2],{"content-query-r8AbmVpCEy":3,"content-navigation-8C37fagqQL":118},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":114,"_type":115,"_source":116,"_file":117,"_extension":115},"/li-han-zhang","",false,"李含章",[9,17,25,33,41,49,57,65,73,81,89,97,105],{"title":10,"content":11,"tags":16},"石盆寺",[12,13,14,15],"巨石何年琢，爲盆勢莫裁。","圓疑通澤國，深可並天台。","法水長時滿，青蓮向日開。","無人知此喻，寂寞首空迴。",[],{"title":18,"content":19,"tags":24},"題武陵護戎林亭",[20,21,22,23],"修笙簇徑出林椒，可與遊人避俗囂。","曲檻隔花安筆硯，小窗延月伴簞瓢。","紅藤引蔓移山豆，綠葉分葩植水蕉。","到此每憐清氣別，可堪寒夜雨瀟瀟。",[],{"title":26,"content":27,"tags":32},"寄題元長老林亭",[28,29,30,31],"聞說幽亭氣象寬，何須基構入雲端。","平瞻一郡連山翠，遠矚千門枕水寒。","風拂石床松子墜，雨籠莎徑栗花乾。","遥知出定牽吟夜，只欠清猿嘯碧湍。",[],{"title":34,"content":35,"tags":40},"漁家",[36,37,38,39],"數家茅屋枕魚磯，歲歲生涯在渺瀰。","雞犬怪人奔竹徑，兒孫逢客倚柴籬。","壁懸簑笠殘煙雨，門對溪山閱盛衰。","一曲新歌一船月，夜深來往碧漣漪。",[],{"title":42,"content":43,"tags":48},"寄內翰王同年",[44,45,46,47],"回翔青瑣已多時，四海歸榮更是誰。","通籍禁中參鳳閣，司言天上煥龍墀。","修文禮樂源何大，振筆雲雷澤益滋。","若把王褒頌勳業，未如際會遇昌期。",[],{"title":50,"content":51,"tags":56},"河北行",[52,53,54,55],"爲儒還解著征衣，遠戍沙連白草齊。","雁陣不衝羌笛怨，狼煙微認塞雲底。","蕎花露溼堆空塹，蓼水泓澄截古堤。","漸近界河分內外，野禽嘐戛路東西。",[],{"title":58,"content":59,"tags":64},"出典宣城三首  其一",[60,61,62,63],"戛雲秋翼健磨天，九萬修程孰可肩。","五馬幸歸鄉國路，百城初認釣魚船。","餘霞綺閣方池上，宿靄人家疊嶂前。","多感聖君垂異寵，力思報效向衰年。",[],{"title":66,"content":67,"tags":72},"出典宣城三首  其二",[68,69,70,71],"蘆花未白蓼初紅，綠水澄藍是處通。","景色不須今日別，煙光仍與昔時同。","馬嘶曉月思閑廐，帆掛秋光足便風。","寄語路人休借問，錦衣歸去一衰翁。",[],{"title":74,"content":75,"tags":80},"出典宣城三首  其三",[76,77,78,79],"分符慙寡術，榮抃出京畿。","況什新秋節，重經舊釣磯。","江村初雨歇，水國淡煙微。","一路風光好，還同衣錦歸。",[],{"title":82,"content":83,"tags":88},"遊桃源觀",[84,85,86,87],"碧草芊綿十洞春，青蒼寒疊五溪雲。","山縈乳竇層層秀，路隔桃花處處分。","苔徑竹深迷鶴跡，石壇松古漏星文。","通宵回想塵寰事，好結茅茨向水濱。",[],{"title":90,"content":91,"tags":96},"相國寺晚鐘",[92,93,94,95],"高韻出重城，林梢鶴夢清。","九衢方月曉，四野復霜晴。","會有神仙聽，能無魍魎驚。","寒山深洞口，依約片雲橫。",[],{"title":98,"content":99,"tags":104},"僧家",[100,101,102,103],"不愁白日斜，唯只是僧家。","遠徑接芳草，閒庭掩落花。","木欄棲悲翠，盆水種蒹葭。","到此常留意，深窗自煮茶。",[],{"title":106,"content":107,"tags":112},"題廬山上化成寺",[108,109,110,111],"樓臺高聳入中天，壓破寒山數畝煙。","永日松欄幽鳥語，半空巖石老僧禪。","窗臨樛木排深澗，簷拂長蘿下翠顛。","野客每來何所得，一聲秋磬似相便。",[113],"庐山","content:li-han-zhang.json","json","content","li-han-zhang.json",[119,122,125,128,131,134,137,140,143,146,149,152,155,158,161,164,167,170,173,176,179,182,185,188,191,194,197,200,203,206,209,212,215,218,221,224,227,230,233,236,239,242,245,248,251,254,257,260,263,266,269,272,275,278,281,284,287,290,293,296,299,302,305,308,311,314,317,320,323,326,329,332,335,338,341,344,347,350,353,356,359,362,365,368,371,374,377,380,383,386,389,392,395,398,401,404,407,410,413,416,419,422,425,426,429,432,435,438,441,444,447,450,453,456,459,462,465,468,471,474,477,480,483,486,489,492,495,498,501,504,507,510,513,516,519,522,525,528,531,534,537,540,543,546,549,552,555,558,561,564,567,570,573,576,579,582,585,588,591,594,597,600,603,606,609,612,615,618,621,624,627,630,633,636,639,642,645,648,651,654,657,660,663,666,669,672,675,678,681,684,687,690,693,696,699,702,705,708,711,714,717,720,723,726,729,732,735,738,741,744,747,750,753,756,759,762,765,768,771,774,777,780,783,786,789,792,795,798,801,804,807,810,813,816,819,822,825,828,831,834,837,840,843,846,849,852,855,858,861,864,867,870,873,876,879,882,885,888,891,894,897,900,903,906,909,912,915,918,921,924,927,930,933,936,939,942,945,948,951,954,957,960,963,966,969,972,975,978,981,984,987,990,993,996,999,1002,1005,1008,1011,1014,1017,1020,1023,1026,1029,1032,1035,1038,1041,1044,1047,1050,1053,1056,1059,1062,1065,1068,1071,1074,1077,1080,1083,1086,1089,1092,1095,1098,1101,1104,1107,1110,1113,1116,1119,1122,1125,1128,1131,1134,1137,1140,1143,1146,1149,1152,1155,1158,1161,1164,1167,1170,1173,1176,1179,1182,1185,1188,1191,1194,1197,1200,1203,1206,1209,1212],{"title":120,"_path":121},"安德裕","/an-de-yu",{"title":123,"_path":124},"白居易","/bai-ju-yi",{"title":126,"_path":127},"畢士安","/bi-shi-an",{"title":129,"_path":130},"卞震","/bian-zhen",{"title":132,"_path":133},"不詳","/bu-xiang",{"title":135,"_path":136},"蔡襄","/cai-xiang",{"title":138,"_path":139},"曹操","/cao-cao",{"title":14