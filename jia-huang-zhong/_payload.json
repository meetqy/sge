[{"data":1,"prerenderedAt":1150},["Reactive",2],{"content-query-VlNWXt2kbD":3,"content-navigation-8C37fagqQL":53},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":49,"_type":50,"_source":51,"_file":52,"_extension":50},"/jia-huang-zhong","",false,"賈黄中",[9,19,25,33,41],{"title":10,"content":11,"tags":18},"伏睹禁林盛事謹賦一章",[12,13,14,15,16,17],"璇題飛白御毫新，三體瓊章妙入神。","特賜禁林爲盛事，只緣明主重名臣。","青編輝映輕前古，丹地深嚴隔世塵。","金籙禎祥非是寶，玉堂名號此方貞。","恩榮誰比烟霄客，文彩長懸日月輪。","爲報鼇宮主人道，蓬萊全勝昔時春。",[],{"title":20,"content":21,"tags":24},"桂林還珠洞",[22,23],"赫赫威聲震百蠻，昔携筐筥溷谿山。","無人爲起文淵問，端的珠還薏苡還。",[],{"title":26,"content":27,"tags":32},"吟貽宣議大師英上人",[28,29,30,31],"金僊子便是師師，高道寧容世綱羈。","浩浩心田龍可擾，飄飄行止鶴應知。","塵機擺落超三界，古篆沉研冠一時。","莫怪伊余苦珍重，白蓮花社有心期。",[],{"title":34,"content":35,"tags":40},"送新知永州潘宮贊若沖赴任",[36,37,38,39],"鴛鷺行中已著名，頌條暫慰遠民情。","道途行去乘軺貴，鄉里過時畫錦榮。","鈴閣曉開江月滿，戟枝寒照雪峰明。","知君游刃多餘暇，莫忘新詩寄鳳城。",[],{"title":42,"content":43,"tags":48},"題華林書院",[44,45,46,47],"旌陽舊山曲，書室構山前。","景物塵埃外，軒窗樹石邊。","寧惟集儒業，兼恐混神僊。","百行家門最，宜當世有賢。",[],"content:jia-huang-zhong.json","json","content","jia-huang-zhong.json",[54,57,60,63,66,69,72,75,78,81,84,87,90,93,96,99,102,105,108,111,114,117,120,123,126,129,132,135,138,141,144,147,150,153,156,159,162,165,168,171,174,177,180,183,186,189,192,195,198,201,204,207,210,213,216,219,222,225,228,231,234,237,240,243,246,249,252,255,258,261,264,267,270,273,276,279,282,285,288,291,294,297,300,303,306,309,310,313,316,319,322,325,328,331,334,337,340,343,346,349,352,355,358,361,364,367,370,373,376,379,382,385,388,391,394,397,400,403,406,409,412,415,418,421,424,427,430,433,436,439,442,445,448,451,454,457,460,463,466,469,472,475,478,481,484,487,490,493,496,499,502,505,508,511,514,517,520,523,526,529,532,535,538,541,544,547,550,553,556,559,562,565,568,571,574,577,580,583,586,589,592,595,598,601,604,607,610,613,616,619,622,625,628,631,634,637,640,643,646,649,652,655,658,661,664,667,670,673,676,679,682,685,688,691,694,697,700,703,706,709,712,715,718,721,724,727,730,733,736,739,742,745,748,751,754,757,760,763,766