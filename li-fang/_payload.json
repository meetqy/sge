[{"data":1,"prerenderedAt":1637},["Reactive",2],{"content-query-QqULKGvKpl":3,"content-navigation-8C37fagqQL":366},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":362,"_type":363,"_source":364,"_file":365,"_extension":363},"/li-fang","",false,"李昉",[9,17,25,33,41,49,57,65,73,81,89,97,105,113,121,139,147,157,165,173,181,189,197,205,213,221,229,237,245,253,261,269,276,284,290,298,304,312,319,324,329,334,339,344,349,354],{"title":10,"content":11,"tags":16},"小園獨坐偶賦所懷寄秘閣侍郎",[12,13,14,15],"烟光澹澹思悠悠，朝退還家懶出遊。","靜坐最憐紅日永，新晴更助小園幽。","砌苔點點青錢小，窗竹森森綠玉稠。","賓友不來春又晚，眼看辜負一年休。",[],{"title":18,"content":19,"tags":24},"偶述所懷寄秘閣侍郎",[20,21,22,23],"何事情懷鬰不開，爲思蓬閣謫仙才。","通宵空有夢魂去，隔月更無篇詠來。","夜室已聞啼蟋蟀，秋庭惟見長莓苔。","清晨懶把菱花照，兩鬢如絲堪自咍。",[],{"title":26,"content":27,"tags":32},"偶書口號寄秘閣侍郎",[28,29,30,31],"朝退歸來只在家，詩書滿架是生涯。","吟成拙句何人和，按得新聲沒處誇。","夜景最憐蟾影潔，秋空時見雁行斜。","望君偷暇來相訪，猶有東籬殘菊花。",[],{"title":34,"content":35,"tags":40},"獨賞牡丹因而成詠",[36,37,38,39],"遶東叢了遶西叢，爲愛叢叢紫閒紅。","怨望乍疑啼曉霧，妖饒渾欲殢春風。","香苞半綻丹砂吐，細朵齊開烈焰烘。","病老情懷慢相對，滿欄應笑白頭翁。",[],{"title":42,"content":43,"tags":48},"依韻和殘春有感  其一",[44,45,46,47],"暮春三月思依依，又到年年惜別時。","暖逼流鶑藏密樹，香迷舞蝶戀空枝。","海棠殘艷紅鋪地，蜀柳長條翠拂池。","便是林亭微暑至，葛衣重喜趁凉披。",[],{"title":50,"content":51,"tags":56},"依韻和殘春有感  其二",[52,53,54,55],"夜來微雨曉來風，春色都歸悵望中。","楊柳带烟嚬細綠，牡丹和露泣殘紅。","嬌鶑遶樹聲初老，野草連門路近通。","韶景欲留留不住，西園悶殺雪髯翁。",[],{"title":58,"content":59,"tags":64},"和喜雨",[60,61,62,63],"溟濛烟景似江臯，靜賞何須郭外遨。","點點暗滋花氣色，祁祁盡作麥脂膏。","洗開玳瑁湘妃竹，裛損烟脂阿母桃。","昨日君王倚欄看，東風吹濕赭黄袍。",[],{"title":66,"content":67,"tags":72},"依韻奉和見貽之什且以答來章而歌盛美也",[68,69,70,71],"逢丘深靜養疏慵，角枕斜欹數過鴻。","萬事不關思想內，一心長在詠歌中。","昂藏鶴貌無凡態，冷淡琴聲有古風。","共說聖君偏注意，朝昏須作黑頭公。",[],{"title":74,"content":75,"tags":80},"攀和嘉篇",[76,77,78,79],"小亭愁坐對殘陽，梧葉翻階片片黄。","老去只添新悵望，病餘無復舊歡狂。","四時奔速都如電，兩鬢凋疏總作霜。","看取衰容今若此，有何情緒聽宮商。",[],{"title":82,"content":83,"tags":88},"老病相攻偶成長句寄秘閣侍郎",[84,85,86,87],"衰病增加我斗諳，頭風目眩一般般。","縱逢盃酒都無味，任聽笙歌亦寡歡。","朝退便思親枕簟，客來多倦著衣冠。","行行漸近懸車歲，轉恐君恩報答難。",[],{"title":90,"content":91,"tags":96},"寄秘閣侍郎",[92,93,94,95],"楮冠仍用竹爲簪，隨分林亭自稱心。","病裏亦難全斷酒，老來無柰更躭吟。","閑行策杖青苔逕，靜坐移床綠樹陰。","時有新詩寄何處，蓬丘仙客是知音。",[],{"title":98,"content":99,"tags":104},"輒歌盛美寄秘閣侍郎",[100,101,102,103],"滿朝清望更誰如，才略文章盡有餘。","避寵怕聞調鼎鼐，愛閑專喜掌圖書。","歌詩唱和心偏樂，勢利奔趨跡自疏。","恰與宗兄性相近，好來城外卜鄰居。",[],{"title":106,"content":107,"tags":112},"對雨閑吟呈吏部侍郎",[108,109,110,111],"羃羃濃雲勢未收，森森凉氣忽如秋。","密籠芳樹飄何急，高卷疏簾看不休。","咫尺莫聞人對語，逡巡已見水平流。","此時遥憶諸君子，堪命棋枰與酒甌。",[],{"title":114,"content":115,"tags":120},"自思忝幸因動詠吟",[116,117,118,119],"如蓬短髮不勝簪，筋力衰羸分所甘。","清職美官皆徧歷，物情時態盡深諳。","乘軒服冕身可用，嘯月吟風意尚躭。","安得故人頻會面，一罇相對共醺酣。",[],{"title":122,"content":123,"tags":138},"依韻奉和千葉玫瑰之什",[124,125,126,127,128,129,130,131,132,133,134,135,136,137],"滿檻妖饒甚，皆因暖律催。","好憑鶑說意，不假蝶爲媒。","带露羞容歛，隨風笑臉迴。","去年觀始種，今日見齊開。","熠熠燈千炷，熒熒火一堆。","濃香蓋天與，碎葉是誰裁。","旋爲除芳草，惟愁落綠苔。","最宜含細雨，肯使撲輕埃。","易賦詩盈軸，難辭酒滿盃。","汲泉頻灌溉，買土更封培。","銷得邀賓賞，堪教選地栽。","醉吟翻悵望，行遶重徘徊。","謾對鮮妍色，慚無綺麗才。","自憐垂白叟，扶病看花來。",[],{"title":140,"content":141,"tags":146},"寶相花送上秘閣侍郎并獻惡詩一首",[142,143,144,145],"寶相爲嘉號，移根自蜀都。","遠來還可重，他處更應無。","嫩葉細裁綠，芳英勻抹朱。","風流別花客，知道此花殊。",[],{"title":148,"content":149,"tags":156},"對海紅花懷吏部侍郎",[150,151,152,153,154,155],"爛熳海紅花，花中信殊異。","萬朵壓欄干，一堆紅錦被。","顔色燒人眼，馨香撲人鼻。","宜哉富豪家，長近歌鍾地。","對花花不語，憶君君不至。","盡日惜穠芳，情懷有如醉。",[],{"title":158,"content":159,"tags":164},"更述荒蕪自詠閑適",[160,161,162,163],"滿架詩書滿炷香，琴碁爲樂是尋常。","誠知老去唯宜靜，自笑閑中亦有忙。","腰下轉嫌金印重，眉間漸長白毫長。","手栽園樹皆成實，引著兒孫旋摘嘗。",[],{"title":166,"content":167,"tags":172},"和夏日直秘閣之什",[168,169,170,171],"靜卧蓬山養道情，百司繁冗盡堪輕。","窗前竹撼森疏影，樹杪蟬吟斷續聲。","閑蹋綠莎芒作履，旋烹芳茗石爲鐺。","料君難戀神仙境，重築沙堤走馬行。",[],{"title":174,"content":175,"tags":180},"和暑夜偶作之什",[176,177,178,179],"新搆華居近禁城，旋栽松檜滿中庭。","順風子細聞清漏，隔竹依稀見落星。","萬乘異恩無路報，千年昌運此時丁。","一軒明月誰爲伴，應共金昆醉復醒。",[],{"title":182,"content":183,"tags":188},"謝侍郎三弟朝蓋相過",[184,185,186,187],"喜得君來慰我心，清歡何假酒頻斟。","共窺鶴跡行苔逕，同聽鶑聲坐柳陰。","蝴蝶遶欄飛又歇，薔薇壓架淺兼深。","相留不用忙忙去，直待林梢夕照沈。",[],{"title":190,"content":191,"tags":196},"輒歌盛美獻秘閣侍郎",[192,193,194,195],"濟時才略本縱橫，翻向文章振大名。","政事堂中辭重位，圖書閣下養閑情。","高高節行將誰比，的的襟懷向我傾。","吟得新詩只相寄，心看軒冕一銖輕。",[],{"title":198,"content":199,"tags":204},"牡丹盛開對之感歎寄秘閣侍郎",[200,201,202,203],"白公曾詠牡丹芳，一種鮮妍獨異常。","眼底見伊真國色，鼻頭聞者是天香。","朝含宿雨低垂淚，晚背殘陽暗斷腸。","多病老翁爭奈向，寄詩遥問少年郎。",[],{"title":206,"content":207,"tags":212},"冬至後作呈秘閣侍郎",[208,209,210,211],"節辰纔過一陽生，草樹依依已有情。","楊柳莫嫌凋舊葉，牡丹還喜動新萌。","潜驚綠竹微添翠，暗覺幽禽漸變聲。","從此日長天又暖，時時獨入小園行。",[],{"title":214,"content":215,"tags":220},"贈鄧洵美",[216,217,218,219],"憶昔詞場共著鞭，當時鶑谷喜同遷。","關河契闊三千里，音信稀疏二十年。","君遇已知依玉帳，我無才藻步花磚。","時情人事堪惆悵，天外相逢一泫然。",[],{"title":222,"content":223,"tags":228},"吊鄧洵美",[224,225,226,227],"十年衣染帝鄉塵，蹤迹仍傳活計貧。","高掇桂枝曾遂志，假拖藍綬至終身。","侯門寂寞非知己，澤國悽惶似旅人。","今已嚮公墳畔過，不勝懷抱暗酸辛。",[],{"title":230,"content":231,"tags":236},"贈賈黄中",[232,233,234,235],"七歲神童古所難，賈家門戶有衣冠。","十人科第排頭上，五部經書誦舌端。","見榜不知名字貴，登筵未識管弦歡。","從今穩上青雲去，萬里誰能測羽翰。",[],{"title":238,"content":239,"tags":244},"寄孟賓于",[240,241,242,243],"幼携書劍別湘潭，金榜標名第十三。","昔日聲塵喧洛下，近年詩價滿江南。","長爲邑令情終屈，縱處曹郎志未甘。","莫學馮唐便休去，明君晚事未爲慚。",[],{"title":246,"content":247,"tags":252},"贈襄陽妓",[248,249,250,251],"峴山亭畔紅妝女，小筆香牋善賦詩。","顔貌共推傾國色，篇章皆是斷腸辭。","便牽魂夢從今日，得見嬋娟在幾時。","千里關河萬重意，夜深無睡暗尋思。",[],{"title":254,"content":255,"tags":260},"禁林春直",[256,257,258,259],"疏簾摇曳日輝輝，直閣深嚴半掩扉。","一院有花春晝永，八方無事詔書稀。","樹頭百囀鶑鶑語，梁上新來燕燕飛。","豈合此身居此地，妨賢尸祿自知非。",[],{"title":262,"content":263,"tags":268},"仙客",[264,265,266,267],"胎化仙禽性本殊，何人携爾到京都。","因加美號爲仙客，稱向閑庭伴野夫。","警露秋聲雲外影，翹沙晴影月中孤。","青田萬里終歸去，暫處鷄羣莫歎吁。",[],{"title":270,"content":271,"tags":274},"題岱宗無字碑",[272,273],"巨石來從十八盤，離宮複道滿千山。","不因封禪窮民力，漢祖何緣便入關。",[275],"泰山",{"title":277,"content":278,"tags":283},"桐柏觀",[279,280,281,282],"子晉棲霞境，高高出世埃。","直疑天上去，歸認下雲來。","銀漢星辰近，金庭洞府開。","遲明欲回首，更上降真臺。",[],{"title":285,"content":286,"tags":289},"退官詩",[287,288],"布裘藜杖鹿胎冠，散率身如不在官。","晝枕靜欹無遠夢，秋窗閑坐有微寒。",[],{"title":291,"content":292,"tags":297},"泰陵忌辰",[293,294,295,296],"秘殿深嚴聖語溫，十年前是一乾坤。","孤臣林壑殺生在，帝里金湯舊業存。","舜殿南風難解慍，漢陵西望欲銷魂。","年年此日無窮恨，風雨瀟瀟獨閉門。",[],{"title":299,"content":300,"tags":303},"永昌陵挽詞",[301,302],"丹青史筆敢虛張，功德巍然軼漢唐。","奠玉五回朝上帝，御樓三度納降王。",[],{"title":305,"content":306,"tags":311},"題義門胡氏華林書院",[307,308,309,310],"孝義冠鄉閭，門多長者車。","歲收千頃稻，家貯一樓書。","待客開新酒，留僧煮嫩蔬。","三公老且病，無暇訪山居。",[],{"title":313,"content":314,"tags":317},"祜公榮歸",[315,316],"南望鄉閭隔楚雲，歸心迢遞更紛紜。","何因得共飛帆上，細看長江濯錦文。",[318],"长江",{"title":320,"content":321,"tags":323},"句  其一",[322],"水光先見月，露氣早知秋。",[],{"title":325,"content":326,"tags":328},"句  其二",[327],"微臣自愧頭如雪，也向鈞天侍玉皇。",[],{"title":330,"content":331,"tags":333},"句  其三",[332],"傷禽再展凌雲翼，枯木重爲犯斗槎。",[],{"title":335,"content":336,"tags":338},"句  其四",[337],"三載經綸棲鳳閣，五年提筆直鰲宮。",[],{"title":340,"content":341,"tags":343},"存目  其一",[342],"詩題：原無題首句：微臣自愧頭如雪，也向鈞天侍玉皇。",[],{"title":345,"content":346,"tags":348},"存目  其二",[347],"詩題：贈正惠公端首句：憶昔僦居明德坊。",[],{"title":350,"content":351,"tags":353},"存目  其三",[352],"（劉瑛整理）。",[],{"title":355,"content":356,"tags":361},"送陳瞻知永州",[357,358,359,360],"昔年同醉杏園春，別後花枝幾度新。","彼此宦遊疏翰墨，等閑交面起絲綸。","榮親未必須萊子，晝錦何嘗只買臣。","布政莫爲三載計，清朝臺閣整搜人。",[],"content:li-fang.json","json","content","li-fang.json",[367,370,373,376,379,382,385,388,391,394,397,400,403,406,409,412,415,418,421,424,427,430,433,436,439,442,445,448,451,454,457,460,463,466,469,472,475,478,481,484,487,490,493,496,499,502,505,508,511,514,517,520,523,526,529,532,535,538,541,544,547,550,553,556,559,562,565,568,571,574,577,580,583,586,589,592,595,598,601,604,607,610,613,616,619,622,625,628,631,634,637,640,643,646,649,652,655,658,661,664,667,670,673,676,679,682,685,688,691,694,697,700,703,706,709,712,715,718,721,724,725,728,731,734,737,740,743,746,749,752,755,758,761,764,767,770,773,776,779,782,785,788,791,794,797,800,803,806,809,812,815,818,821,824,827,830,833,836,839,842,845,848,851,854,857,860,863,866,869,872,875,878,881,884,887,890,893,896,899,902,905,908,911,914,917,920,923,926,929,932,935,938,941,944,947,950,953,956,959,962,965,968,971,974,977,980,983,986,989,992,995,998,1001,1004,1007,1010,1013,1016,1019,1022,1025,1028,1031,1034,1037,1040,1043,1046,1049,1052,1055,1058,1061,1064,1067,1070,1073,1076,1079,1082,1085,1088,1091,1094,1097,1100,1103,1106,1109,1112,1115,1118,1121,1124,1127,1130,1133,1136,1139,1142,1145,1148,1151,1154,1157,1160,1163,1166,1169,1172,1175,1178,1181,1184,1187,1190,1193,1196,1199,1202,1205,1208,1211,1214,1217,1220,1223,1226,1229,1232,1235,1238,1241,1244,1247,1250,1253,1256,1259,1262,1265,1268,1271,1274,1277,1280,1283,1286,1289,1292,1295,1