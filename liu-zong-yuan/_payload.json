[{"data":1,"prerenderedAt":1169},["Reactive",2],{"content-query-uSZWoQ01qG":3,"content-navigation-8C37fagqQL":72},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":68,"_type":69,"_source":70,"_file":71,"_extension":69},"/liu-zong-yuan","",false,"柳宗元",[9,24,34,44,59],{"title":10,"content":11,"tags":19},"晨詣超師院讀禪經",[12,13,14,15,16,17,18],"汲井漱寒齒，清心拂塵服。","閒持貝葉書，步出東齋讀。","真源了無取，妄跡世所逐。","遺言冀可冥，繕性何由熟。","道人庭宇靜，苔色連深竹。","日出霧露餘，青松如膏沐。","澹然離言說，悟悅心自足。",[20,21,22,23],"唐诗三百首","五言古诗","抒情","贬谪",{"title":25,"content":26,"tags":31},"登柳州城樓寄漳汀封連四州",[27,28,29,30],"城上高樓接大荒，海天愁思正茫茫。","驚風亂颭芙蓉水，密雨斜侵薜荔牆。","嶺樹重遮千里目，江流曲似九廻腸。","共來百越文身地，猶自音書滯一鄉。",[20,32,22,33],"七言律诗","怀人",{"title":35,"content":36,"tags":41},"溪居",[37,38,39,40],"久爲簪組累，幸此南夷謫。","閑依農圃鄰，偶似山林客。","曉耕翻露草，夜榜響溪石。","來往不逢人，長歌楚天碧。",[20,42,21,43],"孤独","生活",{"title":45,"content":46,"tags":49},"江雪",[47,48],"千山鳥飛絕，萬逕人蹤滅。","孤舟蓑笠翁，獨釣寒江雪。",[20,50,51,52,53,54,55,56,57,58],"写雪","山水","冬天","雪","冬","写景","写鸟","五言绝句","鸟",{"title":60,"content":61,"tags":65},"漁翁",[62,63,64],"漁翁夜傍西巖宿，曉汲清湘燃楚竹。","煙銷日出不見人，[欸]乃一聲山水綠。","迴看天際下中流，巖上無心雲相逐。",[20,66,67,51],"写人","七言古诗","content:liu-zong-yuan.json","json","content","liu-zong-yuan.json",[73,76,79,82,85,88,91,94,97,100,103,106,109,112,115,118,121,124,127,130,133,136,139,142,145,148,151,154,157,160,163,166,169,172,175,178,181,184,187,190,193,196,199,202,205,208,211,214,217,220,223,226,229,232,235,238,241,244,247,250,253,256,259,262,265,268,271,274,277,280,283,286,289,292,295,298,301,304,307,310,313,316,319,322,325,328,331,334,337,340,343,346,349,352,355,358,361,364,367,370,373,376,379,382,385,388,391,394,397,400,403,406,409,412,415,418,421,424,427,430,433,436,439,442,445,448,451,454,457,460,463,466,469,472,475,476,479,482,485,488,491,494,497,500,503,506,509,512,515,518,521,524,527,530,533,536,539,542,545,548,551,554,557,560,563,566,569,572,575,578,581,584,587,590,593,596,599,602,605,608,611,614,617,620,623,626,629,632,635,638,641,644,647,650,653,656,659,662,665,668,671,674,677,680,683,686,689