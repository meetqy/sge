[{"data":1,"prerenderedAt":1420},["Reactive",2],{"content-query-o2TUyWanSY":3,"content-navigation-8C37fagqQL":149},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":145,"_type":146,"_source":147,"_file":148,"_extension":146},"/yang-hui-zhi","",false,"楊徽之",[9,17,23,29,35,45,53,64,72,80,85,90,95,100,105,110,115,120,125,130,135,140],{"title":10,"content":11,"tags":16},"宿廖融山齋",[12,13,14,15],"清和春尚在，歡醉日何長。","谷鳥隨柯轉，庭花奪酒香。","初晴巖翠滴，嚮晚樹陰凉。","別有堪吟處，相留宿草堂。",[],{"title":18,"content":19,"tags":22},"峨眉",[20,21],"嘉州山水地，二蜀最爲美。","翠嶺疊峨眉，長瀾湧錦水。",[],{"title":24,"content":25,"tags":28},"翠光亭",[26,27],"釣舟浮淺瀨，岡舍晚重林。","雲放千峰出，花藏一徑深。",[],{"title":30,"content":31,"tags":34},"嘉州作",[32,33],"俗遇臘辰持藥獻，吏逢衙日隔花參。","耆宿因來問封部，竹籬西畔是雲南。",[],{"title":36,"content":37,"tags":44},"禁林讌會之什",[38,39,40,41,42,43],"星移歲律應青陽，得奉羣英集玉堂。","龍鳳雙飛觀御札，雲霞五色詠天章。","禁林漸覺清風暖，仙界元知白日長。","詔出紫泥封去潤，朝迴蓮燭賜來香。","二篇稱奬恩尤重，萬國傳聞道更光。","何幸微才逢盛事，願因史冊紀餘芳。",[],{"title":46,"content":47,"tags":52},"漢陽晚泊",[48,49,50,51],"傍橋吟望漢陽城，山徧樓臺徹上層。","犬吠竹籬沽酒客，鶴隨苔岸洗衣僧。","疏鐘未徹聞寒漏，斜月初沈見遠燈。","夜靜鄰船問行計，曉帆相與向巴陵。",[],{"title":54,"content":55,"tags":60},"寒食寄鄭起侍郎",[56,57,58,59],"清明時節出郊原，寂寂山城柳映門。","水隔淡烟脩竹寺，路經疏雨落花村。","天寒酒薄難成醉，地迥樓高易斷魂。","回首故山千里外，別離心緒向誰言。",[61,62,63],"寒食节","思乡","写景",{"title":65,"content":66,"tags":71},"贈譚先生",[67,68,69,70],"古觀重重遶翠微，杉松深處掩雙扉。","雲生萬壑投龍去，海隔三山放鶴歸。","花洞宴遊春日永，石壇朝禮曙星稀。","每聽高論長生理，擬向塵中便拂衣。",[],{"title":73,"content":74,"tags":79},"送夢英大師",[75,76,77,78],"獨携瓶錫欲春殘，深入終南路屈盤。","萬象幽玄吟裏見，一心圓寂定中觀。","翠微寺在杉松老，紫閣峰高水石寒。","莫憑危欄臨北望，滿城烟草是長安。",[],{"title":81,"content":82,"tags":84},"句  其一",[83],"戍樓烟自直，戰地雨長腥。",[],{"title":86,"content":87,"tags":89},"句  其二",[88],"偶題巖石雲生筆，閑遶庭松露濕衣。",[],{"title":91,"content":92,"tags":94},"句  其三",[93],"新霜染楓葉，皓月借蘆花。",[],{"title":96,"content":97,"tags":99},"句  其四",[98],"廢宅寒塘水，荒墳宿草烟。",[],{"title":101,"content":102,"tags":104},"句  其五",[103],"青帝已教春不老，素娥何惜月長圓。",[],{"title":106,"content":107,"tags":109},"句  其六",[108],"浮花水入瞿塘峽，带雨雲歸越嶲州。",[],{"title":111,"content":112,"tags":114},"句  其七",[113],"春歸萬年樹，月滿九重城。",[],{"title":116,"content":117,"tags":119},"句  其八",[118],"開盡菊花秋色老，落殘桐葉雨聲寒。",[],{"title":121,"content":122,"tags":124},"句  其九",[123],"少年牢落今何幸，叨遇君王問姓名。",[],{"title":126,"content":127,"tags":129},"句  其一○",[128],"杳杳烟蕪何處盡，摇摇風柳不勝垂。",[],{"title":131,"content":132,"tags":134},"句  其一一",[133],"江流嶓冢雨，路入漢家山。",[],{"title":136,"content":137,"tags":139},"句  其一二",[138],"山雲收雨半川晴。",[],{"title":141,"content":142,"tags":144},"句  其一三",[143],"少孤心易感。",[],"content:yang-hui-zhi.json","json","content","yang-hui-zhi.json",[150,153,156,159,162,165,168,171,174,177,180,183,186,189,192,195,198,201,204,207,210,213,216,219,222,225,228,231,234,237,240,243,246,249,252,255,258,261,264,267,270,273,276,279,282,285,288,291,294,297,300,303,306,309,312,315,318,321,324,327,330,333,336,339,342,345,348,351,354,357,360,363,366,369,372,375,378,381,384,387,390,393,396,399,402,405,408,411,414,417,420,423,426,429,432,435,438,441,444,447,450,453,456,459,462,465,468,471,474,477,480,483,486,489,492,495,498,501,504,507,510,513,516,519,522,525,528,531,534,537,540,543,546,549,552,555,558,561,564,567,570,573,576,579,582,585,588,591,594,597,600,603,606,609,612,615,618,621,624,627,630,633,636,639,642,645,648,651,654,657,660,663,666,669,672,675,678,681,684,687,690,693,696,699,702,705,708,711,714,717,720,723,726,729,732,735,738,741,744,747,750,753,756,759,762,765,768,771,774,777,780,783,786,789,792,795,798,801,804,807,810,813,816,819,822,825,828,831,834,837,840,843,846,849,852,855,858,861,864,867,870,873,876,879,882,885,888,891,894,897,900,903,906,909,912,915,918,921,924,927,930,933,936,939,942,945,948,951,954,957,960,963,966,969,972,975,978,981,984,987,990,993,996,999,1002,1005,1008,1011,1014,1017,1020,1023,1026,1029,1032,1035,1038,1041,1044,1047,1050,1053,1056,1059,1062,1065,1068,1071,1074,1077,1080,1083,1086,1089,1092,1095,1098,1101,1104,1107,1110,1113,1116,1119,1122,1125,1128,1131,1134,1137,1140,1143,1146,1149,1152,1155,1158,1161,1164,1167,1170,1173,1176,1179,1182,1185,1188,1191,1194,1197,1200,1203,1206,1209,1212,1215,1218,1219,1222,