[{"data":1,"prerenderedAt":1533},["Reactive",2],{"content-query-3Gk5BdV8wL":3,"content-navigation-8C37fagqQL":262},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":258,"_type":259,"_source":260,"_file":261,"_extension":259},"/meng-guan","",false,"孟貫",[9,17,25,33,42,50,58,66,74,82,90,98,106,114,122,130,138,146,154,162,170,178,186,194,202,210,218,226,234,242,250],{"title":10,"content":11,"tags":16},"宿山寺",[12,13,14,15],"溪山盡日行，方聽遠鐘聲。","入院逢僧定，登樓見月生。","露垂羣木潤，泉落一巖清。","此景關吾事，通宵寐不成。",[],{"title":18,"content":19,"tags":24},"贈棲隠洞譚先生",[20,21,22,23],"先生雙鬢華，深谷卧雲霞。","不伐有巢樹，多移無主花。","石泉春釀酒，松火夜煎茶。","因問山中事，如君有幾家。",[],{"title":26,"content":27,"tags":32},"歸雁",[28,29,30,31],"春至衡陽雁，思歸塞路長。","汀洲齊奮翼，霄漢共成行。","雪盡翻風暖，寒收度月凉。","直應到秋日，依舊返瀟湘。",[],{"title":34,"content":35,"tags":40},"春江送人",[36,37,38,39],"春江多去情，相去枕長汀。","數雁別湓浦，片帆離洞庭。","雨餘沙草綠，雲散岸峰青。","誰共觀明月，漁歌夜好聽。",[41],"洞庭湖",{"title":43,"content":44,"tags":49},"過秦嶺",[45,46,47,48],"古今傳此嶺，高下勢崢嶸。","安得青山路，化爲平地行。","蒼苔留虎跡，碧樹障溪聲。","欲過一回首，踟躕無限情。",[],{"title":51,"content":52,"tags":57},"送吳夢誾歸閩",[53,54,55,56],"甌閩在天末，此去整行衣。","久客逢春盡，思家冒暑歸。","海雲添晚景，山瘴滅晴暉。","相憶吟偏苦，不堪書信稀。",[],{"title":59,"content":60,"tags":65},"山中夏日",[61,62,63,64],"深山宜避暑，門戶映嵐光。","夏木蔭溪路，晝雲埋石床。","心源澄道靜，衣葛蘸泉凉。","算得紅塵裏，誰知此興長。",[],{"title":67,"content":68,"tags":73},"宿故人江居",[69,70,71,72],"渡口樹冥冥，南山漸隠青。","漁舟歸舊浦，鷗鳥宿前汀。","靜榻懸燈坐，閒門對浪扃。","相思頻到此，幾番醉還醒。",[],{"title":75,"content":76,"tags":81},"寄伍喬",[77,78,79,80],"蹉跎春又晚，天末信來遲。","長憶分携日，正當摇落時。","獨遊饒旅恨，多事失歸期。","君看前溪樹，山禽巢幾枝。",[],{"title":83,"content":84,"tags":89},"山中訪人不遇",[85,86,87,88],"負琴兼杖藜，特地過巖西。","已見竹軒閉，又聞山鳥啼。","長松寒倚谷，細草暗連溪。","久立無人事，烟霞歸路迷。",[],{"title":91,"content":92,"tags":97},"贈隠者",[93,94,95,96],"世路爭名利，深山獨結茅。","安情自得所，非道豈相交。","百尺松當戶，千年鶴在巢。","知君於此景，未欲等閒拋。",[],{"title":99,"content":100,"tags":105},"寄暹上人",[101,102,103,104],"聞罷城中講，來安頂上禪。","夜燈明石室，清磬出巖泉。","欲訪慚多事，相思恨隔年。","終期息塵慮，接話虎溪邊。",[],{"title":107,"content":108,"tags":113},"江邊閒步",[109,110,111,112],"閒來南渡口，迤邐看江楓。","一路波濤畔，數家蘆葦中。","遠汀排晚樹，深浦漾寒鴻。","吟罷慵回首，此情誰與同。",[],{"title":115,"content":116,"tags":121},"過王逸人園林",[117,118,119,120],"谷口何時住，烟霞一徑深。","水聲離遠洞，山色出疏林。","雪彩從沾鬢，年光不計心。","自言人少到，猶喜我來尋。",[],{"title":123,"content":124,"tags":129},"寄李處士",[125,126,127,128],"僧話磻溪叟，平生重赤松。","夜堂悲蟋蟀，秋水老芙蓉。","吟坐倦垂釣，閒行多倚筇。","聞名來已久，未得一相逢。",[],{"title":131,"content":132,"tags":137},"夏日寄史處士",[133,134,135,136],"掩關苔滿地，終日坐騰騰。","暑氣冷衣葛，暮雲催燭燈。","寂寥知得趣，疏懶似無能。","還憶舊遊否，何年別杜陵。",[],{"title":139,"content":140,"tags":145},"夏日登瀑頂寺因寄諸知己",[141,142,143,144],"曾於塵埃望，此景在烟霄。","巖靜水聲近，山深暑氣遥。","杖藜青石路，煮茗白雲樵。","寄語爲郎者，誰能訪寂寥。",[],{"title":147,"content":148,"tags":153},"懷果上人",[149,150,151,152],"擁錫南遊去，名香幾處焚。","別來無遠信，多恐在深雲。","好月曾同步，幽香省共聞。","相思不相見，林下葉紛紛。",[],{"title":155,"content":156,"tags":161},"寄故園兄弟",[157,158,159,160],"久與鄉關阻，風塵損舊衣。","水思和月泛，山憶共僧歸。","林想添鄰舍，溪應改釣磯。","弟兄無苦事，不用別庭闈。",[],{"title":163,"content":164,"tags":169},"早秋吟眺",[165,166,167,168],"新秋初雨後，獨立對遥山。","去鳥望中沒，好雲吟裏還。","長年慚道薄，明代取身閒。","從有西征思，園林懶閉關。",[],{"title":171,"content":172,"tags":177},"送江爲歸嶺南",[173,174,175,176],"舊山臨海色，歸路到天涯。","此別各多事，重逢是幾時。","江行晴望遠，嶺宿夜吟遲。","珍重南方客，清風失所思。",[],{"title":179,"content":180,"tags":185},"山中答友人",[181,182,183,184],"偶愛春山在，因循值暑時。","風塵非所願，泉石本相宜。","坐久松陰轉，吟餘蟬韻移。","自慚疏野甚，多失故人期。",[],{"title":187,"content":188,"tags":193},"山齋早秋雨中",[189,190,191,192],"深居少往還，卷箔早秋間。","雨灑吟蟬樹，雲藏嘯狖山。","炎蒸如便退，衣葛亦堪閒。","靜坐得無事，酒卮聊暢顔。",[],{"title":195,"content":196,"tags":201},"送人遊南越",[197,198,199,200],"孑然南越去，替爾畏前程。","見說路歧嶮，不通車馬行。","瘴烟迷海色，嶺樹带猿聲。","獨向山家宿，多應鄉思生。",[],{"title":203,"content":204,"tags":209},"酬東溪史處士",[205,206,207,208],"咫尺東溪路，年來偶訪遲。","泉聲迷雨夜，花片落空枝。","石徑逢僧出，山床見鶴移。","貧齋有琴酒，曾許月圓期。",[],{"title":211,"content":212,"tags":217},"秋江送客",[213,214,215,216],"秋風楚江上，送子話遊遨。","遠水宿何處，孤舟春夜濤。","浦雲沈雁影，山月照猿嗥。","莫爲饑寒苦，便成名利勞。",[],{"title":219,"content":220,"tags":225},"寄山中高逸人",[221,222,223,224],"烟霞多放曠，吟嘯是尋常。","猿共摘山果，僧鄰住石房。","躡雲雙屐冷，採藥一身香。","我憶相逢夜，松潭月色凉。",[],{"title":227,"content":228,"tags":233},"懷友人",[229,230,231,232],"浮世況多事，飄流每嘆君。","路岐何處去，消息幾時聞。","吟裏落秋葉，望中生暮雲。","孤懷誰慰我，夕鳥自成羣。",[],{"title":235,"content":236,"tags":241},"送人歸別業",[237,238,239,240],"別業五湖上，春殘去路賒。","還尋舊山水，重到故人家。","門徑掩芳草，園林落異花。","君知釣磯在，猶喜有生涯。",[],{"title":243,"content":244,"tags":249},"冬日登江樓",[245,246,247,248],"高樓臨古岸，野步晚來登。","江水因寒落，山雲爲雪凝。","遠村雖入望，危檻不堪憑。","親老未歸去，鄉愁徒自興。",[],{"title":251,"content":252,"tags":257},"寄張山人",[253,254,255,256],"草堂南澗邊，有客嘯雲烟。","掃葉林風後，拾薪山雨前。","野橋通竹徑，流水入芝田。","琴月相親夜，更深戀不眠。",[],"content:meng-guan.json","json","content","meng-guan.json",[263,266,269,272,275,278,281,284,287,290,293,296,299,302,305,308,311,314,317,320,323,326,329,332,335,338,341,344,347,350,353,356,359,362,365,368,371,374,377,380,383,386,389,392,395,398,401,404,407,410,413,416,419,422,425,428,431,434,437,440,443,446,449,452,455,458,461,464,467,470,473,476,479,482,485,488,491,494,497,500,503,506,509,512,515,518,521,524,527,530,533,536,539,542,545,548,551,554,557,560,563,566,569,572,575,578,581,584,587,590,593,596,599,602,605,608,611,614,617,620,623,626,629,632,635,638,641,644,647,650,653,656,659,662,665,668,671,674,677,680,683,686,689,692,695,698,701,704,707,710,713,716,719,722,725,728,731,734,737,740,743,746,749,752,755,758,761,764,767,770,773,776,779,782,785,788,791,794,797,800,803,806,809,810,813,816,819,822,825,828,831,834,837,840,843,846,849,852,855,858,861,864,867,870,873,876,879,882,885,888,891,894,897,900,903,906,909,912,915,918,921,924,927,930,933,936,939,942,945,948,951,954,957,960,963,966,969,972,975,978,981,984,987,990,993,996,999,1002,1005,1008,1011,1014,1017,1020,1023,1026,1029,1032,1035,1038,1041,1044,1047,1050,1053,1056,1059,1062,1065,1068,1071,1074,1077,1080,1083,1086,1089,1092,1095,1098,1101,1104,1107,1110,1113,1116,1119,1122,1125,1128,1131,1134,1137,1140,1143,1146,1149,1152,1155,1158,1161,1164,1167,1170,1173,1176,1179,1182,1185,1188,1191,1194,1197,1200,1203,1206,1209,1212,1215,1218,1221,1224,1227,1230,1233,1236,1239,1242,1245,1248,1251,1254,1257,1260,1263,1266,1269,1272,1275,1278,1281,1284,1287,1290,1293,1296,1299,1302,1305,1308,1311,1314,1317,1320,1323,1326,1329,1332,1335,1338,1341,1344,1347,1350,1353,1356,1359,1362,1365,1368,1371,1374,1377,1380,1383,1386,1389,1392,1395,