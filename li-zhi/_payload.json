[{"data":1,"prerenderedAt":1315},["Reactive",2],{"content-query-Cm96Bz2zMj":3,"content-navigation-8C37fagqQL":218},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":214,"_type":215,"_source":216,"_file":217,"_extension":215},"/li-zhi","",false,"李至",[9,17,25,33,41,49,57,65,73,81,89,97,105,113,121,129,143,151,158,166,174,182,190,206],{"title":10,"content":11,"tags":16},"奉和小園獨坐偶賦所懷",[12,13,14,15],"歌舞林亭不外求，朝迴多著道衣遊。","薔薇點綴勾欄好，薜荔攀緣怪石幽。","泥浸落花紅片濕，鳥啼深樹綠陰稠。","春風去便還經歲，莫負芳菲醉即休。",[],{"title":18,"content":19,"tags":24},"奉和獨賞牡丹",[20,21,22,23],"遶臺依榭一叢叢，紫映黄苞白映紅。","爛漫只因前夜雨，馨香無奈此時風。","暖融春色交相妬，繁曬晴陽艷欲烘。","應恨官高少同賞，罇前不得召鄰翁。",[],{"title":26,"content":27,"tags":32},"殘春有感  其一",[28,29,30,31],"陰陰垂柳轉黄鸝，腸斷殘春欲去時。","白雪亂堆飛絮地，紅鬚半在落花枝。","園林向晚深藏郭，野水新晴入池。","最是牡丹堪痛惜，風吹雨打漸離枝。",[],{"title":34,"content":35,"tags":40},"殘春有感  其二",[36,37,38,39],"年年九十日春風，風雨多端復在中。","一盞更還辜我綠，亂花知是爲誰細。","樹頭啼鳥疑相勸，窗下吟生似不通。","可惜芳晨莫虛過，眼前便作白頭翁。",[],{"title":42,"content":43,"tags":48},"喜雨",[44,45,46,47],"沈煙漠漠晦平皋，盡日空濛阻出遨。","窗打紙聲聞紅點，檻飄花影見凝膏。","依稀恨曲添鳴溜，悵望啼粧在小桃。","何假錦障泥看麥，門前芳草綠於袍。",[],{"title":50,"content":51,"tags":56},"寄獻僕射相公",[52,53,54,55],"病無憀頼負重陽，虛過東籬爛漫黄。","把酒不成陶令醉，脫巾空羨孟嘉狂。","鴻聲歷歷高橫漢，雲影離離薄護霜。","珍重風流賢相國，此時閑卧奏清商。",[],{"title":58,"content":59,"tags":64},"早春寄獻僕射相公",[60,61,62,63],"城中塵坌林亭遠，知得春風何日來。","城外地寬松石好，乞聞春色過年迴。","竹枝宜靜應分洗，果樹嫌繁更擘栽。","借問曹樽殘幾許，且須留取待花開。",[],{"title":66,"content":67,"tags":72},"伏奉佳句猥寄鄙夫輒寫丹誠仰依高韻",[68,69,70,71],"獨慙師長念遺簪，長感知心在寸心。","要路未離扶遣進，閑曹因病放教吟。","風鵬雖遠丹霄勢，月兔猶依綠桂陰。","從此期公惜高鑒，休將白雪換巴音。",[],{"title":74,"content":75,"tags":80},"再獻五章奉資一笑  其一",[76,77,78,79],"福德才名盡不如，三朝榮貴四旬餘。","花前尚飲連觥酒，燈下猶看細字書。","黄閤暫拋金印重，青銅閑照雪髯疏。","蒼生共指中臺位，除是明公更合居。",[],{"title":82,"content":83,"tags":88},"再獻五章奉資一笑  其二",[84,85,86,87],"勞公相比作相如，才即無餘幸有餘。","但願徧觀蓬閣史，何曾歸著茂陵書。","已慙諭蜀詞章劣，仍恨遊梁價望疏。","惟愛一琴猶頗似，卓文君又恥同居。",[],{"title":90,"content":91,"tags":96},"再獻五章奉資一笑  其三",[92,93,94,95],"逢春春半合何如，火急謀懽二月餘。","且向花間行載酒，誰能窗下卧看書。","追攀每恨朱門遠，鄙恡仍憐玉柄疏。","他日相尋預相報，芳時多不在蓬居。",[],{"title":98,"content":99,"tags":104},"再獻五章奉資一笑  其四",[100,101,102,103],"而今祕閣古難如，何啻梁元十萬餘。","無本盡從三館借，有籤重遣八分書。","芸香欲辟魚心蠹，汗簡猶嫌吏手疏。","號作蓬山應不錯，祇緣清浄似仙居。",[],{"title":106,"content":107,"tags":112},"再獻五章奉資一笑  其五",[108,109,110,111],"小院荒凉亦自如，纔盈五畝又無餘。","檻中脩竹依時種，壁上斜行信手書。","雨打杏花將爛漫，風吹楊柳漸扶疏。","誠知濁酒非公飲，頻爲芳辰過陋居。",[],{"title":114,"content":115,"tags":120},"昨日奉謁還家馬上偶成長句亦次前韻用述鄙懷",[116,117,118,119],"澄波橋遠馬班如，往復依稀十里餘。","春色遶園千樹柳，清風吹面一堂書。","已降詩迸驪珠逸，更羨談揮麈尾疏。","定許貧家卜鄰否，欲收殘俸便尋居。",[],{"title":122,"content":123,"tags":128},"奉和對雨閑吟之什",[124,125,126,127],"南風吹雨驟還收，五月簾櫳却似秋。","陌上莫嫌行未得，田中應待足方休。","人垂折角衝寒點，馬浸障泥過漫流。","遥羨文昌新右相，醉吟時伏一金甌。",[],{"title":130,"content":131,"tags":142},"奉和對海紅花見寄之什",[132,133,134,135,136,137,138,139,140,141],"春風仙杏枝，條忽吹成果。","墓雨牡丹苞，凄凉飄去我。","獨有海紅花，一叢千萬朵。","深似猩血染，香於麝臍破。","繁壓玉欄霞，紅燒翠鬟火。","俗呼錦被堆，又有何不可。","僕射多才情，新停濟川舸。","閑遶復相思，醉袖花邊嚲。","吟成數十字，安安明珠顆。","魚目輒還公，莫笑輕酬麽。",[],{"title":144,"content":145,"tags":150},"依韻奉和來篇",[146,147,148,149],"紅塵赤日欲相燃，頼向蓬丘少事牽。","明月動摇紈扇室，亂雲重疊火□天。","幽屏看雪思寒景，小簟橫風戀畫眠。","安得心源凉似水，虎溪遥羨老僧禪。",[],{"title":144,"content":152,"tags":157},[153,154,155,156],"閑書閑畫滿芸香，卧讀行看固是常。","天爲苦吟終借□，帝憐多病不教忙。","泥封法醖醅猶嫩，茶煮頒冰味更長。","一榻清風兼好獻，可能來此就儂嘗。",[],{"title":159,"content":160,"tags":165},"夏日直祕閣",[161,162,163,164],"蓬閣清虛稱野情，華陽巾穩葛衣輕。","垂簾不見喧囂事，盡日唯聞禽鳥聲。","小檜影中鋪硯席，短槐陰下著茶鐺。","却愁窮巷晚歸去，塵土滿街衝熱行。",[],{"title":167,"content":168,"tags":173},"暑夜偶作",[169,170,171,172],"竹氣荷香夜轉清，趁凉移簟遶空庭。","坊門有月幾多樹，池水無風三四星。","何處一蟬猶切切，此時殘漏尚丁丁。","可憐好夢誰驚覺，正戀五湖吹酒醒。",[],{"title":175,"content":176,"tags":181},"偶述鄙懷奉呈僕射相公",[177,178,179,180],"從來迂僻寡朋儔，今日優恩豈自酬。","抱病也容居粉署，好吟仍使在蓬丘。","利名場裏心尤拙，少俊叢中鬢獨秋。","早晚東山拂衣去，願携鳩杖從公遊。",[],{"title":183,"content":184,"tags":189},"奉和牡丹盛開之什",[185,186,187,188],"主人多思惜芬度，醉遶吟攀自是常。","爛欲燒叢渾是焰，穠來薰物甚於香。","妬偎羅綺迷春眼，狂聒笙歌費酒腸。","久病勞公誤相問，不同前作粉闈郎。",[],{"title":191,"content":192,"tags":205},"桃花犬歌呈修史錢侍郎",[193,194,195,196,197,198,199,200,201,202,203,204],"宮中有犬桃花名，絳繒圍頸懸金鈴。","先皇爲愛馴且異，指顧之間知上意。","珠簾未卷扇未開，桃花援尾常先至。","夜靜不離香砌眠，朝饑祇傍御床餧。","彩雲路熟不勞牽，瑶草風微有時吠。","無何軒后鑄鼎成，忽遺弓劍棄寰瀛。","迢迢松闕伊川上，遠逐龍輴十數程。","兩眥漣漣似垂淚，骨見毛寒頓顦悴。","萬人見者倍傷心，微物感恩猶若是。","韓盧備獵何足嘉，西旅充庭豈爲瑞。","聞君奉詔修實錄，一字爲褒應不曲。","白魚赤雁且勿書，願君書此懲浮俗。",[],{"title":207,"content":208,"tags":213},"題義門胡氏華林書院",[209,210,211,212],"牆頭翠色雲分嶺，井底泉聲瀑落崖。","講席日聞談俎豆，書厨時見整竿牌。","野猿有果頻窺檻，山鳥無人忽下階。","好是魯儒爭就學，不辭千里過江淮。",[],"content:li-zhi.json","json","content","li-zhi.json",[219,222,225,228,231,234,237,240,243,246,249,252,255,258,261,264,267,270,273,276,279,282,285,288,291,294,297,300,303,306,309,312,315,318,321,324,327,330,333,336,339,342,345,348,351,354,357,360,363,366,369,372,375,378,381,384,387,390,393,396,399,402,405,408,411,414,417,420,423,426,429,432,435,438,441,444,447,450,453,456,459,462,465,468,471,474,477,480,483,486,489,492,495,498,501,504,507,510,513,516,519,522,525,528,531,534,537,540,543,546,549,552,555,558,561,562,565,568,571,574,577,580,583,586,589,592,595,598,601,604,607,610,613,616,619,622,625,628,631,634,637,640,643,646,649,652,655,658,661,664,667,670,673,676,679,682,685,688,691,694,697,700,703,706,709,712,715,718,721,724,727,730,733,736,739,742,745,748,751,754,757,760,763,766,769,772,775,778,781,784,787,790,793,796,799,802,805,808,811,814,817,820,823,826,829,832,835,838,841,844,847,850,853,856,859,862,865,868,871,874,877,880,883,886,889,892,895,898,901,904,907,910,913,916,919,922,925,928,931,934,937,940,943,946,949,952,955,958,961,964,967,970,973,976,979,982,985,988,991,994,997,1000,1003,1006,1009,1012,1015,1018,1021,1024,1027,1030,1033,1036,1039,1042,1045,1048,1051,1054,1057,1060,1063,1066,1069,1072,1075,1078,1081,1084,1087,1090,1093,1096,1099,1102,1105,1108,1111,1114,1117,1120,1123,1126,1129,1132,1135,1138,1141,1144,1147,1150,1153,1156,1159,1162,1165,1168,1171,1174,1177,1180,1183,1186,1189,1192,1195,1198,1201,1204,1207,1210,1213,1216,1219,1222,1225,1228,1231,1234,1237,1240,1243,1246,1249,1252,1255,1258,1261,1264,1267,1270,1273,1276,1279,1282,1285,1288,1291,1294,1297,1300,1303,1306,1309,1312],{"title":220,"_path":221},"安德裕","/an-de-yu",{"title":223,"_path":224},"白居易","/bai-ju-yi",{"title":226,"_path":227},"畢士安","/bi-shi-an",{"title":229,"_path":230},"卞震","/bian-zhen",{"title":232,"_path":233},"不詳","/bu-xiang",{"title":235,"_path":236},"蔡襄","/cai-