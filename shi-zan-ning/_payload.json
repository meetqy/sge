[{"data":1,"prerenderedAt":1196},["Reactive",2],{"content-query-ysipImOQ6o":3,"content-navigation-8C37fagqQL":99},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":95,"_type":96,"_source":97,"_file":98,"_extension":96},"/shi-zan-ning","",false,"釋贊寧",[9,17,25,31,39,45,51,57,65,70,75,80,85,90],{"title":10,"content":11,"tags":16},"寄題明月禪院二首  其一",[12,13,14,15],"參差峰岫晝雲昏，入望交蘿濁浪奔。","震澤湧山來北岸，華陽連洞到東門。","日生樹掛紅霞脚，風起波摇白石根。","聞有上方僧住處，橘花林下採蘭蓀。",[],{"title":18,"content":19,"tags":24},"寄題明月禪院二首  其二",[20,21,22,23],"積翠湖心迤邐長，洞臺蕭寺兩交光。","鳥行黑點波濤白，楓葉紅連橘柚黄。","人我絕時隈樹石，是非來處接帆檣。","如何遂得追遊性，擺却營營不急忙。",[],{"title":26,"content":27,"tags":30},"夜吟",[28,29],"獨坐閒吟野思清，秋庭蕭索暮烟輕。","孤燈欲灺月未上，萬籟寂然蛩一聲。",[],{"title":32,"content":33,"tags":38},"居天柱山",[34,35,36,37],"四野豁家庭，柴門夜不扃。","水邊成半偈，月下了殘經。","雖逐諸塵轉，終歸一念醒。","未知斯旨者，萬役盡勞形。",[],{"title":40,"content":41,"tags":44},"秋日寄人",[42,43],"白鳥行從山嘴沒，青鷗羣向水湄分。","松齋獨坐誰爲侶，數片斜飛檻外雲。",[],{"title":46,"content":47,"tags":50},"括浙江潮候",[48,49],"午未未未申，申卯卯辰辰。","巳巳巳午午，朔望一般輪。",[],{"title":52,"content":53,"tags":56},"落花",[54,55],"蝶醉蜂狂香正濃，晚來階下墜衰紅。","開時費盡陽和力，落處難禁一陣風。",[],{"title":58,"content":59,"tags":64},"悟空塔",[60,61,62,63],"浮圖蕭瑟入虛空，一聚全身罔像中。","傳馬祖心開佛印，識龍潛主示神通。","毫光委墜江樓月，道氣馨香海岸風。","此地化緣才始盡，更於何處動魔宮。",[],{"title":66,"content":67,"tags":69},"句  其一",[68],"山黛淺深春氣力，浪花開合水工夫。",[],{"title":71,"content":72,"tags":74},"句  其二",[73],"草濃春逕狹，樹密晚窗陰。",[],{"title":76,"content":77,"tags":79},"句  其三",[78],"登樓千里月，欹枕一聲蟬。",[],{"title":81,"content":82,"tags":84},"句  其四",[83],"要路花爭發，閒門草易荒。",[],{"title":86,"content":87,"tags":89},"句  其五",[88],"與閑爲活計。",[],{"title":91,"content":92,"tags":94},"句  其六",[93],"何用密防姦。",[],"content:shi-zan-ning.json","json","content","shi-zan-ning.json",[100,103,106,109,112,115,118,121,124,127,130,133,136,139,142,145,148,151,154,157,160,163,166,169,172,175,178,181,184,187,190,193,196,199,202,205,208,211,214,217,220,223,226,229,232,235,238,241,244,247,250,253,256,259,262,265,268,271,274,277,280,283,286,289,292,295,298,301,304,307,310,313,316,319,322,325,328,331,