[{"data":1,"prerenderedAt":1254},["Reactive",2],{"content-query-i8QLPw9nkE":3,"content-navigation-8C37fagqQL":157},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":153,"_type":154,"_source":155,"_file":156,"_extension":154},"/qian-zhao-du","",false,"錢昭度",[9,17,23,29,35,43,51,57,63,71,76,81,86,91,96,101,106,111,117,122,127,132,138,143,148],{"title":10,"content":11,"tags":16},"雨霧剡溪",[12,13,14,15],"剡溪風雨霽，航葦重行行。","到處楊柳色，幾家荷葉聲。","噪蟬金鼎沸，游水玉壺清。","最喜魚梁伴，歸帆的的輕。",[],{"title":18,"content":19,"tags":22},"懷會稽",[20,21],"斗牛星外翻成夢，巢鳥枝南略寄身。","越國荷花八百里，別來誰是採蓮人。",[],{"title":24,"content":25,"tags":28},"詠方池",[26,27],"東道主人心匠巧，鑿開方石貯漣漪。","夜深却被寒星映，恰似仙翁一局棋。",[],{"title":30,"content":31,"tags":34},"贈白校書",[32,33],"袖裏青鋒秋水寒，誰疑雙燕是金丸。","出門風雨如何去，空有霜髭在玉盤。",[],{"title":36,"content":37,"tags":42},"憶具區",[38,39,40,41],"平生愛具區，島嶼夾陂湖。","竹雨籠鸂鶒，花煙濕鷓鶘。","神仙疑有宅，魚鼇自爲都。","何事勞長想，機雲本是吳。",[],{"title":44,"content":45,"tags":50},"野水",[46,47,48,49],"野水不如璧，澄心不覺勞。","與天無表裏，共月見分毫。","綠好磨長劍，清宜泛小舠。","淡交今已矣，惆悵越波濤。",[],{"title":52,"content":53,"tags":56},"菊",[54,55],"曾見春花落萬紅，不然隨雨即隨風。","如何得到重陽日，浮在陶家酒盞中。",[],{"title":58,"content":59,"tags":62},"春陰",[60,61],"語燕初飛隴麥青，春雲將雨滯行人。","雲間若有金烏賣，應被豪家占得春。",[],{"title":64,"content":65,"tags":70},"野墅夏晚",[66,67,68,69],"一抹生紅畫杏腮，半園沈綠鎖桐材。","黄蜂衙退海潮上，白蟻戰酣山雨來。","睡思幾家金带枕，酒香何處玉交杯。","太陽西落波東去，惆悵無人喚得歸。",[],{"title":72,"content":73,"tags":75},"句  其一",[74],"西南片月充腸冷，二八飛泉繞齒寒。",[],{"title":77,"content":78,"tags":80},"句  其二",[79],"磻溪重得呂，維嶽再生申。",[],{"title":82,"content":83,"tags":85},"句  其三",[84],"雨被北風吹作雪，水愁東海亦成冰。",[],{"title":87,"content":88,"tags":90},"句  其四",[89],"東北風吹大庚嶺，西南日映小寒天。",[],{"title":92,"content":93,"tags":95},"句  其五",[94],"三年別館風吹入，萬里長沙月照來。",[],{"title":97,"content":98,"tags":100},"句  其六",[99],"人間路到三峰盡，天下秋隨一葉來。",[],{"title":102,"content":103,"tags":105},"句  其七",[104],"西北高樓在，東南王氣銷。",[],{"title":107,"content":108,"tags":110},"句  其八",[109],"繡帔夢驚中酒處，朱門人語上朝時。",[],{"title":112,"content":113,"tags":115},"句  其九",[114],"船中聞雁洞庭夜，床下有蛩長信秋。",[116],"洞庭湖",{"title":118,"content":119,"tags":121},"句  其一○",[120],"風欲拋高雨壓沉，黄昏前後五更深。",[],{"title":123,"content":124,"tags":126},"句  其一一",[125],"閼氏纔聞易妾名，歸期長似俟河清。",[],{"title":128,"content":129,"tags":131},"句  其一二",[130],"荷揮萬朵玉如意，蟬弄一聲金錯刀。",[],{"title":133,"content":134,"tags":136},"句  其一三",[135],"長憶錢塘江上望，酒樓人散雨千絲。",[137],"钱塘江",{"title":139,"content":140,"tags":142},"句  其一四",[141],"蕎麥花殘小雪飛。",[],{"title":144,"content":145,"tags":147},"句  其一五",[146],"南人如問雪，向道是楊花。",[],{"title":149,"content":150,"tags":152},"句  其一六",[151],"伯禹無端教鮮食，水中魚盡不知休。",[],"content:qian-zhao-du.json","json","content","qian-zhao-du.json",[158,161,164,167,170,173,176,179,182,185,188,191,194,197,200,203,206,209,212,215,218,221,224,227,230,233,236,239,242,245,248,251,254,257,260,263,266,269,272,275,278,281,284,287,290,293,296,299,302,305,308,311,314,317,320,323,326,329,332,335,338,341,344,347,350,353,356,359,362,365,368,371,374,377,380,383,386,389,392,395,398,401,404,407,410,413,416,419,422,425,428,431,434,437,440,443,446,449,452,455,458,461,464,467,470,473,476,479,482,485,488,491,494,497,500,503,506,509,512,515,518,521,524,527,530,533,536,539,542,545,548,551,554,557,560,563,566,569,572,575,578,581,584,587,590,593,596,599,602,605,608,611,614,617,620,623,626,629,632,635,638,641,644,647,650,653,656,659,662,665,668,671,674,677,680,681,684,687,690,693,696,699,702,705,708,711,714,717,720,723,726,729,732,735,738,741,744,747,750,753,756,759,762,765,768,771,774,777,780,783,786,789,792,795,798,801,804,807,810,813,816,819,822,825,828,831,834,837,840,843,846,849,852,855,858,861,864,867,870,873,876,879,882,885,888,891,894,897,900,903,906,909,912,915,918,921,924,927,930,933,936,939,942,945,948,951,954,957,960,963,966,969,972,975,978,981,984,987,990,993,996,999,1002,1005,1008,1011,1014,1017,1020,1023,1026,1029,1032,1035,1038,1041,1044,1047,1050,1053,1056,1059,1062,1065,1068,1071,1074,1077,1080,1083,1086,1089,1092,1095,1098,1101,1104,1107,1110,1113,1116,1119,1122,1125,1128,1131,1134,1137,1140,1143,1146,1149,1152,1155,1158,1161,1164,1167,1170,1173,1176,1179,1