[{"data":1,"prerenderedAt":1344},["Reactive",2],{"content-query-Kfou4VDP9N":3,"content-navigation-8C37fagqQL":73},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":69,"_type":70,"_source":71,"_file":72,"_extension":70},"/xi-xi-sou","",false,"席羲叟",[9,15,21,27,33,39,45,51,57,63],{"title":10,"content":11,"tags":14},"次令衿遊玉壺高詠十絕 風雷大作",[12,13],"坐聽風雷起空谷，俄看四面野雲生。","道人不作英雄氣，可笑當年失箸驚。",[],{"title":16,"content":17,"tags":20},"次令衿遊玉壺高詠十絕 獨坐",[18,19],"几檻虛明不著埃，忘機鷗鳥自飛來。","北窗高卧羲皇世，九陌紅塵安在哉。",[],{"title":22,"content":23,"tags":26},"次令衿遊玉壺高詠十絕 處處可人",[24,25],"陂塘五月迎秋爽，脩竹成陰一逕開。","俗客自應慚到此，幽深雅稱杖藜來。",[],{"title":28,"content":29,"tags":32},"次令衿遊玉壺高詠十絕 解衣磐",[30,31],"林宗烏幘任欹傾，緩带荷風拂面清。","城市輪蹄鬧如織，飄然從此便登瀛。",[],{"title":34,"content":35,"tags":38},"次令衿遊玉壺高詠十絕 小雨垂簾就寢",[36,37],"篆香如霧細縈簾，始信壺中別有天。","客舍黄粱今熟否，邯鄲無夢夢遊仙。",[],{"title":40,"content":41,"tags":44},"次令衿遊玉壺高詠十絕 凭欄看花",[42,43],"湖光空闊水天秋，翠蓋紅衣永日留。","惟有詩情磨不盡，碧雲吟就喜湯休。",[],{"title":46,"content":47,"tags":50},"次令衿遊玉壺高詠十絕 欲歸甫留",[48,49],"度塹昏鴉接翅稀，斷霞相映霧山微。","騶車不用飛青蓋，試作山翁倒載歸。",[],{"title":52,"content":53,"tags":56},"次令衿遊玉壺高詠十絕 滿園秋香",[54,55],"黄金勻揉萬英開，步遶清芬過砌臺。","一笑欣然超鼻觀，知公曾到月中來。",[],{"title":58,"content":59,"tags":62},"次令衿遊玉壺高詠十絕 久病便覺清快",[60,61],"內熟先從一念收，浩然勿藥更何求。","西山故自朝來爽，政不驚人語不休。",[],{"title":64,"content":65,"tags":68},"次令衿遊玉壺高詠十絕 對換",[66,67],"絕景頻因警句誇，莫傳俚耳便紛挐。","須知味道耽巖穴，志意難將富貴加。",[],"content:xi-xi-sou.json","json","content","xi-xi-sou.json",[74,77,80,83,86,89,92,95,98,101,104,107,110,113,116,119,122,125,128,131,134,137,140,143,146,149,152,155,158,161,164,167,170,173,176,179,182,185,188,191,194,197,200,203,206,209,212,215,218,221,224,227,230,233,236,239,242,245,248,251,254,257,260,263,266,269,272,275,278,281,284,287,290,293,296,299,302,305,308,311,314,317,320,323,326,329,332,335,338,341,344,347,350,353,356,359,362,365,368,371,374,377,380,383,386,389,392,395,398,401,404,407,410,413,416,419,422,425,428,431,434,437,440,443,446,449,452,455,458,461,464,467,470,473,476,479,482,485,488,491,494,497,500,503,506,509,512,515,518,521,524,527