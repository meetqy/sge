[{"data":1,"prerenderedAt":1195},["Reactive",2],{"content-query-I3fRcfTWVX":3,"content-navigation-8C37fagqQL":98},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":94,"_type":95,"_source":96,"_file":97,"_extension":95},"/wang-chang-ling","",false,"王昌齡",[9,23,34,42,54,58,67,76,88],{"title":10,"content":11,"tags":14},"橫吹曲辭 出塞 一",[12,13],"秦時明月漢時關，萬里長征人未還。","但使龍城飛將在，不教胡馬度陰山。",[15,16,17,18,19,20,21,22],"唐诗三百首","战争","写景","忧国忧民","爱国","乐府","山","边塞",{"title":24,"content":25,"tags":30},"塞下曲四首 一",[26,27,28,29],"蟬鳴空桑林，八月蕭關道。","出塞入塞寒，處處黃蘆草。","從來幽幷客，皆共塵沙老。","莫學遊俠兒，矜夸紫騮好。",[15,17,31,20,32,22,33],"游侠","讽刺","新乐府辞",{"title":35,"content":36,"tags":41},"塞下曲四首 二",[37,38,39,40],"飲馬渡秋水，水寒風似刀。","平沙日未沒，黯黯見臨洮。","昔日長城戰，咸言意氣高。","黃塵足今古，白骨亂蓬蒿。",[15,16,33,20],{"title":43,"content":44,"tags":50},"同從弟銷南齋玩月憶山陰崔少府",[45,46,47,48,49],"高臥南齋時，開帷月初吐。","清輝淡水木，演漾在窗戶。","苒苒幾盈虛，澄澄變今古。","美人清江畔，是夜越吟苦。","千里其如何，微風吹蘭杜。",[15,51,52,53],"五言古诗","月亮","怀人",{"title":55,"content":56,"tags":57},"出塞二首 一",[12,13],[16,17,18,19,15,20,21,22],{"title":59,"content":60,"tags":63},"春宮曲",[61,62],"昨夜風開露井桃，未央前殿月輪高。","平陽歌舞新承寵，簾外春寒賜錦袍。",[15,64,65,66],"七言绝句","宫怨","女子",{"title":68,"content":69,"tags":72},"閨怨",[70,71],"閨中少婦不曾愁，春日凝妝上翠樓。","忽見陌頭楊柳色，悔教夫壻覓封侯。",[15,73,74,64,75],"妇女","春天","闺怨",{"title":77,"content":78,"tags":81},"芙蓉樓送辛漸二首 一",[79,80],"寒雨連天夜入湖，平明送客楚山孤。","洛陽親友如相問，一片冰心在玉壺。",[15,82,83,84,64,85,86,87],"写雨","抒情","送别","雨","六年级下册","小学古诗",{"title":89,"content":90,"tags":93},"相和歌辭 長信怨 二",[91,92],"奉帚平明金殿開，暫將團扇共裴回。","玉顏不及寒鵶色，猶帶昭陽日影來。",[15],"content:wang-chang-ling.json","json","content","wang-chang-ling.json",[99,102,105,108,111,114,117,120,123,126,129,132,135,138,141,144,147,150,153,156,159,162,165,168,171,174,177,180,183,186,189,192,195,198,201,204,207,210,213,216,219,222,225,228,231,234,237,240,243,246,249,252,255,258,261,264,267,270,273,276,279,282,285,288,291,294,297,300,303,306,309,312,315,318,321,324,327,330,333,336,339,342,345,348,351,354,357,360,363,366,369,372,375,378,381,384,387,390,393,396,399,402