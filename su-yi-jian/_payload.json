[{"data":1,"prerenderedAt":1364},["Reactive",2],{"content-query-k1PvEVya0c":3,"content-navigation-8C37fagqQL":93},{"_path":4,"_dir":5,"_draft":6,"_partial":6,"_locale":5,"title":7,"children":8,"_id":89,"_type":90,"_source":91,"_file":92,"_extension":90},"/su-yi-jian","",false,"蘇易簡",[9,19,29,37,43,49,54,59,64,69,74,79,84],{"title":10,"content":11,"tags":18},"贈翰林學士宋公白",[12,13,14,15,16,17],"天子昔取士，先俾分媸妍。","濟濟俊兼秀，師師麟與鸞。","小子最承知，同輩尋改觀。","甲第叨薦名，高飛便凌煙。","遂使拜扆坐，果得超神仙。","迄今才七歲，相接乘華軒。",[],{"title":20,"content":21,"tags":28},"禁林讌會之什",[22,23,24,25,26,27],"雨晴禁署絕纖塵，讌會名賢四海聞。","供職盡居清顯地，崇儒同感聖明君。","翩然飛白璇題字，煥若丹青翠琰文。","梓澤笙歌誠外物，蘭亭詩酒不同羣。","少年已作瀛洲老，他日終棲太華雲。","莫怪坐間全不飲，心中和氣自醺醺。",[],{"title":30,"content":31,"tags":36},"題臨蘭亭序",[32,33,34,35],"有若似夫子，尚興闕里門。","虎賁狀蔡邕，猶旁文舉樽。","昭陵自一閉，真蹟不復存。","予今獲此本，亦可比璵璠。",[],{"title":38,"content":39,"tags":42},"特吟詩二首送英公大師  其一",[40,41],"乘舟南去唯尋酒，上馬西行只詠詩。","醒醉去留皆遂意，如斯方信是男兒。",[],{"title":44,"content":45,"tags":48},"特吟詩二首送英公大師  其二",[46,47],"祝融峰上曾傳納，太一山前舊結廬。","兩地逍遥已三紀，爭教肯在帝城居。",[],{"title":50,"content":51,"tags":53},"句  其一",[52],"玉堂臣老非仙骨，猶在丹臺望泰階。",[],{"title":55,"content":56,"tags":58},"句  其二",[57],"天賦九華名狀別，岧嶢勝槩壓諸方。",[],{"title":60,"content":61,"tags":63},"句  其三",[62],"蝦鬚半卷天香散。",[],{"title":65,"content":66,"tags":68},"句  其四",[67],"憔悴貳卿三十六，與師氣味不爭多。",[],{"title":70,"content":71,"tags":73},"句  其五",[72],"忠孝一生心。",[],{"title":75,"content":76,"tags":78},"存目  其一",[77],"詩題：懷安官舍病起九日作首句：籬菊今年拆最繁。",[],{"title":80,"content":81,"tags":83},"存目  其二",[82],"詩題：十日菊首句：寂寞東籬下。",[],{"title":85,"content":86,"tags":88},"存目  其三",[87],"詩題：重陽述懷首句：屋頭寒雀作饑聲。",[],"content:su-yi-jian.json","json","content","su-yi-jian.json",[94,97,100,103,106,109,112,115,118,121,124,127,130,133,136,139,142,145,148,151,154,157,160,163,166,169,172,175,178,181,184,187,190,193,196,199,202,205,208,211,214,217,220,223,226,229,232,235,238,241,244,247,250,253,256,259,262,265,268,271,274,277,280,283,286,289,292,295,298,301,304,307,310,313,316,319,322,325,328,331,334,337,340,343,346,349,352,355,358,361,364,367,370,373,376,379,382,3